+            {
+                ServiceMernis.Ilce[] sorguSonucu = response.SorguSonucu;
+
+                if (sorguSonucu != null)
+                {
+                    maksResult.ilceler = sorguSonucu;
+                }
+                else
+                {
+                    maksResult.error.code = 1;
+                    maksResult.error.description = "HATA CODE: #" + maksResult.error.code + " Sorgu Sonucu Null.";
+                }
+
+                return maksResult;
+            }
+            else
+            {
+                maksResult.error.code = 0;
+                maksResult.error.description = "HATA CODE: #" + maksResult.error.code + " IleBagliIlceListesiGetirAsync Sonuç Vermedi.";
+                return maksResult;
