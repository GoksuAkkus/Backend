+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+
+namespace MebCbsAnka.Dtos
+{
+    public class MaksGeriDonusDegerleri
+    {
+        public ServiceMernis.Ilce[] ilceler { get; set; }
+        public ServiceMernis.Mahalle[] mahalleler { get; set; }
+        public ServiceMernis.Csbm[] csbmler { get; set; }
+        public ServiceMernis.Bina[] binalar { get; set; }
+        //public ServiceMernis.BagimsizBolum[] bagimsizBolumler { get; set; }
+        public maksGeometry geometry { get; set; }
+        public ErrorDescription error { get; set; }
+
+        public class ErrorDescription
+        {
+            public int code { get; set; }
+            public string description { get; set; }
+        }
+
+        public class maksGeometry
+        {
+            public List<List<double[]>> rings { get; set; }
+        }
+    }
+}
