+        [Produces("application/xml")]
+        [HttpGet("proxy")]
+        //[Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> transparentProxy()
+        {
+            var query = HttpContext.Request.Query;
+            var urlData = "";
+            var index = 0;
+            var controlStatus = false;
+            var data = query.Last();
+            if (data.Key.Contains("http"))
+            {
+                urlData += data.Key + '?';
+                controlStatus = true;
+            }
+            foreach (var item in query)
+            {
+                if (index == 0 && controlStatus == false)
+                {
+                    urlData += item.Key + "?";
+                    index++;
+                }
+                else if (item.Key.Contains("http") && controlStatus == true)
+                {
+                    index++;
+                }
+                else
+                {
+                    urlData += item.Key + "=" + item.Value + "&";
+                }
+            }
+            urlData = urlData.Remove(urlData.Length - 1);
+            HttpClient client = new HttpClient();
+            if (urlData.Contains("GetCapabilities"))
+            {
+                var response = await client.GetAsync(urlData);
+                var content = await response.Content.ReadAsStringAsync();
+
+                XmlDocument xmlDoc = new XmlDocument();
+                xmlDoc.LoadXml(content);
+                var hrefValue = xmlDoc["WMS_Capabilities"]["Capability"]["Request"]["GetMap"]["DCPType"]["HTTP"]["Get"]["OnlineResource"].GetAttribute("xlink:href");
+                var tempValue = hrefValue;
+                if (hrefValue.Contains("?"))
+                {
+                    tempValue = hrefValue.Substring(0, hrefValue.Length - 1);
+                }
+
+                var newValue = "http://mebcbs.meb.gov.tr/meb-ws/api/Cbs/proxy?" + tempValue;
+
+                var stringDocument = content.Replace(hrefValue, newValue);
+
+                xmlDoc = new XmlDocument();
+                xmlDoc.LoadXml(stringDocument);
+                return Ok(xmlDoc);
+            }
+            else
+            {
+                var response = await client.GetAsync(urlData);
+                var content = await response.Content.ReadAsByteArrayAsync();
+                return File(content, "image/png");
+            }
+        }
+
+        [Produces("application/xml")]
+        [HttpGet("maksProxy")]
+        //[Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> maksProxy()
+        {
+            var mernisData = "http://mernis.meb.gov.tr/WMSService/get.aspx?username=mebcbs&password=9UFdAy7x&";
+            var query = HttpContext.Request.Query;
+            var urlData = "";
+            foreach (var item in query)
+            {
+                mernisData += item.Key + "=" + item.Value + "&";
+            }
+            urlData = mernisData.Remove(mernisData.Length - 1);
+            HttpClient client = new HttpClient();
+            if (urlData.Contains("GetCapabilities"))
+            {
+                var response = await client.GetAsync(urlData);
+                var content = await response.Content.ReadAsStringAsync();
+                content = content.Replace("https://kpsv2.nvi.gov.tr/Services/WMSService/Get", "http://mebcbs.meb.gov.tr/meb-ws/api/Cbs/maksProxy?");
+                XmlDocument xmlDoc = new XmlDocument();
+                xmlDoc.LoadXml(content);
+                return Ok(xmlDoc);
+            }
+            else
+            {
+                var response = await client.GetAsync(urlData);
+                var content = await response.Content.ReadAsByteArrayAsync();
+                return File(content, "image/png");
+            }
+        }
+
+        [Produces("application/xml")]
+        [HttpGet("tucbs")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> getTUCBSLayers()
+        {
+            var url = "https://gis-prod-api.csb.gov.tr/cografiveriservishavuzu_intranet";
+            HttpClient client = new HttpClient();
+
+            var response = await client.GetAsync(url);
+            var content = await response.Content.ReadAsStringAsync();
+
+            XmlDocument xmlDoc = new XmlDocument();
+            xmlDoc.LoadXml(content);
+
+            return Ok(xmlDoc);
+        }
