+        [HttpPost("getMaksAnalysis")]
+        public async Task<List<long>> getMaksAnalysis([FromBody] MaksAnalysis maksAnalysis)
+        {
+            List<long> binaNos = new List<long>();
+            if (maksAnalysis.geometry.Count > 1)
+            {
+                return binaNos;
+            }
+            else
+            {
+                var rings = maksAnalysis.geometry[0];
+                var coordinates = "";
+                for (var j = 0; j < rings.Count; j++)
+                {
+                    var ring = rings[j];
+
+                    coordinates += ring[0] + " ";
+                    coordinates += ring[1] + " ";
+                }
+                coordinates = coordinates.Remove(coordinates.Length - 1);
+                //coordinates = "36.007698821 37.353100276 36.007796508 37.353310624 36.007770627 37.35331623 36.00796033 37.353668195 36.008783706 37.353398531 36.008721999 37.353291395 36.008537638 37.353078864 36.008503253 37.353034149 36.008374915 37.352879559 36.007698821 37.353100276";
+                var maksInfo = await getMaksInfo(coordinates);
+                XmlNode lastChildOfCollection = maksInfo.LastChild; // featureCollection
+
+                foreach (XmlNode feature in lastChildOfCollection.ChildNodes) //featureMembers
+                {
+                    XmlNode featureAttributes = feature.FirstChild;
+                    List<MaksYapi> maksYapiFeaturesArray = new List<MaksYapi>();
+                    var kimlikno = featureAttributes["maks:kimlikNo"] != null ? long.Parse(featureAttributes["maks:kimlikNo"].InnerText) : 0;
+
+                    binaNos.Add(kimlikno);
+                }
+
+                return binaNos;
+            }
+        }
+
+        [HttpGet("getYerlesimYerindeOturanBilgi")]
+        public async Task<YerlesimYeriInfo> YerlesimYerindeOturanlarBilgi(List<int> binaNos)
+        {
+            var giris = new ServiceMernis.KullaniciGiris();
+            giris.UserName = "mebcbs";
+            giris.Password = "9UFdAy7x";
+
+            var logClient = new ServiceMernis.LogClient();
+            logClient.EkranKodu = "1";
+            logClient.Aciklama = "MebCbsAnka";
+            logClient.ModulKodu = "1";
+
+            logClient.YetkiliIPAdresi = "10.30.1.244";
+            logClient.YetkiliKullaniciAdi = "1234567910";
+            logClient.YetkiliKullaniciNo = 1234567910;
+            logClient.YetkiliTCKimlikNo = 44257273824;
+
+            var year = DateTime.Now.Year;
+            var buildings = new List<BuildingInfo>();
+            var yerlesimYeriInfo = new YerlesimYeriInfo()
+            {
+                buildings = buildings
+            };
+
+            foreach (var binaNo in binaNos)
+            {
+                var binaNoKriter = new ServiceMernis.AksOkuBagimsizBolumBinaNoSorguKriter();
+                binaNoKriter.BinaNo = binaNo;
+
+                var binaNoServisClient = new ServiceMernis.AksOkuBagimsizBolumBinaNoServisClient();
+                var bagimsizBolumListe = await binaNoServisClient.BinaNodanBagimsizBolumListeleAsync(binaNoKriter, giris, logClient);
+
+                var cagNufus = new List<Inhabitant>();
+                var okulOncesiNufus = new List<Inhabitant>();
+                var ilkokulNufus = new List<Inhabitant>();
+                var ortaokulNufus = new List<Inhabitant>();
+                var liseNufus = new List<Inhabitant>();
+                var digerNufus = new List<Inhabitant>();
+
+                var buildingInfo = new BuildingInfo()
+                {
+                    binaNo = binaNo,
+                    cagNufus = cagNufus,
+                    okulOncesiNufus = okulOncesiNufus,
+                    ilkOkulNufus = ilkokulNufus,
+                    ortaOkulNufus = ortaokulNufus,
+                    liseNufus = liseNufus,
+                    digerNufus = digerNufus
+                };
+
+                if (bagimsizBolumListe.HataBilgisi != null)
+                {
+                    buildingInfo.daireSayisi = 0;
+                }
+                else
+                {
+                    buildingInfo.daireSayisi = bagimsizBolumListe.SorguSonucu.Length;
+                }
+
+                foreach (var adresData in bagimsizBolumListe.SorguSonucu)
+                {
+                    var adresNoKriter = new ServiceMernis.YerlesimYerindeOturanlarBilgiSorguKriteri();
+                    adresNoKriter.AdresNo = (long)adresData.AdresNo;
+
+                    ServiceMernis.YerlesimYerindeOturanlarBilgisiSonucu yerlesimYerindeOturanlarBilgisiSonucu =
+                    await _yerlesimYerindeOturanlarBilgiServisClient.YerlesimYerindeOturanlarBilgiSorgulaAsync(adresNoKriter, giris, logClient);
+
+                    var inhabitants = yerlesimYerindeOturanlarBilgisiSonucu.SorguSonucu;
+                    foreach (var inhabitant in inhabitants)
+                    {
+                        if (inhabitant.DogumTarih.Yil != null)
+                        {
+                            var age = year - inhabitant.DogumTarih.Yil;
+                            var inhabitantInfo = new Inhabitant()
+                            {
+                                age = (int)age,
+                                gender = (int)inhabitant.Cinsiyet.Kod
+                            };
+
+                            if (age <= 18)
+                            {
+                                if (age >= 3 && age <= 4)
+                                {
+                                    cagNufus.Add(inhabitantInfo);
+                                }
+                                else if (age >= 5 && age <= 6)
+                                {
+                                    okulOncesiNufus.Add(inhabitantInfo);
+                                }
+                                else if (age >= 7 && age <= 10)
+                                {
+                                    ilkokulNufus.Add(inhabitantInfo);
+                                }
+                                else if (age >= 11 && age <= 14)
+                                {
+                                    ortaokulNufus.Add(inhabitantInfo);
+                                }
+                                else
+                                {
+                                    liseNufus.Add(inhabitantInfo);
+                                }
+                            }
+                            else
+                            {
+                                digerNufus.Add(inhabitantInfo);
+                            }
+                        }
+                    }
+                }
+
+                buildings.Add(buildingInfo);
+            }
+
+            return yerlesimYeriInfo;
+        }
+
+        public async Task<XmlDocument> getMaksInfo(string coordinates)
+        {
+            var url = "http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Yapi&username=mebcbs&password=9UFdAy7x&FILTER=%3CFilter%20xmlns=%22http://www.opengis.net/ogc%22%20xmlns:gml=%22http://www.opengis.net/gml%22%3E%3CIntersects%3E%3CPropertyName%3Egeometri%3C/PropertyName%3E%3Cgml:Polygon%20xmlns:gml=%22http://www.opengis.net/gml%22%20srsName=%22EPSG:4326%22%3E%3Cgml:exterior%3E%3Cgml:LinearRing%3E%3Cgml:posList%3E" + coordinates + "%3C/gml:posList%3E%3C/gml:LinearRing%3E%3C/gml:exterior%3E%3C/gml:Polygon%3E%3C/Intersects%3E%3C/Filter%3E";
+
+            var httpClient = new HttpClient();
+
+            var response = await httpClient.GetAsync(url);
+
+            var content = await response.Content.ReadAsStringAsync();
+
+            XmlDocument doc = new XmlDocument();
+            doc.LoadXml(content);
+
+            return doc;
+        }
+
+        [HttpGet("getMaxInfo")]
+        public async Task<string> GetirMaxInfo()
+        {
+            var queryString = HttpContext.Request.QueryString;
+            var collection = HttpContext.Request.Query;
+            var authQuery = "Username=mebcbs&Password=9UFdAy7x";
+            if (!queryString.Value.EndsWith("&"))
+            {
+                authQuery = "&" + authQuery;
+            }
+            var client = new HttpClient();
+            // client.BaseAddress = "http://mernis.meb.gov.tr/WFSService/Get";
+            // client.PostAsync('http://mernis.meb.gov.tr/WFSService/Get',)
+            var url = "http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get" + queryString.Value + authQuery;
+            var result = await client.GetAsync(url);
+            var body = await result.Content.ReadAsStringAsync();
+            return body;
+        }
+    }
+}
