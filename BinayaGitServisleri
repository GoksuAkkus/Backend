+        [HttpPost("createSayistayReport")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> CreateSayistayReport(int ilID, string ilAdi)
+        {
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            using (var package = new ExcelPackage())
+            {
+                var offsetValue = 2;
+                string[] columnHeader = new string[17] {"TAKBİS ZEMİN NO", "TAŞINMAZ NO", "İLÇE", "MAHALLE", "ADA", "PARSEL", "YÜZÖLÇÜM", "MALİKİ","HİSSE ORANI", "CİNSİ", "MEVCUT KULLANIM ŞEKLİ", "EDİNME ŞEKLİ", "EDİNME TARİHİ",
+                "RAYİÇ BEDELİ", "EMLAK V.D.","KAYIT ÇIKIŞ NEDENİ", "KAYIT ÇIKIŞ TARİHİ"};
+                var workSheetName = "TAHSİS FORMU";
+
+                var worksheet = package.Workbook.Worksheets.Add(workSheetName);
+
+                using (var cells = worksheet.Cells[1, 1, 1, columnHeader.Count()])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                worksheet.Cells[1, 19].Value = "TAHSİS FORMU";
+                worksheet.Cells[2, 1].Value = "İL:";
+                worksheet.Cells[2, 2].Value = ilAdi;
+
+                for (int i = 0; i < columnHeader.Count(); i++)
+                {
+                    worksheet.Cells[3, i + 1].Value = columnHeader[i];
+                }
+                HttpClient httpClient = new HttpClient();
+
+                httpClient.DefaultRequestHeaders.Authorization
+                         = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+                var kadastroParselJsonURI = mebCbsUrl + "FeatureServer/6?f=pjson";
+
+                var formUrlEncodedContentForKadastro = new FormUrlEncodedContent(new[]
+                {
+                    new KeyValuePair<string, string>("where", "il="+ilID),
+                    new KeyValuePair<string, string>("returnCountOnly", "true"),
+                    new KeyValuePair<string, string>("f", "pjson"),
+                    new KeyValuePair<string, string>("token", arcgisTokenDto.token)
+
+                });
+
+                var response = await httpClient.PostAsync(kadastroParselJsonURI, formUrlEncodedContentForKadastro);
+                var content = await response.Content.ReadAsStringAsync();
+                var arcgisServiceData = JObject.Parse(content);
+
+                var fieldData = arcgisServiceData["fields"];
+
+                string[] columnHeaderFields = new string[17] { "kimlik_no","tasinmaz_id", "tkgm_ilce", "mahalle", "ada_no", "parsel_no", "kadastro_alan", "mulkiyet"
+                    ,"hisse", "tapu_cins", "mevcut_kullanim","meb_edinim_sekli", "meb_edinim_tarih", "rayic_bedeli" , "vergi_degeri", "kayit_neden", "kayit_tarih"};
+
+                int featureCount = 1;
+
+                var kadastroParselQueryURI = mebCbsUrl + "FeatureServer/6/query";
+                var malikTableUrL = mebCbsUrl + "FeatureServer/15/query";
+
+                string query = "il=" + ilID;
+                int offsetMultiplier = 0;
+
+                while (featureCount != 0)
+                {
+                    var resulttOffset = offsetMultiplier * 1000;
+
+                    var formUrlEncodedContent = new FormUrlEncodedContent(new[]
+                    {
+                        new KeyValuePair<string, string>("where", query),
+                        new KeyValuePair<string, string>("geometryType", "esriGeometryEnvelope"),
+                        new KeyValuePair<string, string>("spatialRel", "esriSpatialRelIntersects"),
+                        new KeyValuePair<string, string>("outFields", "kimlik_no,tasinmaz_id, tkgm_ilce, mahalle, ada_no, parsel_no, kadastro_alan, mulkiyet, tapu_cins, mevcut_kullanim,meb_edinim_tarih, meb_edinim_sekli, rayic_bedeli ,vergi_degeri, kayit_neden, kayit_tarih" ),
+                        new KeyValuePair<string, string>("returnGeometry", "false"),
+                        new KeyValuePair<string, string>("f", "pjson"),
+                        new KeyValuePair<string, string>("resultOffset", resulttOffset.ToString()),
+                        new KeyValuePair<string, string>("token", arcgisTokenDto.token)
+                    });
+
+                    response = await httpClient.PostAsync(kadastroParselQueryURI, formUrlEncodedContent);
+                    content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+
+                    var features = arcgisResponseData["features"].ToArray();
+                    featureCount = features.Count();
+
+                    for (int i = 0; i < features.Count(); i++)
+                    {
+                        var feature = arcgisResponseData["features"][i];
+                        var featureAttributes = feature["attributes"].ToArray();
+
+                        string tasinmazID = "";
+                        string maliyeHazinesiPay = "";
+
+                        for (int j = 0; j < columnHeaderFields.Count(); j++)
+                        {
+                            var cellRow = offsetValue + i + 2;
+                            var cellColumn = j + 1;
+
+                            string column = columnHeaderFields[j];
+
+                            for (int k = 0; j < featureAttributes.Count(); k++)
+                            {
+                                var attribute = featureAttributes[k];
+                                JProperty jProperty = attribute.ToObject<JProperty>();
+
+                                if (jProperty.Name == "tasinmaz_id")
+                                {
+                                    tasinmazID = attribute.First.ToString();
+                                }
+
+                                if (column == jProperty.Name)
+                                {
+                                    for (int l = 0; l < fieldData.Count(); l++)
+                                    {
+                                        var field = fieldData[l];
+
+                                        if (field.Value<string>("name") == column)
+                                        {
+                                            var domainData = field["domain"];
+
+                                            if (domainData.Count() != 0)
+                                            {
+                                                var codedValues = domainData["codedValues"];
+                                                foreach (var codedValue in codedValues)
+                                                {
+                                                    try
+                                                    {
+                                                        if (codedValue["code"].Value<int>() == attribute.First.Value<int>())
+                                                        {
+                                                            var name = codedValue["name"].Value<string>();
+                                                            worksheet.Cells[cellRow, cellColumn].Value = name;
+                                                            break;
+                                                        }
+                                                    }
+                                                    catch (Exception ex)
+                                                    {
+                                                        worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                                        break;
+                                                    }
+
+                                                }
+                                            }
+                                            else
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                            }
+
+                                        }
+                                    }
+
+                                    break;
+                                }
+                                else if (column == "hisse")
+                                {
+                                    for (int m = 0; m < featureAttributes.Count(); m++)
+                                    {
+                                        var attribute2 = featureAttributes[m];
+                                        JProperty jProperty2 = attribute2.ToObject<JProperty>();
+
+                                        if (jProperty2.Name == "mulkiyet")
+                                        {
+                                            string value = attribute2.First.ToString();
+
+                                            if (value == "1")
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = "1";
+                                            }
+                                            else if (value == "2")
+                                            {
+                                                var formUrlEncodedContent2 = new FormUrlEncodedContent(new[]
+                                                {
+                                                    new KeyValuePair<string, string>("where", "tasinmazid ="+tasinmazID+" and UPPER(ad) LIKE '%MALİYE%'"),
+                                                    new KeyValuePair<string, string>("outfields", "pay"),
+                                                    new KeyValuePair<string, string>("returnGeometry", "false"),
+                                                    new KeyValuePair<string, string>("f", "pjson"),
+                                                    new KeyValuePair<string, string>("token", arcgisTokenDto.token)
+                                                });
+
+                                                response = await httpClient.PostAsync(malikTableUrL, formUrlEncodedContent2);
+                                                content = await response.Content.ReadAsStringAsync();
+                                                var arcgisResponseData2 = JObject.Parse(content);
+
+                                                if (arcgisResponseData2["features"].Count() != 0)
+                                                {
+                                                    var malikFeature = arcgisResponseData2["features"][0];
+                                                    var malikFeatureAtt = malikFeature["attributes"].ToArray();
+
+                                                    maliyeHazinesiPay = malikFeatureAtt[0].ToObject<JProperty>().First.ToString();
+                                                }
+                                                else
+                                                {
+                                                    maliyeHazinesiPay = "0";
+                                                }
+
+                                                worksheet.Cells[cellRow, cellColumn].Value = maliyeHazinesiPay;
+                                            }
+                                            else
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = "";
+                                            }
+                                            break;
+                                        }
+                                    }
+                                    break;
+                                }
+                            }
+                        }
+                    }
+
+                    offsetMultiplier++;
+                    offsetValue += featureCount;
+                }
+
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+
+        }
+
+        [HttpPost("exportExcelBufferAnalysis")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> exportExcelGeom([FromForm] IFormCollection collection)
+        {
+            using (var package = new ExcelPackage())
+            {
+                var offsetValue = 0;
+                var columnHeader = collection["headers"].ToString().Split(',').ToList();
+                var workSheetName = collection["workSheetName"].ToString();
+                var URI = collection["uri"].ToString();
+
+                var worksheet = package.Workbook.Worksheets.Add(workSheetName);
+
+                var tokenObject = await getToken();
+
+                HttpClient httpClient = new HttpClient();
+                Regex regex = new Regex(@"\/[0-9]{1}\/", RegexOptions.Compiled);
+
+                Match matchURI = regex.Match(URI);
+                var index = matchURI.Captures[0].Index + matchURI.Captures[0].Length - 1;
+                var serviceAddress = URI.Substring(0, index);
+                var requestURI = serviceAddress + "?f=json&token=" + tokenObject.token;
+
+                var response = await httpClient.GetAsync(requestURI);
+                var content = await response.Content.ReadAsStringAsync();
+                var arcgisServiceData = JObject.Parse(content);
+                var fieldData = arcgisServiceData["fields"];
+
+                using (var cells = worksheet.Cells[1, 1, 1, fieldData.Count()])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                for (int i = 0; i < fieldData.Count(); i++)
+                {
+                    var field = fieldData[i];
+                    worksheet.Cells[1, i + 1].Value = field.Value<string>("name");
+                }
+
+                int featureCount = 1;
+                int lastObjectid = 0;
+
+                URI = URI + "?token=" + tokenObject.token;
+                List<KeyValuePair<string, string>> keyValues = new List<KeyValuePair<string, string>>();
+                keyValues.Add(new KeyValuePair<string, string>("f", "json"));
+                //keyValues.Add(new KeyValuePair<string, string>("resultRecordCount", "1000"));
+                while (featureCount != 0)
+                {
+                    if (keyValues.Count <= 2)
+                    {
+                        foreach (var item in collection)
+                        {
+                            if (!(item.Key == "headers" || item.Key == "workSheetName" || item.Key == "uri"))
+                            {
+                                if (item.Key == "where")
+                                {
+                                    var itemValue = item.Value.ToString();
+                                    regex = new Regex(@" AND objectid>\d{1,}", RegexOptions.Compiled);
+                                    Match mathedObjectid = regex.Match(itemValue);
+                                    var mathedObjectidIndex = mathedObjectid.Captures[mathedObjectid.Captures.Count - 1].Index;
+
+                                    var subStractedValue = itemValue.Substring(0, mathedObjectidIndex);
+                                    var keyValue = item.Key;
+                                    var keyValuePair = new KeyValuePair<string, string>(keyValue, subStractedValue);
+                                    keyValues.Add(keyValuePair);
+                                }
+                                else
+                                {
+                                    var keyValue = item.Key;
+                                    var keyValuePair = new KeyValuePair<string, string>(keyValue, item.Value.ToString());
+                                    keyValues.Add(keyValuePair);
+                                }
+
+                            }
+
+                            if (item.Key == "geometry")
+                            {
+                                var itemValue = item.Value.ToString();
+                                if (itemValue.Contains("rings"))
+                                {
+                                    var geomTypeKeyValuePair = new KeyValuePair<string, string>("geometryType", "esriGeometryPolygon");
+                                    keyValues.Add(geomTypeKeyValuePair);
+                                }
+                                else if (itemValue.Contains("paths"))
+                                {
+                                    var geomTypeKeyValuePair = new KeyValuePair<string, string>("geometryType", "esriGeometryPolyline");
+                                    keyValues.Add(geomTypeKeyValuePair);
+                                }
+                                else
+                                {
+                                    var geomTypeKeyValuePair = new KeyValuePair<string, string>("geometryType", "esriGeometryMultipoint");
+                                    keyValues.Add(geomTypeKeyValuePair);
+                                }
+                            }
+                        }
+                    }
+                    var stringContent = new FormUrlEncodedContent(keyValues);
+
+                    response = await httpClient.PostAsync(URI, stringContent);
+                    content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+
+                    var features = arcgisResponseData["features"].ToArray();
+                    featureCount = features.Count();
+                    for (int i = 0; i < features.Count(); i++)
+                    {
+                        var feature = arcgisResponseData["features"][i];
+                        var featureAttributes = feature["attributes"].ToArray();
+
+                        for (int j = 0; j < featureAttributes.Count(); j++)
+                        {
+                            var cellRow = offsetValue + i + 2;
+                            var cellColumn = j + 1;
+                            var attribute = featureAttributes[j];
+                            JProperty jProperty = attribute.ToObject<JProperty>();
+                            for (int k = 0; j < fieldData.Count(); k++)
+                            {
+                                var field = fieldData[k];
+                                if (field.Value<string>("name") == jProperty.Name)
+                                {
+                                    var domainData = field["domain"];
+                                    if (domainData.Count() != 0)
+                                    {
+                                        var codedValues = domainData["codedValues"];
+                                        foreach (var codedValue in codedValues)
+                                        {
+                                            try
+                                            {
+                                                if (codedValue["code"].Value<int>() == attribute.First.Value<int>())
+                                                {
+                                                    var name = codedValue["name"].Value<string>();
+                                                    worksheet.Cells[cellRow, cellColumn].Value = name;
+                                                    break;
+                                                }
+                                            }
+                                            catch (Exception ex)
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                                break;
+                                            }
+
+                                        }
+                                    }
+                                    else
+                                    {
+                                        var type = field["type"].Value<string>();
+                                        if (type == "esriFieldTypeDate")
+                                        {
+                                            var dateString = attribute.First.ToString();
+                                            if (dateString != "")
+                                            {
+                                                var date = epoch.AddMilliseconds(Convert.ToDouble(dateString));
+                                                worksheet.Cells[cellRow, cellColumn].Value = date.ToString();
+                                            }
+                                            else
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                            }
+                                            break;
+                                        }
+                                        worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                    }
+                                    break;
+                                }
+                            }
+                        }
+                    }
+                    if (featureCount > 0)
+                    {
+                        offsetValue += featureCount;
+                        foreach (KeyValuePair<string, string> keyValuePair in keyValues)
+                        {
+                            int keyValueIndex = 0;
+                            if (keyValuePair.Key == "where")
+                            {
+                                var value = keyValuePair.Value.ToString();
+                                if (value.Length > 0)
+                                {
+
+                                    var stringValue = "objectid>" + lastObjectid;
+                                    int valueIndex = value.LastIndexOf(stringValue);
+                                    if (valueIndex < 0)
+                                    {
+                                        var lastFeature = arcgisResponseData["features"][featureCount - 1];
+                                        lastObjectid = lastFeature["attributes"]["objectid"].Value<int>();
+                                        value = "(" + value + ") AND objectid>" + lastObjectid;
+                                    }
+                                    else
+                                    {
+                                        var lastFeature = arcgisResponseData["features"][featureCount - 1];
+                                        lastObjectid = lastFeature["attributes"]["objectid"].Value<int>();
+                                        value = value.Replace(stringValue, "objectid>" + lastObjectid);
+                                    }
+                                }
+                                else
+                                {
+                                    var lastFeature = arcgisResponseData["features"][featureCount - 1];
+                                    lastObjectid = lastFeature["attributes"]["objectid"].Value<int>();
+                                    value = "objectid>" + lastObjectid;
+                                }
+                                keyValues.Remove(keyValuePair);
+                                keyValues.Insert(keyValueIndex, new KeyValuePair<string, string>("where", value));
+                                break;
+                            }
+                            keyValueIndex++;
+                        }
+                    }
+                }
+
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+
+            return Ok();
+        }
+
+        [HttpPost("exportExcelTkgmMalik")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> ExportExcelTkgmMalik([FromBody] ExportTkgmMalikData exportDataDto)
+        {
+            List<MalikData> malik = exportDataDto.malik;
+            List<Field> malikFields = exportDataDto.malikFields;
+
+            using (var package = new ExcelPackage())
+            {
+                var columnHeader = exportDataDto.headers;
+                var workSheetName = exportDataDto.workSheetName;
+
+                var worksheet = package.Workbook.Worksheets.Add(workSheetName);
+
+                using (var cells = worksheet.Cells[1, 1, 1, columnHeader.Count])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                worksheet.Cells[1, 1].Value = workSheetName;
+
+                for (int j = 0; j < columnHeader.Count(); j++)
+                {
+                    worksheet.Cells[2, j + 1].Value = columnHeader[j];
+                }
+
+                int i = 3;
+
+                foreach (var m in malik)
+                {
+                    for (var j = 0; j < malikFields.Count; j++)
+                    {
+                        var field = malikFields[j].fieldName;
+                        var fieldInfo = m.GetType().GetProperty(field);
+
+                        if (fieldInfo != null)
+                        {
+                            var value = fieldInfo.GetValue(m);
+
+                            worksheet.Cells[i, j + 1].Value = value;
+                        }
+                        else
+                        {
+                            continue;
+                        }
+
+                    }
+                    i++;
+                }
+
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+        }
+
+        [HttpPost("exportExcelMebMalik")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> ExportExcelMebMalik([FromBody] ExportMebMalikData exportDataDto)
+        {
+            List<MalikDataMeb> malik = exportDataDto.malik;
+            List<ArcgisField> malikFields = exportDataDto.malikFields;
+
+            using (var package = new ExcelPackage())
+            {
+                var columnHeader = exportDataDto.headers;
+                var workSheetName = exportDataDto.workSheetName;
+
+                var worksheet = package.Workbook.Worksheets.Add(workSheetName);
+
+                using (var cells = worksheet.Cells[1, 1, 1, columnHeader.Count])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                worksheet.Cells[1, 1].Value = workSheetName;
+
+                for (int j = 0; j < columnHeader.Count(); j++)
+                {
+                    worksheet.Cells[2, j + 1].Value = columnHeader[j];
+                }
+
+                int i = 3;
+
+                foreach (var m in malik)
+                {
+                    for (var j = 0; j < malikFields.Count; j++)
+                    {
+                        var field = malikFields[j].name;
+                        var type = malikFields[j].type;
+
+                        var fieldInfo = m.GetType().GetProperty(field);
+
+                        if (fieldInfo != null)
+                        {
+                            var val = fieldInfo.GetValue(m, null);
+
+                            if (type == "esriFieldTypeDate")
+                            {
+                                var date = epoch.AddMilliseconds(Convert.ToDouble(val));
+                                worksheet.Cells[i, j + 1].Value = date.ToString("dd.MM.yyyy");
+                            }
+                            else
+                            {
+                                worksheet.Cells[i, j + 1].Value = val;
+                            }
+                        }
+                        else
+                        {
+                            continue;
+                        }
+
+                    }
+                    i++;
+                }
+
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+        }
+
+        [HttpPost("exportExcelDynamic")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> ExportExcelDynamic([FromBody] exportExcelDynamicDto exportExcelDynamicDto)
+        {
+            var offsetValue = 0;
+            using (var package = new ExcelPackage())
+            {
+                var fields = exportExcelDynamicDto.fields;
+                var workSheetName = exportExcelDynamicDto.workSheetName;
+                var uri = exportExcelDynamicDto.uri;
+
+                var worksheet = package.Workbook.Worksheets.Add(workSheetName);
+
+                using (var cells = worksheet.Cells[1, 1, 1, fields.Count])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                for (int j = 0; j < fields.Count(); j++)
+                {
+                    var splittedName = fields[j].name.Split('.');
+                    var name = splittedName[splittedName.Length - 2];
+                    worksheet.Cells[1, j + 1].Value = name + '.' + fields[j].alias;
+                }
+
+                var tokenObject = await getToken();
+                HttpClient httpClient = new HttpClient();
+
+                var requestURI = uri + "&token=" + tokenObject.token;
+
+                int featureCount = 1;
+
+                while (featureCount != 0)
+                {
+                    var response = await httpClient.GetAsync(requestURI);
+                    var content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+
+                    var features = arcgisResponseData["features"].ToArray();
+                    featureCount = features.Count();
+
+                    for (int i = 0; i < features.Count(); i++)
+                    {
+                        var feature = arcgisResponseData["features"][i];
+                        var featureAttributes = feature["attributes"].ToArray();
+
+                        for (int j = 0; j < featureAttributes.Count(); j++)
+                        {
+                            var cellRow = offsetValue + i + 2;
+                            var cellColumn = j + 1;
+                            var attribute = featureAttributes[j];
+                            JProperty jProperty = attribute.ToObject<JProperty>();
+                            for (int k = 0; j < fields.Count(); k++)
+                            {
+                                var field = fields[k];
+                                if (field.name == jProperty.Name)
+                                {
+                                    var domainData = field.domain;
+                                    if (domainData != null)
+                                    {
+                                        var codedValues = domainData.codedValues;
+                                        foreach (var codedValue in codedValues)
+                                        {
+                                            try
+                                            {
+                                                if (codedValue.code == attribute.First.Value<int>())
+                                                {
+                                                    var name = codedValue.name;
+                                                    worksheet.Cells[cellRow, cellColumn].Value = name;
+                                                    break;
+                                                }
+                                            }
+                                            catch (Exception ex)
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                                break;
+                                            }
+
+                                        }
+                                    }
+                                    else
+                                    {
+                                        var type = field.type;
+                                        if (type == "esriFieldTypeDate")
+                                        {
+                                            var dateString = attribute.First.ToString();
+                                            if (dateString != "")
+                                            {
+                                                var date = epoch.AddMilliseconds(Convert.ToDouble(dateString));
+                                                worksheet.Cells[cellRow, cellColumn].Value = date.ToString();
+                                            }
+                                            else
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                            }
+                                            break;
+                                        }
+                                        worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                    }
+                                    break;
+                                }
+                            }
+                        }
+                    }
+
+                    if (featureCount > 0)
+                    {
+                        offsetValue += featureCount;
+                        Regex reg = new Regex(@"resultOffset=[0-9]{1,10}&", RegexOptions.Compiled);
+
+                        Match match = reg.Match(requestURI);
+                        var matchLength = match.Captures[0].Length;
+                        var matchIndex = match.Captures[0].Index;
+
+                        var startString = requestURI.Substring(0, matchIndex);
+                        var endString = requestURI.Substring(matchIndex + matchLength - 1);
+                        var offSetCount = match.Value.TrimEnd('&');
+
+                        var splittedText = offSetCount.Split('=');
+                        offsetValue = int.Parse(splittedText[1]) + 1000;
+
+                        requestURI = startString + "resultOffset=" + (offsetValue) + endString;
+                    }
+                }
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+        }
+
+        [HttpPost("addLog")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> createLog([FromBody] Log log)
+        {
+            _logRepository.Add(log);
+
+            return Ok();
+        }
