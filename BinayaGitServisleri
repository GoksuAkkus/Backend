+        [HttpGet("getHisseByTasinmazId")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<HisseObject> GetHisseByTasinmazId(decimal tasinmazId, decimal malikId)
+        {
+            List<HisseObjectAttribute> HisseList = new List<HisseObjectAttribute>();
+
+            Hisse[] hisseList = await GetirHisseTasinmazIDDen(tasinmazId, TapuBolumDurum.Aktif);
+
+            if (hisseList.Length > 0)
+            {
+                Hisse hisse = hisseList.ToList().Find(h => h.Malik.ID == malikId);
+
+                if (hisse != null)
+                {
+                    HisseObjectAttribute hisseObj = new HisseObjectAttribute()
+                    {
+                        tasinmazID = hisse.TasinmazID,
+                        pay = hisse.Pay,
+                        payda = hisse.Payda,
+                        tip = hisse.Tip,
+                        tapuBolumDurum = hisse.TapuBolumDurum
+                    };
+
+                    if (hisse.Malik != null)
+                    {
+                        Malik malik = hisse.Malik;
+
+                        hisseObj.malikId = malik.ID;
+                        hisseObj.malikOzetBilgi = malik.MalikOzetBilgi;
+                        hisseObj.malikTip = malik.MalikTip;
+                    }
+
+                    if (hisse.TesisIslem != null)
+                    {
+                        hisseObj.islemTanimAd = hisse.TesisIslem.IslemTanimAd;
+                        hisseObj.baslamaSekilAd = hisse.TesisIslem.BaslamaSekilAd;
+
+                        if (hisse.TesisIslem.Yevmiye != null)
+                        {
+                            hisseObj.islemTarih = hisse.TesisIslem.Yevmiye.Tarih;
+                        }
+                    }
+
+                    HisseList.Add(hisseObj);
+                }
+            }
+
+            HisseObject hisseObject = new HisseObject()
+            {
+                hisseList = HisseList,
+                hisseFields = CreateHisseFields()
+            };
+
+            return hisseObject;
+
+        }
+
+        private async Task<List<TKGMFeatureSet>> SetTKGMFeatureSet(string cql_filter)
+        {
+            List<MEGSISParselDTO> parcelList = await GetParcelInfo(cql_filter);
+            List<TKGMFeatureSet> responseList = new List<TKGMFeatureSet>();
+            if (parcelList.Count > 0)
+            {
+                foreach (MEGSISParselDTO MEGSISparcel in parcelList)
+                {
+                    if (MEGSISparcel.tapuKimlikNO != -1)
+                    {
+                        Tasinmaz tasinmaz = await GetirTasinmazIDDen(MEGSISparcel.tasinmazID);
+                        DateTime tarih = DateTime.MinValue;
+
+
+                        ParcelObject TKGMParcelObject = new ParcelObject()
+                        {
+                            tasinmazID = tasinmaz.ID,
+                            tapuKimlikNO = MEGSISparcel.tapuKimlikNO,
+                            il = tasinmaz.Il,
+                            ilce = tasinmaz.Ilce,
+                            mahalle = tasinmaz.Mahalle,
+                            mahalleID = tasinmaz.MahalleID,
+                            nitelik = tasinmaz.Nitelik,
+                            ada = tasinmaz.Ada == null ? "0" : tasinmaz.Ada,
+                            parsel = tasinmaz.Parsel,
+                            tasinmazTip = tasinmaz.Tip,
+                            kadastroAlan = Math.Round(MEGSISparcel.kadastroAlan, 2),
+                            tapuAlan = tasinmaz.Yuzolcum,
+                            tapuBolumDurum = tasinmaz.TapuBolumDurum,
+                            //parselTip = MEGSISparcel.tip,
+                            tarih = tarih,
+                        };
+
+                        if (tasinmaz.TesisIslem != null)
+                        {
+                            if (tasinmaz.TesisIslem.Yevmiye != null)
+                            {
+                                TKGMParcelObject.tarih = tasinmaz.TesisIslem.Yevmiye.Tarih;
+                            }
+
+                            TKGMParcelObject.edinimSekli = tasinmaz.TesisIslem.IslemTanimAd;
+                        }
+
+                        TKGMGeometryObject geom = new TKGMGeometryObject()
+                        {
+                            type = "polygon",
+                            rings = MEGSISparcel.geom,
+                        };
+                        List<Field> parcelObjectFields = GetParcelObjectFields();
+
+                        TKGMFeatureSet TKGMFeatureSet = new TKGMFeatureSet()
+                        {
+                            attributes = TKGMParcelObject,
+                            fields = parcelObjectFields,
+                            geom = geom,
+                        };
+                        responseList.Add(TKGMFeatureSet);
+                    }
+                }
+                return responseList;
+            }
+            else
+            {
+                return responseList;
+            }
+        }
+
+        private async Task<List<TKGMFeatureSet>> SetTKGMActiveOrPassiveFeatureSet(MEGSISSearchParcelRequestDto MEGSISSearchParcelRequest)
+        {
+            string cql_filter = "tapumahalleref=" + MEGSISSearchParcelRequest.mahalleIds[0] + " and adano=" + MEGSISSearchParcelRequest.adano +
+                " and parselno=" + MEGSISSearchParcelRequest.parselno;
+
+            List<MEGSISParselDTO> parcelList = await GetParcelInfo(cql_filter);
+            List<TKGMFeatureSet> responseList = new List<TKGMFeatureSet>();
+            if (parcelList.Count > 0)
+            {
+                foreach (MEGSISParselDTO MEGSISparcel in parcelList)
+                {
+                    Tasinmaz tasinmaz = await GetirTasinmazIDDen(parcelList[0].tasinmazID);
+                    DateTime tarih = DateTime.MinValue;
+
+                    if (tasinmaz.TesisIslem != null && tasinmaz.TesisIslem.Yevmiye != null)
+                    {
+                        tarih = tasinmaz.TesisIslem.Yevmiye.Tarih;
+                    }
+
+                    ParcelObject TKGMParcelObject = new ParcelObject()
+                    {
+                        tasinmazID = tasinmaz.ID,
+                        tapuKimlikNO = MEGSISparcel.tapuKimlikNO,
+                        il = tasinmaz.Il,
+                        ilce = tasinmaz.Ilce,
+                        mahalle = tasinmaz.Mahalle,
+                        mahalleID = tasinmaz.MahalleID,
+                        nitelik = tasinmaz.Nitelik,
+                        ada = tasinmaz.Ada == null ? "0" : tasinmaz.Ada,
+                        parsel = tasinmaz.Parsel,
+                        tasinmazTip = tasinmaz.Tip,
+                        kadastroAlan = Math.Round(MEGSISparcel.kadastroAlan, 2),
+                        tapuAlan = tasinmaz.Yuzolcum,
+                        tapuBolumDurum = tasinmaz.TapuBolumDurum,
+                        parselTip = MEGSISparcel.tip,
+                        tarih = tarih,
+                        edinimSekli = tasinmaz.TesisIslem != null ? tasinmaz.TesisIslem.IslemTanimAd : "",
+                    };
+
+                    TKGMGeometryObject geom = new TKGMGeometryObject()
+                    {
+                        type = "polygon",
+                        rings = MEGSISparcel.geom,
+                    };
+                    List<Field> parcelObjectFields = GetParcelObjectFields();
+
+                    TKGMFeatureSet TKGMFeatureSet = new TKGMFeatureSet()
+                    {
+                        attributes = TKGMParcelObject,
+                        fields = parcelObjectFields,
+                        geom = geom,
+                    };
+                    responseList.Add(TKGMFeatureSet);
+                }
+                return responseList;
+            }
+            else
+            {
+                Tasinmaz[] tasinmazList = await GetirAnaTasinmaz(MEGSISSearchParcelRequest.mahalleIds, MEGSISSearchParcelRequest.adano, MEGSISSearchParcelRequest.parselno, TapuBolumDurum.Hepsi);
+
+                foreach (Tasinmaz tasinmaz in tasinmazList)
+                {
+                    DateTime tarih = DateTime.MinValue;
+
+                    if (tasinmaz.TesisIslem.Yevmiye != null)
+                    {
+                        tarih = tasinmaz.TesisIslem.Yevmiye.Tarih;
+                    }
+
+                    ParcelObject TKGMPassiveParcelObject = new ParcelObject()
+                    {
+                        tasinmazID = tasinmaz.ID,
+                        il = tasinmaz.Il,
+                        ilce = tasinmaz.Ilce,
+                        mahalle = tasinmaz.Mahalle,
+                        mahalleID = tasinmaz.MahalleID,
+                        nitelik = tasinmaz.Nitelik,
+                        ada = tasinmaz.Ada == null ? "0" : tasinmaz.Ada,
+                        parsel = tasinmaz.Parsel,
+                        tasinmazTip = tasinmaz.Tip,
+                        tapuAlan = tasinmaz.Yuzolcum,
+                        tapuBolumDurum = tasinmaz.TapuBolumDurum,
+                        tarih = tarih,
+                        edinimSekli = tasinmaz.TesisIslem.IslemTanimAd,
+                    };
+
+                    List<Field> parcelObjectFields = GetParcelObjectFields();
+
+
+                    TKGMFeatureSet TKGMFeatureSet = new TKGMFeatureSet()
+                    {
+                        attributes = TKGMPassiveParcelObject,
+                        fields = parcelObjectFields,
+                        geom = null,
+
+                    };
+                    responseList.Add(TKGMFeatureSet);
+                }
+            }
+            return responseList;
+        }
+
+        [HttpPost("getParcelInfoByCqlFilter")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<List<TKGMFeatureSet>> GetParcelInfoByCqlFilter([FromBody] MEGSISSearchParcelRequestDto MEGSISSearchParcelRequest)
+        {
+            List<TKGMFeatureSet> tkgmFeatureSetList = new List<TKGMFeatureSet>();
+
+            if (MEGSISSearchParcelRequest.mahalleIds[0] == 0 &&
+                (MEGSISSearchParcelRequest.adano != null && MEGSISSearchParcelRequest.parselno != null))
+            {
+                int ilceID = MEGSISSearchParcelRequest.ilceID;
+
+                Mahalle[] mahalleList = await GetirMahalleIlceIDDen(ilceID, Durum.Aktif);
+
+                for (var i = 0; i < mahalleList.Length; i++)
+                {
+
+                    MEGSISSearchParcelRequest.mahalleIds[0] = mahalleList[i].ID;
+                    List<TKGMFeatureSet> parcelList = await SetTKGMActiveOrPassiveFeatureSet(MEGSISSearchParcelRequest);
+
+                    if (parcelList.Count() > 0)
+                    {
+                        tkgmFeatureSetList.Add(parcelList[0]);
+                    }
+                }
+                return tkgmFeatureSetList;
+            }
+            else
+            {
+                tkgmFeatureSetList = await SetTKGMActiveOrPassiveFeatureSet(MEGSISSearchParcelRequest);
+
+                List<TKGMFeatureSet> tempList = new List<TKGMFeatureSet>();
+
+                tempList.Add(tkgmFeatureSetList[0]);
+
+                return tempList;
+            }
+        }
+
+        [HttpPost("parcelInfo")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<List<TKGMFeatureSet>> GetIntersectingParcelInfo([FromBody] TKGMIntersectingParcelRequest tkgmIntersectingParcelRequest)
+        {
+            string cql_filter = "INTERSECTS(geom,POINT(" + tkgmIntersectingParcelRequest.pointData + ")) and durum=3";
+
+            List<TKGMFeatureSet> tkgmFeatureSetList = await SetTKGMFeatureSet(cql_filter);
+
+            return tkgmFeatureSetList;
+        }
+
+        [HttpPost("getParcelsByPlanGeometry")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<List<TKGMParcelAndMalikSBIDto>> GetParcelsFromTKGMByPlanGeometry([FromBody] TKGMIntersectingPlanParcelRequest tkgmIntersectingPlanParcelRequest)
+        {
+            string cql_filter = SetCqlFilterByRings(tkgmIntersectingPlanParcelRequest.rings);
+
+            List<TKGMFeatureSet> tkgmFeatureSetList = await SetTKGMFeatureSet(cql_filter);
+            List<TKGMParcelAndMalikSBIDto> parcelAndMalikSbiList = new List<TKGMParcelAndMalikSBIDto>();
+            if (tkgmFeatureSetList.Count() > 0)
+            {
+                foreach (var feature in tkgmFeatureSetList)
+                {
+
+
+                    ParcelObject attributes = feature.attributes;
+                    decimal tasinmazID = attributes.tasinmazID;
+                    decimal mahalleID = attributes.mahalleID;
+                    string adaNo = attributes.ada;
+                    string parselNo = attributes.parsel;
+                    double tapuAlan = Decimal.ToDouble(attributes.tapuAlan);
+
+                    SerhBeyanMalikData serhBeyanMalikData = await GetMalikSerhBeyan(tasinmazID, tapuAlan, mahalleID, adaNo, parselNo);
+
+                    TKGMParcelAndMalikSBIDto tkgmParcelAndMalikSBI = new TKGMParcelAndMalikSBIDto()
+                    {
+                        serhBeyanMalikData = serhBeyanMalikData,
+                        parcel = feature
+                    };
+                    parcelAndMalikSbiList.Add(tkgmParcelAndMalikSBI);
+                }
+            }
+
+            return parcelAndMalikSbiList;
+        }
+
+        public string SetCqlFilterByRings(List<List<double[]>> rings)
+        {
+            List<double[]> tempRings = new List<double[]>();
+            tempRings = rings[0];
+
+            var geometryString = "";
+            for (var i = 0; i < tempRings.Count; i++)
+            {
+                string coordx = tempRings[i][0].ToString(CultureInfo.CreateSpecificCulture("en-US"));
+                string coordy = tempRings[i][1].ToString(CultureInfo.CreateSpecificCulture("en-US"));
+
+                var item = coordx + " " + coordy;
+                geometryString += item;
+                if (i != tempRings.Count - 1)
+                {
+                    geometryString += ",";
+                }
+            }
+            string cql_filter = "INTERSECTS(geom, MULTIPOLYGON(((" + geometryString + ")))) and durum=3";
+
+            return cql_filter;
+        }
+
+        [HttpPost("getGeometry")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<TKGMGeometryObject> GetGeometry([FromBody] MEGSISGeometryRequestDto MEGSISGeometryRequest)
+        {
+            HttpClient httpClient = new HttpClient();
+            httpClient = GetMEGSISHeader(httpClient);
+
+            var megsisURL = _configuration.GetSection("MEGSISConnection:MEGSISURL").Value;
+            var formUrlEncodedContent = new FormUrlEncodedContent(new[]
+            {
+                new KeyValuePair<string, string>("request","GetFeature"),
+                new KeyValuePair<string, string>("service", "WFS"),
+                new KeyValuePair<string, string>("typeName", MEGSISGeometryRequest.typeName),
+                new KeyValuePair<string, string>("srsName", "EPSG:4326"),
+                new KeyValuePair<string, string>("propertyName","geom"),
+                new KeyValuePair<string, string>("cql_filter",MEGSISGeometryRequest.cql_filter),
+            });
+
+            var response = await httpClient.PostAsync(megsisURL, formUrlEncodedContent);
+            var content = await response.Content.ReadAsStringAsync();
+
+            XmlDocument doc = new XmlDocument();
+            doc.LoadXml(content);
+            XmlNode collection = doc.LastChild;
+            collection.RemoveChild(collection.FirstChild);
+
+            XmlNode featureAttributes = collection.FirstChild.FirstChild;
+            string geom = featureAttributes["TKGM:geom"].InnerText;
+            var rings = convertPolygonData(featureAttributes["TKGM:geom"]);
+            if (geom != null)
+            {
+                TKGMGeometryObject geometryObject = new TKGMGeometryObject()
+                {
+                    rings = rings,
+                    type = "polygon"
+                };
+                return geometryObject;
+            }
+            else
+            {
+                return null;
+            }
+        }
+
