+        [HttpGet("GetAllMahalle")]
+        public async Task<List<Mahalle>> GetAllMahalle()
+        {
+            Il[] il_All = await GetirIl();
+
+            List<Mahalle> mahalle_All = new List<Mahalle>();
+
+            //foreach (Il il in il_All)
+            //{
+            Ilce[] il_ilce = await GetirIlceIlIDDen(il_All[10].ID); //İL İL ÇEKİLECEK
+
+            Console.WriteLine("İL ADI: {0} / İL ID: {1} ", il_All[10].Ad, il_All[10].ID);
+
+            foreach (Ilce ilce in il_ilce)
+            {
+                Mahalle[] ilce_mahalle = await GetirMahalleIlceIDDen(ilce.ID, Durum.Aktif); //Durum parametresi aktif olan bütün mahalleleri getir.
+
+                mahalle_All.AddRange(ilce_mahalle);
+            }
+            //}
+            //İLLERİ GETİR İLLERİN İLÇELERİNİ GETİR. İLÇELERİN MAHALLELERİNİ ALLMAHALLEYE KAYDET
+            return mahalle_All;
+        }
