+        [HttpPut("ResetPassword/{id}")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public IActionResult ResetPasswordById(int id, [FromBody] AdminForResetPasswordDto adminForResetPasswordDto)
+        {
+            _authRepository.ResetPassword(id, adminForResetPasswordDto.NewPassword);
+            return Ok(adminForResetPasswordDto);
