+
+namespace MebCbsAnka.Dtos.ArcgisDtos
+{
+    public class ArcgisParcelItem
+    {
+        public ParcelItemAttribute attributes { get; set; }
+        public ParcelItemGeometry geometry { get; set; }
+    }
+
+    public class ParcelItemAttribute
+    {
+        public Int32 objectid { get; set; }
+        public long kimlik_no { get; set; }
+        public decimal tasinmaz_id { get; set; }
+        public string tkgm_ilce { get; set; }
+        public string mahalle { get; set; }
+        public string ada_no { get; set; }
+        public string parsel_no { get; set; }
+        public string kadastro_alan { get; set; }
+        public Mulkiyet mulkiyet { get; set; }
+        public string tapu_cins { get; set; }
+        public string meb_edinim_sekli { get; set; }
+        public DateTime meb_edinim_tarih { get; set; }
+        public double? maliyet_bedeli { get; set; }
+        public double? rayic_bedeli { get; set; }
+        public double? iz_bedeli { get; set; }
+        public double? vergi_degeri { get; set; }
+        public double? kayit_neden { get; set; }
+        public DateTime kayit_tarih { get; set; }
+        public TasinmazTip tapu_parsel_tip { get; set; }
+        public int kadastro_parsel_tip { get; set; }
+        public int il { get; set; }
+        public int ilce { get; set; }
+        public string tkgm_il { get; set; }
+        public string nitelik { get; set; }
+        public DateTime yevmiye_tarih { get; set; }
+        public decimal yevmiye_no { get; set; }
+        public string baslama_sekil_ad { get; set; }
+        public TapuBolumDurum tapu_durum { get; set; }
+        public string edinme_sekli { get; set; }
+        public string tapu_alan { get; set; }
+        public decimal mahalle_id { get; set; }
+    }
+
+    public class ParcelItemGeometry
+    {
+        public List<List<double[]>> rings { get; set; }
+    }
+
+    public enum Mulkiyet
+    {
+        None,
+        MüstakilHazine,
+        MüşterekHazine,
+        Kamu,
+        Şahıs,
+        KamuŞahıs
+    }
