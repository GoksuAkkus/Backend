+        private List<Field> CreateSerhBeyanFields()
+        {
+            List<Field> SerhBeyanObjectFields = new List<Field>();
+
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Taşınmaz ID",
+                fieldName = "tasinmazID",
+                type = "number",
+                arcgisFieldName = "tasinmazid"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Şerh Beyan Tip",
+                fieldName = "serhBeyanTip",
+                type = "string",
+                arcgisFieldName = "serh_beyan_tip",
+                domain = new List<string>()
+                {
+                    "Hiçbiri","Şerh", "Beyan", "İrtifak"
+                }
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Şablon",
+                fieldName = "sablon",
+                type = "string",
+                arcgisFieldName = "sablon"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Şerh Beyan Durum",
+                fieldName = "serhBeyanDurum",
+                type = "string",
+                arcgisFieldName = "serh_beyan_durum",
+                domain = new List<string>()
+                {
+                    "Hepsi","Taslak","Aktif","Pasif"
+                }
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Cümle",
+                fieldName = "cumle",
+                type = "string",
+                arcgisFieldName = "cumle"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Başlama Tarih",
+                fieldName = "baslamaTarih",
+                type = "date",
+                arcgisFieldName = "baslama_tarih",
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Bitiş Tarih",
+                fieldName = "bitisTarih",
+                type = "date",
+                arcgisFieldName = "bitis_tarih",
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Süre",
+                fieldName = "sure",
+                type = "number",
+                arcgisFieldName = "sure"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Tapu Bölüm",
+                fieldName = "tapuBolum",
+                type = "string",
+                arcgisFieldName = "tapu_bolum",
+                domain = new List<string>()
+                {
+                    "Hiçbiri","Zemin", "Zemin Hisse", "Zemin Mühdesat","Zemin Mühdesat Malik",
+                    "Şerh Beyan Malik", "Kişi", "Tüzel Kişi","Mahalle Köy","Ülke", "Organizasyon",
+                    "Şerh","Beyan", "Rehin","Rehin Miktar","Irtifak",
+                }
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Tesis İşlem TanımAd",
+                fieldName = "tesisIslemTanimAd",
+                type = "string",
+                arcgisFieldName = "tesis_islem_tanim_ad"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Tesis İşlem Baslama ŞekilAd",
+                fieldName = "tesisIslemBaslamaSekilAd",
+                type = "string",
+                arcgisFieldName = "tesis_islem_baslama_sekil_ad"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Tesis İşlem Tarih",
+                fieldName = "tesisIslemTarih",
+                type = "date",
+                arcgisFieldName = "tesis_islem_tarih"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Terkin İşlem TanımAd",
+                fieldName = "terkinIslemTanimAd",
+                type = "string",
+                arcgisFieldName = "terkin_islem_tanim_ad"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Terkin İşlem Baslama ŞekilAd",
+                fieldName = "terkinIslemBaslamaSekilAd",
+                type = "string",
+                arcgisFieldName = "terkin_islem_baslama_sekil_ad"
+            });
+            SerhBeyanObjectFields.Add(new Field()
+            {
+                alias = "Terkin İşlem Tarih",
+                fieldName = "terkinIslemTarih",
+                type = "date",
+                arcgisFieldName = "terkin_islem_tarih"
+            });
+
+            return SerhBeyanObjectFields;
+        }
+
+        private List<Field> CreateTasinmazFields()
+        {
+            List<Field> tasinmazObjectFields = new List<Field>();
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Taşınmaz ID",
+                fieldName = "tasinmazID",
+                type = "number"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Malik ID",
+                fieldName = "malikID",
+                type = "number"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "İl",
+                fieldName = "il",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "İlçe",
+                fieldName = "ilce",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Mahalle",
+                fieldName = "mahalle",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Kurum",
+                fieldName = "kurum",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Kurum ID",
+                fieldName = "kurumID",
+                type = "number"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Mahalle ID",
+                fieldName = "mahalleID",
+                type = "number"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Nitelik",
+                fieldName = "nitelik",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Sayfa No",
+                fieldName = "sayfaNo",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Cilt No",
+                fieldName = "ciltNo",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Ada",
+                fieldName = "ada",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Parsel",
+                fieldName = "parsel",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Taşınmaz Tip",
+                fieldName = "tasinmazTip",
+                type = "string",
+                domain = new List<string>(new string[] { "None", "Ana Taşınmaz", "Bağımsız Bölüm", "Daimi Müstakil Hak", "KOM" }),
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Yuzolcum",
+                fieldName = "yuzolcum",
+                type = "number"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Pafta",
+                fieldName = "pafta",
+                type = "number"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Tapu Bölüm Durum",
+                fieldName = "tapuBolumDurum",
+                type = "string",
+                domain = new List<string>(new string[] { "Hepsi", "Taslak", "Aktif", "Pasif" }),
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Islem Tanim Ad",
+                fieldName = "islemTanimAd",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Baslama Sekil Ad",
+                fieldName = "baslamaSekilAd",
+                type = "string"
+            });
+            tasinmazObjectFields.Add(new Field()
+            {
+                alias = "Islem Tarihi",
+                fieldName = "islemTarih",
+                type = "date"
+            });
+
+            return tasinmazObjectFields;
+        }
+
+        private async Task<List<MalikData>> GetMalik(decimal tasinmazID, double tapuAlan)
+        {
+            Hisse[] shares = await GetirHisseTasinmazIDDen(tasinmazID, TapuBolumDurum.Hepsi);
+            List<MalikData> malikData = new List<MalikData>();
+
+            for (int i = 0; i < shares.Count(); i++)
+            {
+                Hisse share = shares[i];
+                Malik shareHolder = share.Malik;
+                decimal malikID = shareHolder.ID;
+                MalikData malikShareHolder = new MalikData();
+                if (shareHolder.MalikTip == MalikTip.TuzelKisi)
+                {
+                    TuzelKisi tuzelKisi = await GetirTuzelKisiIDDen(malikID);
+                    MalikData entityShareHolder = new MalikData()
+                    {
+                        tasinmazID = share.TasinmazID,
+                        pay = Double.Parse(share.Pay),
+                        payda = Double.Parse(share.Payda),
+                        hisseTip = share.Tip,
+                        hisseDurum = share.TapuBolumDurum,
+                        malikTip = share.Malik.MalikTip,
+                        ad = tuzelKisi.Ad,
+                        tuzelKisiTip = tuzelKisi.TuzelKisiTipi,
+                        vergiNo = tuzelKisi.VergiNo,
+                        sicilNo = tuzelKisi.SicilNo,
+                        yabanciTemsilcili = tuzelKisi.YabanciTemsilcili,
+                        yabanciSermayeli = tuzelKisi.YabanciSermayeli,
+                        hisseId = share.ID
+
+                    };
+                    malikShareHolder = entityShareHolder;
+                }
+                else if (shareHolder.MalikTip == MalikTip.GercekKisi)
+                {
+                    GercekKisi gercekKisi = await GetirGercekKisiIDDen(malikID);
+                    MalikData personShareHolder = new MalikData()
+                    {
+                        tasinmazID = share.TasinmazID,
+                        pay = Double.Parse(share.Pay),
+                        payda = Double.Parse(share.Payda),
+                        hisseTip = share.Tip,
+                        hisseDurum = share.TapuBolumDurum,
+                        malikTip = share.Malik.MalikTip,
+                        tcKimlikNo = gercekKisi.TCKimlikNo.ToString(),
+                        ad = gercekKisi.Ad,
+                        soyad = gercekKisi.Soyad,
+                        babaAd = gercekKisi.BabaAd,
+                        anaAd = gercekKisi.AnaAd,
+                        dogumTarih = gercekKisi.DogumTarih,
+                        dogumYer = gercekKisi.DogumYer,
+                        cilt = gercekKisi.Cilt,
+                        sira = gercekKisi.Sira,
+                        cinsiyet = gercekKisi.Cinsiyet,
+                        olumTarih = gercekKisi.OlumTarih,
+                        hisseId = share.ID
+                    };
+                    malikShareHolder = personShareHolder;
+                }
+                if (share.TesisIslem != null)
+                {
+                    malikShareHolder.islemTanimAd = share.TesisIslem.IslemTanimAd;
+                    malikShareHolder.baslamaSekilAd = share.TesisIslem.BaslamaSekilAd;
+                    if (share.TesisIslem.Yevmiye != null)
+                    {
+                        malikShareHolder.islemTarih = share.TesisIslem.Yevmiye.Tarih;
+                    }
+                }
+
+                malikShareHolder.hisseYuzOlcum = Math.Round(tapuAlan * (malikShareHolder.pay / malikShareHolder.payda), 2);
+                malikData.Add(malikShareHolder);
+            }
+
+            return malikData;
+        }
+
+        [HttpGet("getSBIByHisseId")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<SerhBeyanMalikData> GetirSBIHisseIDDen(decimal hisseID)
+        {
+            List<SerhBeyan> serhBeyanIrtifakData = new List<SerhBeyan>();
+            List<SerhBeyanData> serhBeyanData = new List<SerhBeyanData>();
+
+            SerhBeyan[] tuzelKisi = await GetirSerhBeyan(TapuBolum.TuzelKisi, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] beyan = await GetirSerhBeyan(TapuBolum.Beyan, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] irtifak = await GetirSerhBeyan(TapuBolum.Irtifak, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] kisi = await GetirSerhBeyan(TapuBolum.Kisi, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] mahalleKoy = await GetirSerhBeyan(TapuBolum.MahalleKoy, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] none = await GetirSerhBeyan(TapuBolum.None, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] organizasyon = await GetirSerhBeyan(TapuBolum.Organizasyon, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] rehin = await GetirSerhBeyan(TapuBolum.Rehin, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] rehinMiktar = await GetirSerhBeyan(TapuBolum.RehinMiktar, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] serh = await GetirSerhBeyan(TapuBolum.Serh, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] serhBeyanMalik = await GetirSerhBeyan(TapuBolum.SerhBeyanMalik, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] ulke = await GetirSerhBeyan(TapuBolum.Ulke, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] zemin = await GetirSerhBeyan(TapuBolum.Zemin, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] zeminHisse = await GetirSerhBeyan(TapuBolum.ZeminHisse, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] zeminMuhdesat = await GetirSerhBeyan(TapuBolum.ZeminMuhdesat, hisseID, TapuBolumDurum.Aktif);
+            SerhBeyan[] zeminMuhdesatMalik = await GetirSerhBeyan(TapuBolum.ZeminMuhdesatMalik, hisseID, TapuBolumDurum.Aktif);
+
+            serhBeyanIrtifakData.AddRange(beyan);
+            serhBeyanIrtifakData.AddRange(irtifak);
+            serhBeyanIrtifakData.AddRange(kisi);
+            serhBeyanIrtifakData.AddRange(mahalleKoy);
+            serhBeyanIrtifakData.AddRange(none);
+            serhBeyanIrtifakData.AddRange(organizasyon);
+            serhBeyanIrtifakData.AddRange(rehin);
+            serhBeyanIrtifakData.AddRange(rehinMiktar);
+            serhBeyanIrtifakData.AddRange(serh);
+            serhBeyanIrtifakData.AddRange(serhBeyanMalik);
+            serhBeyanIrtifakData.AddRange(tuzelKisi);
+            serhBeyanIrtifakData.AddRange(ulke);
+            serhBeyanIrtifakData.AddRange(zemin);
+            serhBeyanIrtifakData.AddRange(zeminHisse);
+            serhBeyanIrtifakData.AddRange(zeminMuhdesat);
+            serhBeyanIrtifakData.AddRange(zeminMuhdesatMalik);
+
+            for (int i = 0; i < serhBeyanIrtifakData.Count; i++)
+            {
+                SerhBeyan serhBeyan = serhBeyanIrtifakData[i];
+                string sablon = serhBeyan.Sablon;
+                string cumle = serhBeyan.Cumle;
+
+                SerhBeyanData serhData = new SerhBeyanData()
+                {
+                    tasinmazID = serhBeyan.TapuBolumReferans,
+                    serhBeyanTip = serhBeyan.Tip,
+                    sablon = sablon.Length > 256 ? sablon.Substring(0, 255) : sablon,
+                    serhBeyanDurum = serhBeyan.TapuBolumDurum,
+                    cumle = cumle.Length > 256 ? cumle.Substring(0, 255) : cumle,
+                    baslamaTarih = serhBeyan.BaslamaTarih,
+                    bitisTarih = serhBeyan.BitisTarih,
+                    sure = serhBeyan.Sure,
+                    tapuBolum = serhBeyan.TapuBolum
+                };
+
+                if (serhBeyan.TerkinIslem != null)
+                {
+                    Islem terkinIslem = serhBeyan.TerkinIslem;
+
+                    serhData.terkinIslemTanimAd = terkinIslem.IslemTanimAd;
+                    serhData.terkinIslemBaslamaSekilAd = terkinIslem.BaslamaSekilAd;
+
+                    if (terkinIslem.Yevmiye != null)
+                    {
+                        serhData.terkinIslemTarih = terkinIslem.Yevmiye.Tarih;
+                    }
+                }
+
+                if (serhBeyan.TesisIslem != null)
+                {
+                    Islem tesisIslem = serhBeyan.TesisIslem;
+
+                    serhData.tesisIslemTanimAd = tesisIslem.IslemTanimAd;
+                    serhData.tesisIslemBaslamaSekilAd = tesisIslem.BaslamaSekilAd;
+
+                    if (tesisIslem.Yevmiye != null)
+                    {
+                        serhData.tesisIslemTarih = tesisIslem.Yevmiye.Tarih;
+                    }
+                }
+
+                serhBeyanData.Add(serhData);
+            }
+
+            List<Field> serhBeyanFields = CreateSerhBeyanFields();
+            SerhBeyanMalikData SerhBeyanData = new SerhBeyanMalikData()
+            {
+                serhBeyan = serhBeyanData,
+                serhBeyanFields = serhBeyanFields
+            };
+
+            return SerhBeyanData;
+        }
+
+        private async Task<List<SerhBeyanData>> GetSerhBeyan(decimal tasinmazID)
+        {
+            List<SerhBeyan> serhBeyanIrtifakData = new List<SerhBeyan>();
+            List<SerhBeyanData> serhBeyanData = new List<SerhBeyanData>();
+
+            SerhBeyan[] beyan = await GetirSerhBeyan(TapuBolum.Beyan, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] irtifak = await GetirSerhBeyan(TapuBolum.Irtifak, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] kisi = await GetirSerhBeyan(TapuBolum.Kisi, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] mahalleKoy = await GetirSerhBeyan(TapuBolum.MahalleKoy, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] none = await GetirSerhBeyan(TapuBolum.None, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] organizasyon = await GetirSerhBeyan(TapuBolum.Organizasyon, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] rehin = await GetirSerhBeyan(TapuBolum.Rehin, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] rehinMiktar = await GetirSerhBeyan(TapuBolum.RehinMiktar, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] serh = await GetirSerhBeyan(TapuBolum.Serh, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] serhBeyanMalik = await GetirSerhBeyan(TapuBolum.SerhBeyanMalik, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] tuzelKisi = await GetirSerhBeyan(TapuBolum.TuzelKisi, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] ulke = await GetirSerhBeyan(TapuBolum.Ulke, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] zemin = await GetirSerhBeyan(TapuBolum.Zemin, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] zeminHisse = await GetirSerhBeyan(TapuBolum.ZeminHisse, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] zeminMuhdesat = await GetirSerhBeyan(TapuBolum.ZeminMuhdesat, tasinmazID, TapuBolumDurum.Hepsi);
+            SerhBeyan[] zeminMuhdesatMalik = await GetirSerhBeyan(TapuBolum.ZeminMuhdesatMalik, tasinmazID, TapuBolumDurum.Hepsi);
+
+            serhBeyanIrtifakData.AddRange(beyan);
+            serhBeyanIrtifakData.AddRange(irtifak);
+            serhBeyanIrtifakData.AddRange(kisi);
+            serhBeyanIrtifakData.AddRange(mahalleKoy);
+            serhBeyanIrtifakData.AddRange(none);
+            serhBeyanIrtifakData.AddRange(organizasyon);
+            serhBeyanIrtifakData.AddRange(rehin);
+            serhBeyanIrtifakData.AddRange(rehinMiktar);
+            serhBeyanIrtifakData.AddRange(serh);
+            serhBeyanIrtifakData.AddRange(serhBeyanMalik);
+            serhBeyanIrtifakData.AddRange(tuzelKisi);
+            serhBeyanIrtifakData.AddRange(ulke);
+            serhBeyanIrtifakData.AddRange(zemin);
+            serhBeyanIrtifakData.AddRange(zeminHisse);
+            serhBeyanIrtifakData.AddRange(zeminMuhdesat);
+            serhBeyanIrtifakData.AddRange(zeminMuhdesatMalik);
+
+            for (int i = 0; i < serhBeyanIrtifakData.Count; i++)
+            {
+                SerhBeyan serhBeyan = serhBeyanIrtifakData[i];
+                string serhBeyanSablon = serhBeyan.Sablon;
+                string serhBeyanCumle = serhBeyan.Cumle;
+                string sablon = "";
+                string cumle = "";
+
+                if (serhBeyanSablon != null)
+                {
+                    sablon = serhBeyanSablon.Length > 256 ? serhBeyanSablon.Substring(0, 255) : serhBeyanSablon;
+                }
+                if (serhBeyanCumle != null)
+                {
+                    cumle = serhBeyanCumle.Length > 256 ? serhBeyanCumle.Substring(0, 255) : serhBeyanCumle;
+                }
+
+                SerhBeyanData serhData = new SerhBeyanData()
+                {
+                    tasinmazID = serhBeyan.TapuBolumReferans,
+                    serhBeyanTip = serhBeyan.Tip,
+                    sablon = sablon,
+                    serhBeyanDurum = serhBeyan.TapuBolumDurum,
+                    cumle = cumle,
+                    baslamaTarih = serhBeyan.BaslamaTarih,
+                    bitisTarih = serhBeyan.BitisTarih,
+                    sure = serhBeyan.Sure,
+                    tapuBolum = serhBeyan.TapuBolum
+                };
+
+                if (serhBeyan.TerkinIslem != null)
+                {
+                    Islem terkinIslem = serhBeyan.TerkinIslem;
+
+                    serhData.terkinIslemTanimAd = terkinIslem.IslemTanimAd;
+                    serhData.terkinIslemBaslamaSekilAd = terkinIslem.BaslamaSekilAd;
+
+                    if (terkinIslem.Yevmiye != null)
+                    {
+                        serhData.terkinIslemTarih = terkinIslem.Yevmiye.Tarih;
+                    }
+                }
+
+                if (serhBeyan.TesisIslem != null)
+                {
+                    Islem tesisIslem = serhBeyan.TesisIslem;
+
+                    serhData.tesisIslemTanimAd = tesisIslem.IslemTanimAd;
+                    serhData.tesisIslemBaslamaSekilAd = tesisIslem.BaslamaSekilAd;
+
+                    if (tesisIslem.Yevmiye != null)
+                    {
+                        serhData.tesisIslemTarih = tesisIslem.Yevmiye.Tarih;
+                    }
+                }
+
+                serhBeyanData.Add(serhData);
+            }
+            return serhBeyanData;
+        }
+
+        private async Task<List<MEGSISParselDTO>> GetParcelInfo(string cql_filter)
+        {
+            HttpClient httpClient = new HttpClient();
+            httpClient = GetMEGSISHeader(httpClient);
+            var megsisURL = _configuration.GetSection("MEGSISConnection:MEGSISURL").Value;
+            var formUrlEncodedContent = new FormUrlEncodedContent(new[]
+            {
+                new KeyValuePair<string, string>("request","GetFeature"),
+                new KeyValuePair<string, string>("service", "WFS"),
+                new KeyValuePair<string, string>("typeName", "TKGM:parseller"),
+                new KeyValuePair<string, string>("srsName", "EPSG:4326"),
+                new KeyValuePair<string, string>("propertyName","tapuzeminref,tip,tapukimlikno,kadastroalan,geom"),
+                new KeyValuePair<string, string>("cql_filter",cql_filter),
+            });
+            var response = await httpClient.PostAsync(megsisURL, formUrlEncodedContent);
+            var content = await response.Content.ReadAsStringAsync();
+            XmlDocument doc = new XmlDocument();
+            doc.LoadXml(content);
+            XmlNode collection = doc.LastChild;
+            collection.RemoveChild(collection.FirstChild);
+
+            List<MEGSISParselDTO> parcelList = new List<MEGSISParselDTO>();
+            foreach (XmlNode feature in collection.ChildNodes)
+            {
+                XmlNode featureAttributes = feature.FirstChild;
+                if (featureAttributes["TKGM:geom"] != null)
+                {
+                    MEGSISParselDTO parcel = new MEGSISParselDTO()
+                    {
+                        tip = featureAttributes["TKGM:tip"] != null ? int.Parse(featureAttributes["TKGM:tip"].InnerText) : 0,
+                        tasinmazID = featureAttributes["TKGM:tapuzeminref"] != null ? int.Parse(featureAttributes["TKGM:tapuzeminref"].InnerText) : 0,
+                        tapuKimlikNO = featureAttributes["TKGM:tapukimlikno"] != null ? long.Parse(featureAttributes["TKGM:tapukimlikno"].InnerText) : 0,
+                        kadastroAlan = featureAttributes["TKGM:kadastroalan"] != null ? Double.Parse(featureAttributes["TKGM:kadastroalan"].InnerText) : 0,
+                        geom = featureAttributes["TKGM:geom"] != null ? convertPolygonData(featureAttributes["TKGM:geom"]) : null,
+                    };
+                    parcelList.Add(parcel);
+                }
+
+            }
+            return parcelList;
+        }
+
+        private List<List<double[]>> convertPolygonData(XmlNode geomNode)
+        {
+            List<List<double[]>> ringList = new List<List<double[]>>();
+            XmlNodeList childNodes = geomNode.ChildNodes;
+            var firstChild = childNodes.Item(0);
+            var rings = firstChild.ChildNodes;
+            for (int i = 0; i < rings.Count; i++)
+            {
+                var outerBoundary = rings[i].ChildNodes[0]["gml:outerBoundaryIs"];
+                if (outerBoundary == null)
+                {
+                    var linearRings = rings[i]["gml:LinearRing"];
+
+                    var ring = createRings(linearRings.InnerText);
+                    ringList.Add(ring);
+                }
+                else
+                {
+                    var ring = createRings(outerBoundary.InnerText);
+                    ringList.Add(ring);
+                }
+            }
+            return ringList;
+        }
+
+        private List<double[]> createRings(string geom)
+        {
+            string[] rings = geom.Split(" ");
+            List<double[]> data = new List<double[]>();
+            foreach (string ring in rings)
+            {
+                ring.Trim();
+                string[] coord = ring.Split(',');
+
+                data.Add(new double[] { Convert.ToDouble(coord[0], CultureInfo.InvariantCulture), Convert.ToDouble(coord[1], CultureInfo.InvariantCulture) });
+            }
+            return data;
+        }
+
+        private List<double[]> convertGeom(string geom)
+        {
+            try
+            {
+                string[] rings = geom.Split(" ");
+                List<double[]> data = new List<double[]>();
+                foreach (string ring in rings)
+                {
+                    ring.Trim();
+                    string[] coord = ring.Split(',');
+                    if (coord.Length == 3)
+                    {
+                        string coordinateData = coord[1];
+                        string[] coordinateArray = coordinateData.Split('.');
+                        string firstCoordNum = coordinateArray[0];
+                        string firstCoordPrecision = coordinateArray[1].Substring(0, coordinateArray[1].Length - 2);
+                        string secondCoordNum = coordinateArray[1].Substring(coordinateArray[1].Length - 2);
+                        string secondCoordPrcision = coordinateArray[2];
+
+                        string firstCoord = firstCoordNum + "." + firstCoordPrecision;
+                        string secondCoord = secondCoordNum + "." + secondCoordPrcision;
+
+                        data.Add(new double[] { Convert.ToDouble(coord[0], CultureInfo.InvariantCulture), Convert.ToDouble(firstCoord, CultureInfo.InvariantCulture) });
+                        data.Add(new double[] { Convert.ToDouble(secondCoord, CultureInfo.InvariantCulture), Convert.ToDouble(coord[2], CultureInfo.InvariantCulture) });
+                    }
+                    else
+                    {
+                        data.Add(new double[] { Convert.ToDouble(coord[0], CultureInfo.InvariantCulture), Convert.ToDouble(coord[1], CultureInfo.InvariantCulture) });
+                    }
+                }
+                return data;
+            }
+            catch (Exception ex)
+            {
+                return null;
+            }
+
+        }
+
