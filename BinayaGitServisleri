+        [HttpGet("GetBinaNoWithMernisBinaKoduForNumarataj")]
+        public async Task<int?> GetBinaNoWithMernisBinaKoduForNumarataj(int? mernisBinaKodu, int sayac)
+        {
+            string path = @"C:/numarataj_logs.txt";
+            // kimlikNo = NumaratajNo
+            // yapiKimlikNo = BinaNo
+            // KPS Servisinde binaNo ile ifadele edilen aslında NumaratajNo dur. Bu yüzden parametre adı binaNo olarak isimlendirilmiş olsa da aslında gerçek binaNo değildir.
+
+            var giris = new ServiceMernis.KullaniciGiris();
+            giris.UserName = "mebcbs";
+            giris.Password = "9UFdAy7x";
+
+            var logClient = new ServiceMernis.LogClient();
+            logClient.EkranKodu = "1";
+            logClient.Aciklama = "MebCbsAnka";
+            logClient.ModulKodu = "1";
+
+            logClient.YetkiliIPAdresi = "10.30.1.244";
+            logClient.YetkiliKullaniciAdi = "1234567910";
+            logClient.YetkiliKullaniciNo = 1234567910;
+            logClient.YetkiliTCKimlikNo = 44257273824;
+
+            var kriter = new ServiceMernis.AksOkuMahalleSorguKriter();
+
+            kriter.Kod = int.Parse(mernisBinaKodu.ToString());
+
+            var client = new ServiceMernis.AksOkuBinaServisClient();
+            ServiceMernis.BinaSonucu response = await client.BinaBilgiGetirAsync(kriter, giris, logClient);
+
+            if (response != null)
+            {
+                var sorguSonucu = response.SorguSonucu[0];
+                var ada = sorguSonucu.Ada;
+                var parsel = sorguSonucu.Parsel;
+                var numaratajTipi = sorguSonucu.BinaNumaratajTipi;
+                var binaYapiTipi = sorguSonucu.BinaYapiTipi;
+                var esbinaKimlikNo = sorguSonucu.EsBinaKimlikNo;
+                var esBinaKodu = sorguSonucu.EsBinaKodu;
+                var csbmKodu = sorguSonucu.CsbmKodu;
+                var disKapiNo = sorguSonucu.DisKapiNo;
+
+                sayac++;
+
+                Console.WriteLine("____________________________________________");
+                Console.WriteLine("{0}. Islem Bilgileri: ", sayac);
+
+                if (binaYapiTipi == null)
+                {
+                    Console.WriteLine("Bina Yapi Tipi: null ");
+                }
+                else
+                {
+                    Console.WriteLine("Bina Yapi Tipi: {0} ", binaYapiTipi.Aciklama);
+                }
+                if (numaratajTipi == null)
+                {
+                    Console.WriteLine("Bina Numarataj Tipi: null");
+                }
+                else
+                {
+                    Console.WriteLine("Bina Numarataj Tipi: {0} ", numaratajTipi.Aciklama);
+                }
+
+                Console.WriteLine("Ada: {0}/ Parsel: {1}", ada, parsel);
+                Console.WriteLine("EsBina KimlikNo: {0}/ EsBina Kodu: {1}", esbinaKimlikNo, esBinaKodu);
+
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+                    sw.WriteLine("____________________________________________");
+                    sw.WriteLine("{0}. Islem Bilgileri: ", sayac);
+
+                    if (binaYapiTipi == null)
+                    {
+                        sw.WriteLine("Bina Yapi Tipi: null ");
+                    }
+                    else
+                    {
+                        sw.WriteLine("Bina Yapi Tipi: {0} ", binaYapiTipi.Aciklama);
+                    }
+                    if (numaratajTipi == null)
+                    {
+                        sw.WriteLine("Bina Numarataj Tipi: null");
+                    }
+                    else
+                    {
+                        sw.WriteLine("Bina Numarataj Tipi: {0} ", numaratajTipi.Aciklama);
+                    }
+
+                    sw.WriteLine("Ada: {0}/ Parsel: {1}", ada, parsel);
+                    sw.WriteLine("EsBina KimlikNo: {0}/ EsBina Kodu: {1}", esbinaKimlikNo, esBinaKodu);
+
+                }
+
+                if (response != null)
+                {
+                    int? binaNo = response.SorguSonucu[0].BinaNo;
+
+                    return binaNo;
+                }
+                else
+                {
+                    return 0;
+                }
+            }
+            else
+            {
+                return 1;
+            }
+
+        }
+
+        [HttpGet("getPilotBinaGeometriesFromNumarataj")]
+        public async Task<IActionResult> getPilotBinaGeometriesFromNumarataj()
+        {
+            string path = @"C:/numarataj_logs.txt";
+
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            var mebIEDBServiceUrl = _configuration.GetSection("ArcgisRefererURL:MebIEDBArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            HttpClient httpClient = new HttpClient();
+
+            httpClient.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+            var pilotBinaQueryURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/2/query";
+            var maksYapiAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/1/addFeatures";
+            int offsetMultiplier = 0;
+            int featureCount = 1;
+            int sayac = 0;
+            int basariliKayitSayisi = 0;
+
+            try
+            {
+                while (featureCount != 0)
+                {
+                    var resultOffset = offsetMultiplier * 1000;
+
+                    var formUrlEncodedContent = new FormUrlEncodedContent(new[] {
+                        new KeyValuePair<string,string>("where", "1=1"),
+                        new KeyValuePair<string, string>("geometryType", "esriGeometryEnvelope"),
+                        new KeyValuePair<string, string>("spatialRel", "esriSpatialRelIntersects"),
+                        new KeyValuePair<string, string>("outFields", "objectid,il_kodu,il_adi,ilce_kodu,mernis_ilc,ilce_adi,tur_kodu,tur_adi,kurum_kodu,kurum_adi,mernis_bin,kayit_bolg"),
+                        new KeyValuePair<string, string>("orderByFields", "objectid asc"),
+                        new KeyValuePair<string, string>("returnGeometry", "true"),
+                        new KeyValuePair<string, string>("f", "pjson"),
+                        new KeyValuePair<string, string>("resultOffset", resultOffset.ToString()),
+                        new KeyValuePair<string, string>("token", arcgisTokenDto.token)
+                    });
+
+                    var response = await httpClient.PostAsync(pilotBinaQueryURI, formUrlEncodedContent);
+                    var content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+
+                    if (arcgisResponseData != null)
+                    {
+                        var features = arcgisResponseData["features"].ToArray();
+
+                        for (var i = 0; i < features.Length; i++)
+                        {
+                            int? mernisBinaKodu = features[i]["attributes"]["mernis_bin"].ToObject<int>();
+
+                            if (mernisBinaKodu != null)
+                            {
+                                int? kimlikNo = await GetBinaNoWithMernisBinaKoduForNumarataj(mernisBinaKodu, sayac); // Buradan gelen binaNo verisi aslında => KimlikNo = NumaratajNo
+
+                                if (kimlikNo == 0)
+                                {
+                                    Console.WriteLine("Bina Kodu: {0} => Maks:Yapı Response Null. ", mernisBinaKodu);
+
+                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                    {
+                                        sw.WriteLine("Bina Kodu: {0} => Maks:Yapı Response Null. ", mernisBinaKodu);
+                                    }
+                                    sayac++;
+                                    continue;
+                                }
+                                else if (kimlikNo == 1)
+                                {
+                                    Console.WriteLine("Bina Kodu: {0} => BinaBilgiGetir Servisinden Sonuç Dönmedi. ", mernisBinaKodu);
+
+                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                    {
+                                        sw.WriteLine("Bina Kodu: {0} => BinaBilgiGetir Servisinden Sonuç Dönmedi ", mernisBinaKodu);
+                                    }
+                                    sayac++;
+                                    continue;
+                                }
+                                else if (kimlikNo == null)
+                                {
+                                    Console.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin  Kimlik Nosu Null!. ", mernisBinaKodu, kimlikNo);
+
+                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                    {
+                                        sw.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin Kimlik Nosu Null!. ", mernisBinaKodu, kimlikNo);
+                                    }
+                                    sayac++;
+                                    continue;
+                                }
+                                else
+                                {
+                                    string yapiKimlikNoString = await getMaksNumaratajInfoWithKimlikNo(kimlikNo);
+
+                                    if (yapiKimlikNoString == "" || yapiKimlikNoString == null)
+                                    {
+                                        Console.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin  Yapı Kimlik Nosu Yok!. ", mernisBinaKodu, kimlikNo);
+
+                                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                                        {
+                                            sw.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin  Yapı Kimlik Nosu Yok!. ", mernisBinaKodu, kimlikNo);
+                                        }
+                                        sayac++;
+                                        continue;
+                                    }
+                                    else
+                                    {
+                                        int? yapiKimlikNo = int.Parse(yapiKimlikNoString);
+
+                                        XmlDocument maksYapiInfo;
+
+                                        maksYapiInfo = await getMaksYapiInfoWithBinaNo(yapiKimlikNo);
+
+                                        if (maksYapiInfo != null)
+                                        {
+                                            XmlNode lastChildOfCollection = maksYapiInfo.LastChild; // featureCollection
+
+                                            if (lastChildOfCollection.ChildNodes.Count == 0)
+                                            {
+                                                Console.WriteLine("Maks Tarafinda Yapı Kimlik No: {0} / Bina Kodu: {1} / Kimlik No: {2} Olan Verilerin Karsiliği Bulunamadi. ", yapiKimlikNo, mernisBinaKodu, kimlikNo);
+                                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                {
+                                                    sw.WriteLine("Maks Tarafinda Yapı Kimlik No: {0} / Bina Kodu: {1} / Kimlik No: {2} Olan Verilerin Karsiliği Bulunamadi. ", yapiKimlikNo, mernisBinaKodu, kimlikNo);
+                                                }
+                                                sayac++;
+                                                continue;
+                                            }
+                                            foreach (XmlNode feature in lastChildOfCollection.ChildNodes) //featureMembers
+                                            {
+                                                XmlNode featureAttributes = feature.FirstChild;
+                                                List<MaksYapi> maksYapiFeaturesArray = new List<MaksYapi>();
+
+                                                var maksGeom = featureAttributes["maks:geometri"] != null ? featureAttributes["maks:geometri"].InnerText : "";
+
+                                                if (maksGeom != "")
+                                                {
+                                                    var geomForNoComma = createRingsForNoComma(maksGeom);
+
+                                                    MaksYapi maksYapiFeatures = new MaksYapi()
+                                                    {
+                                                        attributes = new maksAttributes()
+                                                        {
+                                                            tip = featureAttributes["maks:tip"] != null ? featureAttributes["maks:tip"].InnerText : "",
+                                                            durum = featureAttributes["maks:durum"] != null ? featureAttributes["maks:durum"].InnerText : "",
+                                                            olcek = featureAttributes["maks:olcek"] != null ? int.Parse(featureAttributes["maks:olcek"].InnerText) : 0,
+                                                            olusumyontemi = featureAttributes["maks:olusumYontemi"] != null ? featureAttributes["maks:olusumYontemi"].InnerText : "",
+                                                            zemin_alti_kat_sayisi = featureAttributes["maks:zeminAltiKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminAltiKatSayisi"].InnerText) : 0,
+                                                            zemin_ustu_kat_sayisi = featureAttributes["maks:zeminUstuKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminUstuKatSayisi"].InnerText) : 0,
+                                                            kimlikno = featureAttributes["maks:kimlikNo"] != null ? long.Parse(featureAttributes["maks:kimlikNo"].InnerText) : 0,
+                                                            ad = featureAttributes["maks:ad"] != null ? featureAttributes["maks:ad"].InnerText : "",
+                                                            id = mernisBinaKodu
+                                                        },
+                                                        geometry = new maksGeometry()
+                                                        {
+                                                            rings = geomForNoComma
+                                                        }
+                                                    };
+
+                                                    maksYapiFeaturesArray.Add(maksYapiFeatures);
+                                                    if (geomForNoComma != null)
+                                                    {
+                                                        string stringFeatureObject = JsonConvert.SerializeObject(maksYapiFeaturesArray);
+                                                        var maksYapiFormUrlEncodedContent = new FormUrlEncodedContent(new[]
+                                                        {
+                                                    new KeyValuePair<string, string>("f","json"),
+                                                    new KeyValuePair<string, string>("features", stringFeatureObject),
+                                                    new KeyValuePair<string, string>("rollbackOnFailure", "true")
+
+                                                 });
+
+
+                                                        var maksYapiResponse = await httpClient.PostAsync(maksYapiAddFeaturesURI, maksYapiFormUrlEncodedContent);
+                                                        var maksYapiContent = await maksYapiResponse.Content.ReadAsStringAsync();
+                                                        var addFeatureResponse = JsonConvert.DeserializeObject<MaksYapiAddFeatureResponse>(maksYapiContent);
+                                                        var addFeatureResponseArray = addFeatureResponse.addResults;
+                                                        if (addFeatureResponseArray[0].success == true)
+                                                        {
+                                                            sayac++;
+                                                            basariliKayitSayisi++;
+
+                                                            using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                            {
+                                                                sw.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                                                sw.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                                            }
+
+                                                            Console.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                                            Console.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+
+                                                        }
+                                                        else
+                                                        {
+                                                            sayac++;
+                                                            string errorCode = addFeatureResponseArray[0].error.code;
+
+                                                            string errorDescription = addFeatureResponseArray[0].error.description;
+
+                                                            using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                            {
+                                                                sw.WriteLine("HATA!!!!");
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("ObjectId: {0} ", addFeatureResponseArray[0].objectId);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("MernisBinaKodu: {0} ", maksYapiFeatures.attributes.id);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("Bina No: {0} ", maksYapiFeatures.attributes.kimlikno);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("MaksYapiFeatures: {0}", maksYapiFeatures);
+                                                            }
+
+                                                            Console.WriteLine("HATA!!!!");
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("ObjectId:{0} ", addFeatureResponseArray[0].objectId);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("MernisBinaKodu:{0} ", maksYapiFeatures.attributes.id);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("Ad:{0} ", maksYapiFeatures.attributes.ad);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("Bina No:{0} ", maksYapiFeatures.attributes.kimlikno);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("MaksYapiFeatures: {0}", maksYapiFeatures);
+
+                                                            errorDescription = Console.ReadLine();
+
+                                                        }
+                                                    }
+                                                }
+                                                else
+                                                {
+                                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                    {
+                                                        sayac++;
+                                                        sw.WriteLine("Sayac: {0}", sayac);
+                                                        sw.WriteLine("Maks Geometrisi Boş");
+                                                        sw.WriteLine("Geometri: {0} ", maksGeom);
+                                                    }
+                                                    Console.WriteLine("Sayac{0}", sayac);
+                                                    Console.WriteLine("Maks Geometrisi Boş");
+                                                    Console.WriteLine("Geometri: {0} ", maksGeom);
+                                                    continue;
+                                                }
+
+                                            }
+
+                                        }
+                                        else
+                                        {
+                                            sayac++;
+                                            Console.WriteLine("Bina Kodu: {0} / Yapı Kimlik No: {1} => Maks Verisi Null", mernisBinaKodu, yapiKimlikNo);
+                                            Console.WriteLine();
+                                            continue;
+                                        }
+                                    }
+                                }
+                            }
+                            else
+                            {
+                                sayac++;
+                                Console.WriteLine("FeatureAttributes: {0} => Bina Kodu Bulunamadı", features[i]["attributes"]);
+                                Console.WriteLine();
+                                continue;
+                            }
+
+                        }
+
+                        featureCount = features.Count();
+                    }
+
+                    else
+                    {
+                        Console.WriteLine("Arcgis Datası Null");
+                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                        {
+                            sayac++;
+                            sw.WriteLine("Arcgis Datası Null");
+                        }
+                    }
+
+                    offsetMultiplier++;
+                }
+            }
+
+            catch (Exception ex)
+            {
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+                    sayac++;
+                    sw.WriteLine("Sayac: {0}", sayac);
+                    sw.WriteLine("HATA: {0}", ex);
+                }
+                Console.WriteLine("Sayac{0}", sayac);
+                Console.WriteLine("HATA: {0}", ex);
+            }
+
+            using (StreamWriter sw = System.IO.File.AppendText(path))
+            {
+                sw.WriteLine("Tarama Bitti.");
+            }
+            Console.WriteLine("Tarama Bitti");
+
+            return Ok();
+        }
+
+        [HttpGet("getPilotBinaGeometriesFromNumaratajForDigerYapi")]
+        public async Task<IActionResult> getPilotBinaGeometriesFromNumaratajForDigerYapi()
+        {
+            string path = @"C:/numarataj_logs.txt";
+
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            var mebIEDBServiceUrl = _configuration.GetSection("ArcgisRefererURL:MebIEDBArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            HttpClient httpClient = new HttpClient();
+
+            httpClient.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+            var pilotBinaQueryURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/2/query";
+            var maksYapiAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/1/addFeatures";
+            int offsetMultiplier = 0;
+            int featureCount = 1;
+            int sayac = 0;
+            int basariliKayitSayisi = 0;
+
+            try
+            {
+                while (featureCount != 0)
+                {
+                    var resultOffset = offsetMultiplier * 1000;
+
+                    var formUrlEncodedContent = new FormUrlEncodedContent(new[] {
+                        new KeyValuePair<string,string>("where", "1=1"),
+                        new KeyValuePair<string, string>("geometryType", "esriGeometryEnvelope"),
+                        new KeyValuePair<string, string>("spatialRel", "esriSpatialRelIntersects"),
+                        new KeyValuePair<string, string>("outFields", "objectid,il_kodu,il_adi,ilce_kodu,mernis_ilc,ilce_adi,tur_kodu,tur_adi,kurum_kodu,kurum_adi,mernis_bin,kayit_bolg"),
+                        new KeyValuePair<string, string>("orderByFields", "objectid asc"),
+                        new KeyValuePair<string, string>("returnGeometry", "true"),
+                        new KeyValuePair<string, string>("f", "pjson"),
+                        new KeyValuePair<string, string>("resultOffset", resultOffset.ToString()),
+                        new KeyValuePair<string, string>("token", arcgisTokenDto.token)
+                    });
+
+                    var response = await httpClient.PostAsync(pilotBinaQueryURI, formUrlEncodedContent);
+                    var content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+
+                    if (arcgisResponseData != null)
+                    {
+                        var features = arcgisResponseData["features"].ToArray();
+
+                        for (var i = 0; i < features.Length; i++)
+                        {
+                            int? mernisBinaKodu = features[i]["attributes"]["mernis_bin"].ToObject<int>();
+
+                            if (mernisBinaKodu != null)
+                            {
+                                int? kimlikNo = await GetBinaNoWithMernisBinaKoduForNumarataj(mernisBinaKodu, sayac); // Buradan gelen binaNo verisi aslında => KimlikNo = NumaratajNo
+
+                                if (kimlikNo == 0)
+                                {
+                                    Console.WriteLine("Bina Kodu: {0} => Maks:Yapı Response Null. ", mernisBinaKodu);
+
+                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                    {
+                                        sw.WriteLine("Bina Kodu: {0} => Maks:Yapı Response Null. ", mernisBinaKodu);
+                                    }
+                                    sayac++;
+                                    continue;
+                                }
+                                else if (kimlikNo == 1)
+                                {
+                                    Console.WriteLine("Bina Kodu: {0} => BinaBilgiGetir Servisinden Sonuç Dönmedi. ", mernisBinaKodu);
+
+                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                    {
+                                        sw.WriteLine("Bina Kodu: {0} => BinaBilgiGetir Servisinden Sonuç Dönmedi ", mernisBinaKodu);
+                                    }
+                                    sayac++;
+                                    continue;
+                                }
+                                else if (kimlikNo == null)
+                                {
+                                    Console.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin  Kimlik Nosu Null!. ", mernisBinaKodu, kimlikNo);
+
+                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                    {
+                                        sw.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin Kimlik Nosu Null!. ", mernisBinaKodu, kimlikNo);
+                                    }
+                                    sayac++;
+                                    continue;
+                                }
+                                else
+                                {
+                                    string yapiKimlikNoString = await getMaksNumaratajInfoWithKimlikNoForDigerYapi(kimlikNo);
+
+                                    if (yapiKimlikNoString == "" || yapiKimlikNoString == null)
+                                    {
+                                        Console.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin  Yapı Kimlik Nosu Yok!. ", mernisBinaKodu, kimlikNo);
+
+                                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                                        {
+                                            sw.WriteLine("Bina Kodu: {0} / Kimlik No: {1} Olan Verilerin  Yapı Kimlik Nosu Yok!. ", mernisBinaKodu, kimlikNo);
+                                        }
+                                        sayac++;
+                                        continue;
+                                    }
+                                    else
+                                    {
+                                        int? yapiKimlikNo = int.Parse(yapiKimlikNoString);
+
+                                        XmlDocument maksDigerYapiInfo;
+
+                                        maksDigerYapiInfo = await getMaksDigerYapiInfoWithDigerYapiKimlikNo(yapiKimlikNo);
+
+                                        if (maksDigerYapiInfo != null)
+                                        {
+                                            XmlNode lastChildOfCollection = maksDigerYapiInfo.LastChild; // featureCollection
+
+                                            if (lastChildOfCollection.ChildNodes.Count == 0)
+                                            {
+                                                Console.WriteLine("Maks Tarafinda Yapı Kimlik No: {0} / Bina Kodu: {1} / Kimlik No: {2} Olan Verilerin Karsiliği Bulunamadi. ", yapiKimlikNo, mernisBinaKodu, kimlikNo);
+                                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                {
+                                                    sw.WriteLine("Maks Tarafinda Yapı Kimlik No: {0} / Bina Kodu: {1} / Kimlik No: {2} Olan Verilerin Karsiliği Bulunamadi. ", yapiKimlikNo, mernisBinaKodu, kimlikNo);
+                                                }
+                                                sayac++;
+                                                continue;
+                                            }
+                                            foreach (XmlNode feature in lastChildOfCollection.ChildNodes) //featureMembers
+                                            {
+                                                XmlNode featureAttributes = feature.FirstChild;
+                                                List<MaksYapi> maksYapiFeaturesArray = new List<MaksYapi>();
+
+                                                var maksGeom = featureAttributes["maks:geometri"] != null ? featureAttributes["maks:geometri"].InnerText : "";
+
+                                                if (maksGeom != "")
+                                                {
+                                                    var geomForNoComma = createRingsForNoComma(maksGeom);
+
+                                                    MaksYapi maksYapiFeatures = new MaksYapi()
+                                                    {
+                                                        attributes = new maksAttributes()
+                                                        {
+                                                            tip = featureAttributes["maks:tip"] != null ? featureAttributes["maks:tip"].InnerText : "",
+                                                            durum = featureAttributes["maks:durum"] != null ? featureAttributes["maks:durum"].InnerText : "",
+                                                            olcek = featureAttributes["maks:olcek"] != null ? int.Parse(featureAttributes["maks:olcek"].InnerText) : 0,
+                                                            olusumyontemi = featureAttributes["maks:olusumYontemi"] != null ? featureAttributes["maks:olusumYontemi"].InnerText : "",
+                                                            zemin_alti_kat_sayisi = featureAttributes["maks:zeminAltiKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminAltiKatSayisi"].InnerText) : 0,
+                                                            zemin_ustu_kat_sayisi = featureAttributes["maks:zeminUstuKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminUstuKatSayisi"].InnerText) : 0,
+                                                            kimlikno = featureAttributes["maks:kimlikNo"] != null ? long.Parse(featureAttributes["maks:kimlikNo"].InnerText) : 0,
+                                                            ad = featureAttributes["maks:ad"] != null ? featureAttributes["maks:ad"].InnerText : "",
+                                                            id = mernisBinaKodu
+                                                        },
+                                                        geometry = new maksGeometry()
+                                                        {
+                                                            rings = geomForNoComma
+                                                        }
+                                                    };
+
+                                                    maksYapiFeaturesArray.Add(maksYapiFeatures);
+                                                    if (geomForNoComma != null)
+                                                    {
+                                                        string stringFeatureObject = JsonConvert.SerializeObject(maksYapiFeaturesArray);
+                                                        var maksYapiFormUrlEncodedContent = new FormUrlEncodedContent(new[]
+                                                        {
+                                                    new KeyValuePair<string, string>("f","json"),
+                                                    new KeyValuePair<string, string>("features", stringFeatureObject),
+                                                    new KeyValuePair<string, string>("rollbackOnFailure", "true")
+
+                                                 });
+
+
+                                                        var maksYapiResponse = await httpClient.PostAsync(maksYapiAddFeaturesURI, maksYapiFormUrlEncodedContent);
+                                                        var maksYapiContent = await maksYapiResponse.Content.ReadAsStringAsync();
+                                                        var addFeatureResponse = JsonConvert.DeserializeObject<MaksYapiAddFeatureResponse>(maksYapiContent);
+                                                        var addFeatureResponseArray = addFeatureResponse.addResults;
+                                                        if (addFeatureResponseArray[0].success == true)
+                                                        {
+                                                            sayac++;
+                                                            basariliKayitSayisi++;
+
+                                                            using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                            {
+                                                                sw.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                                                sw.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                                            }
+
+                                                            Console.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                                            Console.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+
+                                                        }
+                                                        else
+                                                        {
+                                                            sayac++;
+                                                            string errorCode = addFeatureResponseArray[0].error.code;
+
+                                                            string errorDescription = addFeatureResponseArray[0].error.description;
+
+                                                            using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                            {
+                                                                sw.WriteLine("HATA!!!!");
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("ObjectId: {0} ", addFeatureResponseArray[0].objectId);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("MernisBinaKodu: {0} ", maksYapiFeatures.attributes.id);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("Bina No: {0} ", maksYapiFeatures.attributes.kimlikno);
+                                                                sw.WriteLine();
+                                                                sw.WriteLine("MaksYapiFeatures: {0}", maksYapiFeatures);
+                                                            }
+
+                                                            Console.WriteLine("HATA!!!!");
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("ObjectId:{0} ", addFeatureResponseArray[0].objectId);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("MernisBinaKodu:{0} ", maksYapiFeatures.attributes.id);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("Ad:{0} ", maksYapiFeatures.attributes.ad);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("Bina No:{0} ", maksYapiFeatures.attributes.kimlikno);
+                                                            Console.WriteLine();
+                                                            Console.WriteLine("MaksYapiFeatures: {0}", maksYapiFeatures);
+
+                                                            errorDescription = Console.ReadLine();
+
+                                                        }
+                                                    }
+                                                }
+                                                else
+                                                {
+                                                    using (StreamWriter sw = System.IO.File.AppendText(path))
+                                                    {
+                                                        sayac++;
+                                                        sw.WriteLine("Sayac: {0}", sayac);
+                                                        sw.WriteLine("Maks Geometrisi Boş");
+                                                        sw.WriteLine("Geometri: {0} ", maksGeom);
+                                                    }
+                                                    Console.WriteLine("Sayac{0}", sayac);
+                                                    Console.WriteLine("Maks Geometrisi Boş");
+                                                    Console.WriteLine("Geometri: {0} ", maksGeom);
+                                                    continue;
+                                                }
+
+                                            }
+
+                                        }
+                                        else
+                                        {
+                                            sayac++;
+                                            Console.WriteLine("Bina Kodu: {0} / Yapı Kimlik No: {1} => Maks Verisi Null", mernisBinaKodu, yapiKimlikNo);
+                                            Console.WriteLine();
+                                            continue;
+                                        }
+                                    }
+                                }
+                            }
+                            else
+                            {
+                                sayac++;
+                                Console.WriteLine("FeatureAttributes: {0} => Bina Kodu Bulunamadı", features[i]["attributes"]);
+                                Console.WriteLine();
+                                continue;
+                            }
+
+                        }
+
+                        featureCount = features.Count();
+                    }
+
+                    else
+                    {
+                        Console.WriteLine("Arcgis Datası Null");
+                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                        {
+                            sayac++;
+                            sw.WriteLine("Arcgis Datası Null");
+                        }
+                    }
+
+                    offsetMultiplier++;
+                }
+            }
+
+            catch (Exception ex)
+            {
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+                    sayac++;
+                    sw.WriteLine("Sayac: {0}", sayac);
+                    sw.WriteLine("HATA: {0}", ex);
+                }
+                Console.WriteLine("Sayac{0}", sayac);
+                Console.WriteLine("HATA: {0}", ex);
+            }
+
+            using (StreamWriter sw = System.IO.File.AppendText(path))
+            {
+                sw.WriteLine("Tarama Bitti.");
+            }
+            Console.WriteLine("Tarama Bitti");
+
+            return Ok();
+        }
+
+        public async Task<XmlDocument> getMaksYapiInfoWithBinaNo(int? binaNo)
+        {
+            var url = "http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Yapi&username=mebcbs&password=9UFdAy7x&FILTER=%3CFilter%20xmlns=%22http://www.opengis.net/ogc%22%20xmlns:gml=%22http://www.opengis.net/gml%22%3E%3CPropertyIsEqualTo%3E%3CPropertyName%3EkimlikNo%3C/PropertyName%3E%3CLiteral%3E" + binaNo + "%3C/Literal%3E%3C/PropertyIsEqualTo%3E%3C/Filter%3E";
+            // http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Yapi&username=mebcbs&password=9UFdAy7x&FILTER=<Filter xmlns="http://www.opengis.net/ogc" xmlns:gml="http://www.opengis.net/gml"><PropertyIsEqualTo><PropertyName>kimlikNo</PropertyName><Literal>140561879</Literal></PropertyIsEqualTo></Filter>
+
+            var httpClient = new HttpClient();
+
+            // System.Threading.Thread.Sleep(1000);
+
+            var response = await httpClient.GetAsync(url);
+
+            var content = await response.Content.ReadAsStringAsync();
+
+            XmlDocument doc = new XmlDocument();
+
+            doc.LoadXml(content);
+
+            return doc;
+        }
+
+        public async Task<XmlDocument> getMaksDigerYapiInfoWithDigerYapiKimlikNo(int? yapiKimlikNo)
+        {
+            var url = "http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:DigerYapi&username=mebcbs&password=9UFdAy7x&FILTER=%3CFilter%20xmlns=%22http://www.opengis.net/ogc%22%20xmlns:gml=%22http://www.opengis.net/gml%22%3E%3CPropertyIsEqualTo%3E%3CPropertyName%3EkimlikNo%3C/PropertyName%3E%3CLiteral%3E" + yapiKimlikNo + "%3C/Literal%3E%3C/PropertyIsEqualTo%3E%3C/Filter%3E";
+            // http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Yapi&username=mebcbs&password=9UFdAy7x&FILTER=<Filter xmlns="http://www.opengis.net/ogc" xmlns:gml="http://www.opengis.net/gml"><PropertyIsEqualTo><PropertyName>kimlikNo</PropertyName><Literal>140561879</Literal></PropertyIsEqualTo></Filter>
+
+            var httpClient = new HttpClient();
+
+            // System.Threading.Thread.Sleep(1000);
+
+            var response = await httpClient.GetAsync(url);
+
+            var content = await response.Content.ReadAsStringAsync();
+
+            XmlDocument doc = new XmlDocument();
+
+            doc.LoadXml(content);
+
+            return doc;
+        }
