+        [HttpGet("getMaksNumaratajInfoWithKimlikNoForDigerYapi")]
+        public async Task<XmlNode> getMaksNumaratajInfoWithKimlikNoForDigerYapiAlanKampus(int? kimlikNo)
+        {
+            // numaratajNo = kimlikNo
+            // binaNo = yapiKimlikNo
+
+            var url = "http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Numarataj&username=mebcbs&password=9UFdAy7x&FILTER=%3CFilter%20xmlns=%22http://www.opengis.net/ogc%22%20xmlns:gml=%22http://www.opengis.net/gml%22%3E%3CPropertyIsEqualTo%3E%3CPropertyName%3EkimlikNo%3C/PropertyName%3E%3CLiteral%3E" + kimlikNo + "%3C/Literal%3E%3C/PropertyIsEqualTo%3E%3C/Filter%3E";
+            // http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Yapi&username=mebcbs&password=9UFdAy7x&FILTER=<Filter xmlns="http://www.opengis.net/ogc" xmlns:gml="http://www.opengis.net/gml"><PropertyIsEqualTo><PropertyName>kimlikNo</PropertyName><Literal>140561879</Literal></PropertyIsEqualTo></Filter>
+
+            var httpClient = new HttpClient();
+
+            // System.Threading.Thread.Sleep(1000);
+
+            var response = await httpClient.GetAsync(url);
+
+            var content = await response.Content.ReadAsStringAsync();
+
+            XmlDocument doc = new XmlDocument();
+
+            doc.LoadXml(content);
+
+            XmlNode lastChildOfCollection = doc.LastChild;
+            XmlNode childNodes = lastChildOfCollection.ChildNodes[0];
+
+            return childNodes;
+        }
+
+        [HttpGet("getAlanKampusGeometriesContinuation")]
+        public async Task<Sayac> getAlanKampusGeometriesContinuation(string type, XmlNode lastChildOfCollection, Sayac sayacObject, string kampusId)
+        {
+            string path = @"C:/alankampus.txt";
+
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            var mebIEDBServiceUrl = _configuration.GetSection("ArcgisRefererURL:MebIEDBArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            HttpClient httpClient = new HttpClient();
+
+            httpClient.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+            int basariliKayitSayisi = sayacObject.basariliKayitSayisi;
+            int sayac = sayacObject.sayac;
+            string yapiKimlikNoString = "";
+            var maksYapiAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/1/addFeatures";
+            var maksYapiInfoAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/3/addFeatures";
+            try
+            {
+                foreach (XmlNode feature in lastChildOfCollection.ChildNodes) //featureMembers
+                {
+                    XmlNode featureAttributes = feature.FirstChild;
+                    List<MaksYapi> maksYapiFeaturesArray = new List<MaksYapi>();
+                    List<MaksYapiInfo> maksYapiInfoFeaturesArray = new List<MaksYapiInfo>();
+                    // var geom = createRings(coordinatesForGeom);
+                    var maksGeom = featureAttributes["maks:geometri"] != null ? featureAttributes["maks:geometri"].InnerText : "";
+                    if (maksGeom != "")
+                    {
+                        var geomForNoComma = createRingsForNoComma(maksGeom);
+                        if (geomForNoComma.Count == 0) // 0 dönerse geometride hata var demektir.
+                        {
+                            var kimlikNoAttribute = featureAttributes["maks:kimlikNo"].InnerText != null ? featureAttributes["maks:kimlikNo"].InnerText : "";
+
+                            if (kimlikNoAttribute == null)
+                            {
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sayac++;
+                                    sw.WriteLine("Sayac: {0}", sayac);
+                                    sw.WriteLine("KampusId: {0} / Kimlik No: {1} Olan Verinin Feature Attributes Bulunamadi.", kampusId, kimlikNoAttribute);
+                                }
+                                Console.WriteLine("Sayac{0}", sayac);
+                                Console.WriteLine("KampusId: {0} / Kimlik No: {1} Olan Verinin Feature Attributes Bulunamadi.", kampusId, kimlikNoAttribute);
+                                continue;
+                            }
+                            else
+                            {
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sayac++;
+                                    sw.WriteLine("Sayac: {0}", sayac);
+                                    sw.WriteLine("Maks Geometrisinde Hata Var.");
+                                    sw.WriteLine("KimlikNo:{0} ", kimlikNoAttribute);
+                                    sw.WriteLine("Geometri: {0} ", maksGeom);
+                                }
+
+                                Console.WriteLine("Sayac{0}", sayac);
+                                Console.WriteLine("Maks Geometrisinde Hata Var."); ;
+                                Console.WriteLine("KimlikNo:{0} ", kimlikNoAttribute);
+                                Console.WriteLine("Geometri: {0} ", maksGeom);
+                                continue;
+                            }
+
+                        }
+
+                        MaksYapi maksYapiFeatures = new MaksYapi()
+                        {
+                            attributes = new maksAttributes()
+                            {
+                                tip = featureAttributes["maks:tip"] != null ? featureAttributes["maks:tip"].InnerText : "",
+                                durum = featureAttributes["maks:durum"] != null ? featureAttributes["maks:durum"].InnerText : "",
+                                olcek = featureAttributes["maks:olcek"] != null ? int.Parse(featureAttributes["maks:olcek"].InnerText) : 0,
+                                olusumyontemi = featureAttributes["maks:olusumYontemi"] != null ? featureAttributes["maks:olusumYontemi"].InnerText : "",
+                                zemin_alti_kat_sayisi = featureAttributes["maks:zeminAltiKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminAltiKatSayisi"].InnerText) : 0,
+                                zemin_ustu_kat_sayisi = featureAttributes["maks:zeminUstuKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminUstuKatSayisi"].InnerText) : 0,
+                                kimlikno = featureAttributes["maks:kimlikNo"] != null ? long.Parse(featureAttributes["maks:kimlikNo"].InnerText) : 0,
+                                ad = featureAttributes["maks:ad"] != null ? featureAttributes["maks:ad"].InnerText : "",
+                                katman = type
+                            },
+                            geometry = new maksGeometry()
+                            {
+                                rings = geomForNoComma
+                            }
+                        };
+
+                        string kimlikNoString = maksYapiFeatures.attributes.kimlikno.ToString();
+                        long kimlikNo = long.Parse(kimlikNoString); //bu aslında numarataj no
+
+                        if (type == "yapi")
+                        {
+                            yapiKimlikNoString = await getMaksNumaratajInfoWithKimlikNo(kimlikNo);
+                        }
+                        else // digerYapi
+                        {
+                            yapiKimlikNoString = await getMaksNumaratajInfoWithKimlikNoForDigerYapi(kimlikNo);
+                        }
+                        // yapikimlikNo aslında binanın gerçek bina nosu .
+                        long yapiKimlikNo = 0;
+
+                        if (yapiKimlikNoString != "")
+                        {
+                            yapiKimlikNo = long.Parse(yapiKimlikNoString);
+                        }
+
+                        var mernisBinaKodu = await GetMernisBinaKoduFromKimlikNo(yapiKimlikNo);
+
+                        if (mernisBinaKodu != "")
+                        {
+                            maksYapiFeatures.attributes.id = int.Parse(mernisBinaKodu);
+                        }
+                        else
+                        {
+                            maksYapiFeatures.attributes.id = 0;
+                        }
+
+                        MaksYapiInfo maksYapiInfoFeatures = new MaksYapiInfo()
+                        {
+                            attributes = new MaksYapiInfo.maksInfoAttributes
+                            {
+                                bina_kod = long.Parse(maksYapiFeatures.attributes.id.ToString()),
+                                numarataj_no = kimlikNo
+                            }
+                        };
+
+                        if (type == "yapi")
+                        {
+                            maksYapiInfoFeatures.attributes.dy_kimlik_no = 0;
+                            maksYapiInfoFeatures.attributes.y_kimlik_no = yapiKimlikNo;
+                        }
+                        else // type digerYapi
+                        {
+                            maksYapiInfoFeatures.attributes.dy_kimlik_no = yapiKimlikNo;
+                            maksYapiInfoFeatures.attributes.y_kimlik_no = 0;
+                        }
+
+                        maksYapiFeaturesArray.Add(maksYapiFeatures);
+                        maksYapiInfoFeaturesArray.Add(maksYapiInfoFeatures);
+                        if (geomForNoComma != null)
+                        {
+                            string stringFeatureObject = JsonConvert.SerializeObject(maksYapiFeaturesArray);
+                            string stringFeatureObjectForInfo = JsonConvert.SerializeObject(maksYapiInfoFeaturesArray);
+
+                            var maksYapiFormUrlEncodedContent = new FormUrlEncodedContent(new[]
+                            {
+                                new KeyValuePair<string, string>("f","json"),
+                                new KeyValuePair<string, string>("features", stringFeatureObject),
+                                new KeyValuePair<string, string>("rollbackOnFailure", "true")
+
+                             });
+
+                            var maksYapiFormUrlEncodedContentForInfo = new FormUrlEncodedContent(new[]
+                            {
+                                new KeyValuePair<string, string>("f","json"),
+                                new KeyValuePair<string, string>("features", stringFeatureObjectForInfo),
+                                new KeyValuePair<string, string>("rollbackOnFailure", "true")
+
+                             });
+
+
+                            var maksYapiResponse = await httpClient.PostAsync(maksYapiAddFeaturesURI, maksYapiFormUrlEncodedContent);
+                            var maksYapiContent = await maksYapiResponse.Content.ReadAsStringAsync();
+                            var addFeatureResponse = JsonConvert.DeserializeObject<MaksYapiAddFeatureResponse>(maksYapiContent);
+                            var addFeatureResponseArray = addFeatureResponse.addResults;
+
+                            var maksYapiResponseInfo = await httpClient.PostAsync(maksYapiInfoAddFeaturesURI, maksYapiFormUrlEncodedContentForInfo);
+                            var maksYapiContentInfo = await maksYapiResponseInfo.Content.ReadAsStringAsync();
+                            var addFeatureResponseInfo = JsonConvert.DeserializeObject<MaksYapiAddFeatureResponse>(maksYapiContentInfo);
+                            var addFeatureResponseArrayInfo = addFeatureResponseInfo.addResults;
+
+                            if (addFeatureResponseArray[0].success == true)
+                            {
+                                basariliKayitSayisi++;
+
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                    sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                    sw.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Verinin Kayit Islemi Basarili.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+                                    sw.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                }
+
+                                Console.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                Console.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                Console.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Verinin Kayit Islemi Basarili.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+                                Console.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                sayac++;
+
+                            }
+                            else
+                            {
+                                string errorCode = addFeatureResponseArray[0].error.code;
+
+                                string errorDescription = addFeatureResponseArray[0].error.description;
+
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                    sw.WriteLine();
+                                    sw.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Veriler Kaydedilemedi.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+
+                                }
+
+                                Console.WriteLine("Ad:{0} ", maksYapiFeatures.attributes.ad);
+                                Console.WriteLine();
+                                Console.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Veriler Kaydedilemedi.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+
+                                errorDescription = Console.ReadLine();
+                            }
+
+                            if (addFeatureResponseArrayInfo[0].success == true)
+                            {
+                                basariliKayitSayisi++;
+
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                    sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                    sw.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Verinin Kayit Islemi Basarili.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+                                    sw.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                }
+
+                                Console.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                Console.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                Console.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Verinin Kayit Islemi Basarili.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+                                Console.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                sayac++;
+
+                            }
+                            else
+                            {
+                                string errorCode = addFeatureResponseArray[0].error.code;
+
+                                string errorDescription = addFeatureResponseArray[0].error.description;
+
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                    sw.WriteLine();
+                                    sw.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Veriler Kaydedilemedi.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+
+                                }
+
+                                Console.WriteLine("Ad:{0} ", maksYapiFeatures.attributes.ad);
+                                Console.WriteLine();
+                                Console.WriteLine("MernisBinaKodu:{0} / Bina No: {1} / Numarataj No: {2} / KampusId: {3} Olan Veriler Kaydedilemedi.", mernisBinaKodu, yapiKimlikNo, kimlikNo, kampusId);
+
+                                errorDescription = Console.ReadLine();
+                            }
+                        }
+                    }
+                    else
+                    {
+                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                        {
+                            sw.WriteLine("Sayac: {0}", sayac);
+                            sw.WriteLine("KampusId: {0} => Maks Geometrisi Boş", kampusId);
+
+                        }
+                        Console.WriteLine("Sayac{0}", sayac);
+                        Console.WriteLine("KampusId: {0} => Maks Geometrisi Boş", kampusId);
+
+                        sayac++;
+                        continue;
+                    }
+                }
+
+                sayacObject.sayac = sayac;
+                sayacObject.basariliKayitSayisi = basariliKayitSayisi;
+                return sayacObject;
+            }
+            catch (Exception ex)
+            {
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+
+                    sw.WriteLine("Sayac: {0}", sayac);
+                    sw.WriteLine("HATA: {0}", ex);
+                }
+                Console.WriteLine("Sayac{0}", sayac);
+                Console.WriteLine("HATA: {0}", ex);
+                sayac++;
+
+                sayacObject.sayac = sayac;
+                sayacObject.basariliKayitSayisi = basariliKayitSayisi;
+                return sayacObject;
+            }
+        }
+
+        public async Task<Sayac> getAlanKampusGeometriesContinuationForOnlyPostGeometries(string type, XmlNode lastChildOfCollection, Sayac sayacObject, string kampusId)
+        {
+            string path = @"C:/alankampus.txt";
+
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            var mebIEDBServiceUrl = _configuration.GetSection("ArcgisRefererURL:MebIEDBArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            HttpClient httpClient = new HttpClient();
+
+            httpClient.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+            int basariliKayitSayisi = sayacObject.basariliKayitSayisi;
+            int sayac = sayacObject.sayac;
+            var maksYapiAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/1/addFeatures";
+            var maksYapiInfoAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/3/addFeatures";
+            try
+            {
+                foreach (XmlNode feature in lastChildOfCollection.ChildNodes) //featureMembers
+                {
+                    XmlNode featureAttributes = feature.FirstChild;
+                    List<MaksYapi> maksYapiFeaturesArray = new List<MaksYapi>();
+                    List<MaksYapiInfo> maksYapiInfoFeaturesArray = new List<MaksYapiInfo>();
+                    // var geom = createRings(coordinatesForGeom);
+                    var maksGeom = featureAttributes["maks:geometri"] != null ? featureAttributes["maks:geometri"].InnerText : "";
+                    if (maksGeom != "")
+                    {
+                        var geomForNoComma = createRingsForNoComma(maksGeom);
+                        if (geomForNoComma.Count == 0) // 0 dönerse geometride hata var demektir.
+                        {
+                            var kimlikNoAttribute = featureAttributes["maks:kimlikNo"].InnerText != null ? featureAttributes["maks:kimlikNo"].InnerText : "";
+
+                            if (kimlikNoAttribute == null)
+                            {
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sayac++;
+                                    sw.WriteLine("Sayac: {0}", sayac);
+                                    sw.WriteLine("KampusId: {0} / Kimlik No: {1} Olan Verinin Feature Attributes Bulunamadi.", kampusId, kimlikNoAttribute);
+                                }
+                                Console.WriteLine("Sayac{0}", sayac);
+                                Console.WriteLine("KampusId: {0} / Kimlik No: {1} Olan Verinin Feature Attributes Bulunamadi.", kampusId, kimlikNoAttribute);
+                                continue;
+                            }
+                            else
+                            {
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sayac++;
+                                    sw.WriteLine("Sayac: {0}", sayac);
+                                    sw.WriteLine("Maks Geometrisinde Hata Var.");
+                                    sw.WriteLine("KimlikNo:{0} ", kimlikNoAttribute);
+                                    sw.WriteLine("Geometri: {0} ", maksGeom);
+                                }
+
+                                Console.WriteLine("Sayac{0}", sayac);
+                                Console.WriteLine("Maks Geometrisinde Hata Var."); ;
+                                Console.WriteLine("KimlikNo:{0} ", kimlikNoAttribute);
+                                Console.WriteLine("Geometri: {0} ", maksGeom);
+                                continue;
+                            }
+
+                        }
+
+                        MaksYapi maksYapiFeatures = new MaksYapi()
+                        {
+                            attributes = new maksAttributes()
+                            {
+                                tip = featureAttributes["maks:tip"] != null ? featureAttributes["maks:tip"].InnerText : "",
+                                durum = featureAttributes["maks:durum"] != null ? featureAttributes["maks:durum"].InnerText : "",
+                                olcek = featureAttributes["maks:olcek"] != null ? int.Parse(featureAttributes["maks:olcek"].InnerText) : 0,
+                                olusumyontemi = featureAttributes["maks:olusumYontemi"] != null ? featureAttributes["maks:olusumYontemi"].InnerText : "",
+                                zemin_alti_kat_sayisi = featureAttributes["maks:zeminAltiKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminAltiKatSayisi"].InnerText) : 0,
+                                zemin_ustu_kat_sayisi = featureAttributes["maks:zeminUstuKatSayisi"] != null ? int.Parse(featureAttributes["maks:zeminUstuKatSayisi"].InnerText) : 0,
+                                kimlikno = featureAttributes["maks:kimlikNo"] != null ? long.Parse(featureAttributes["maks:kimlikNo"].InnerText) : 0,
+                                ad = featureAttributes["maks:ad"] != null ? featureAttributes["maks:ad"].InnerText : "",
+                                katman = type
+                            },
+                            geometry = new maksGeometry()
+                            {
+                                rings = geomForNoComma
+                            }
+                        };
+
+                        maksYapiFeaturesArray.Add(maksYapiFeatures);
+                        var kimlikNo = maksYapiFeatures.attributes.kimlikno;
+
+                        if (geomForNoComma != null)
+                        {
+                            string stringFeatureObject = JsonConvert.SerializeObject(maksYapiFeaturesArray);
+
+                            var maksYapiFormUrlEncodedContent = new FormUrlEncodedContent(new[]
+                            {
+                                new KeyValuePair<string, string>("f","json"),
+                                new KeyValuePair<string, string>("features", stringFeatureObject),
+                                new KeyValuePair<string, string>("rollbackOnFailure", "true")
+
+                             });
+
+                            var maksYapiResponse = await httpClient.PostAsync(maksYapiAddFeaturesURI, maksYapiFormUrlEncodedContent);
+                            var maksYapiContent = await maksYapiResponse.Content.ReadAsStringAsync();
+                            var addFeatureResponse = JsonConvert.DeserializeObject<MaksYapiAddFeatureResponse>(maksYapiContent);
+                            var addFeatureResponseArray = addFeatureResponse.addResults;
+
+
+                            if (addFeatureResponseArray[0].success == true)
+                            {
+                                basariliKayitSayisi++;
+
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                    sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                    sw.WriteLine("KampusId: {0}/ KimlikNo: {1} Olan Verinin Kayit Islemi Basarili.", kampusId, kimlikNo);
+                                    sw.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                }
+
+                                Console.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                                Console.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                Console.WriteLine("KampusId: {0}/ KimlikNo: {1} Olan Verinin Kayit Islemi Basarili.", kampusId, kimlikNo);
+                                Console.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                                sayac++;
+
+                            }
+                            else
+                            {
+                                string errorCode = addFeatureResponseArray[0].error.code;
+
+                                string errorDescription = addFeatureResponseArray[0].error.description;
+
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("Ad: {0} ", maksYapiFeatures.attributes.ad);
+                                    sw.WriteLine();
+                                    sw.WriteLine("KampusId: {0}/ KimlikNo: {1} Olan Veriler Kaydedilemedi.", kampusId, kimlikNo);
+
+                                }
+
+                                Console.WriteLine("Ad:{0} ", maksYapiFeatures.attributes.ad);
+                                Console.WriteLine();
+                                Console.WriteLine("KampusId: {0}/ KimlikNo: {1} Olan Veriler Kaydedilemedi.", kampusId, kimlikNo);
+
+                                errorDescription = Console.ReadLine();
+                            }
+
+                        }
+                    }
+                    else
+                    {
+                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                        {
+                            sw.WriteLine("Sayac: {0}", sayac);
+                            sw.WriteLine("KampusId: {0} => Maks Geometrisi Boş", kampusId);
+
+                        }
+                        Console.WriteLine("Sayac{0}", sayac);
+                        Console.WriteLine("KampusId: {0} => Maks Geometrisi Boş", kampusId);
+
+                        sayac++;
+                        continue;
+                    }
+                }
+
+                sayacObject.sayac = sayac;
+                sayacObject.basariliKayitSayisi = basariliKayitSayisi;
+                return sayacObject;
+            }
+            catch (Exception ex)
+            {
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+
+                    sw.WriteLine("Sayac: {0}", sayac);
+                    sw.WriteLine("HATA: {0}", ex);
+                }
+                Console.WriteLine("Sayac{0}", sayac);
+                Console.WriteLine("HATA: {0}", ex);
+                sayac++;
+
+                sayacObject.sayac = sayac;
+                sayacObject.basariliKayitSayisi = basariliKayitSayisi;
+                return sayacObject;
+            }
+        }
+
+        public async Task<Sayac> getAlanKampusGeometriesContinuationForOnlyPostInfos(string type, XmlNode lastChildOfCollection, Sayac sayacObject, string kimlikNo)
+        {
+            string path = @"C:/alankampus.txt";
+
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            var mebIEDBServiceUrl = _configuration.GetSection("ArcgisRefererURL:MebIEDBArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            HttpClient httpClient = new HttpClient();
+
+            httpClient.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+            int basariliKayitSayisi = sayacObject.basariliKayitSayisi;
+            int sayac = sayacObject.sayac;
+            var sourceURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/1/addFeatures";
+            var maksYapiInfoAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/3/addFeatures";
+            try
+            {
+                foreach (XmlNode feature in lastChildOfCollection.ChildNodes) //featureMembers
+                {
+                    XmlNode featureAttributes = feature.FirstChild;
+                    List<MaksYapiInfo> maksYapiInfoFeaturesArray = new List<MaksYapiInfo>();
+
+                    MaksYapiInfo maksYapiInfoFeatures = new MaksYapiInfo()
+                    {
+                        attributes = new MaksYapiInfo.maksInfoAttributes() { }
+
+                    };
+
+                    string binaKodu = "";
+
+                    var digerYapi = featureAttributes["maks:digerYapiKimlikNo"];
+                    var yapi = featureAttributes["maks:yapiKimlikNo"];
+
+                    if (digerYapi == null)
+                    {
+                        type = "yapi";
+                    }
+                    else if (yapi == null)
+                    {
+                        type = "digerYapi";
+                    }
+
+                    if (type == "yapi")
+                    {
+                        var attributes = maksYapiInfoFeatures.attributes;
+                        var yKimlikNo = featureAttributes["maks:yapiKimlikNo"].InnerText != null ? long.Parse(featureAttributes["maks:yapiKimlikNo"].InnerText) : 0;
+                        var numaratajNo = featureAttributes["maks:kimlikNo"].InnerText != null ? long.Parse(featureAttributes["maks:kimlikNo"].InnerText) : 0;
+
+                        attributes.y_kimlik_no = yKimlikNo;
+                        attributes.numarataj_no = numaratajNo;
+                        attributes.dy_kimlik_no = 0;
+                        binaKodu = await GetMernisBinaKoduFromKimlikNo(numaratajNo);
+                        if (binaKodu == "")
+                        {
+                            attributes.bina_kod = 0;
+                        }
+                        else
+                        {
+                            attributes.bina_kod = long.Parse(binaKodu);
+                        }
+                    }
+                    else
+                    {
+                        var attributes = maksYapiInfoFeatures.attributes;
+                        var dyKimlikNo = featureAttributes["maks:digerYapiKimlikNo"].InnerText != null ? long.Parse(featureAttributes["maks:digerYapiKimlikNo"].InnerText) : 0;
+                        var numaratajNo = featureAttributes["maks:kimlikNo"].InnerText != null ? long.Parse(featureAttributes["maks:kimlikNo"].InnerText) : 0;
+
+                        attributes.dy_kimlik_no = dyKimlikNo;
+                        attributes.numarataj_no = numaratajNo;
+                        attributes.y_kimlik_no = 0;
+                        binaKodu = await GetMernisBinaKoduFromKimlikNo(numaratajNo);
+                        if (binaKodu == "")
+                        {
+                            attributes.bina_kod = 0;
+                        }
+                        else
+                        {
+                            attributes.bina_kod = long.Parse(binaKodu);
+                        }
+                    }
+
+                    maksYapiInfoFeaturesArray.Add(maksYapiInfoFeatures);
+                    var numaratajNo_ = maksYapiInfoFeatures.attributes.numarataj_no;
+                    string stringFeatureObject = JsonConvert.SerializeObject(maksYapiInfoFeaturesArray);
+
+                    var maksYapiFormUrlEncodedContent = new FormUrlEncodedContent(new[]
+                    {
+                                new KeyValuePair<string, string>("f","json"),
+                                new KeyValuePair<string, string>("features", stringFeatureObject),
+                                new KeyValuePair<string, string>("rollbackOnFailure", "true")
+
+                             });
+
+                    var maksYapiResponse = await httpClient.PostAsync(maksYapiInfoAddFeaturesURI, maksYapiFormUrlEncodedContent);
+                    var maksYapiContent = await maksYapiResponse.Content.ReadAsStringAsync();
+                    var addFeatureResponse = JsonConvert.DeserializeObject<MaksYapiAddFeatureResponse>(maksYapiContent);
+                    var addFeatureResponseArray = addFeatureResponse.addResults;
+
+
+                    if (addFeatureResponseArray[0].success == true)
+                    {
+                        basariliKayitSayisi++;
+
+                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                        {
+                            sw.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                            sw.WriteLine("KimlikNo: {0} / Numarataj No: {1} / Bina Kodu: {2} Olan Verinin Kayit Islemi Basarili.", kimlikNo, numaratajNo_, binaKodu);
+                            sw.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                        }
+
+                        Console.WriteLine("{0}. Kayıt İşlemi Başarılı.", sayac);
+                        Console.WriteLine("KimlikNo: {0} / Numarataj No: {1} / Bina Kodu: {2} Olan Verinin Kayit Islemi Basarili.", kimlikNo, numaratajNo_, binaKodu);
+                        Console.WriteLine("Toplam Basarili kayit Sayisi: {0}", basariliKayitSayisi);
+                        sayac++;
+                    }
+                    else
+                    {
+                        string errorCode = addFeatureResponseArray[0].error.code;
+
+                        string errorDescription = addFeatureResponseArray[0].error.description;
+
+                        using (StreamWriter sw = System.IO.File.AppendText(path))
+                        {
+                            sw.WriteLine("KimlikNo: {0} / Numarataj No: {1} / Bina Kodu: {2} Olan Veriler Kaydedilemedi.", kimlikNo, numaratajNo_, binaKodu);
+                        }
+
+                        Console.WriteLine();
+                        Console.WriteLine("KimlikNo: {0} / Numarataj No: {1} / Bina Kodu: {2} Olan Veriler Kaydedilemedi.", kimlikNo, numaratajNo_, binaKodu);
+
+                        errorDescription = Console.ReadLine();
+                    }
+
+                }
+
+                sayacObject.sayac = sayac;
+                sayacObject.basariliKayitSayisi = basariliKayitSayisi;
+                return sayacObject;
+            }
+            catch (Exception ex)
+            {
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+
+                    sw.WriteLine("Sayac: {0}", sayac);
+                    sw.WriteLine("HATA: {0}", ex);
+                }
+                Console.WriteLine("Sayac{0}", sayac);
+                Console.WriteLine("HATA: {0}", ex);
+                sayac++;
+
+                sayacObject.sayac = sayac;
+                sayacObject.basariliKayitSayisi = basariliKayitSayisi;
+                return sayacObject;
+            }
+        }
+
+        [HttpGet("getAlanKampusGeometriesForOnlyPostInfos")]
+        public async Task<IActionResult> getAlanKampusGeometriesForOnlyPostInfos()
+        {
+            string path = @"C:/alankampus.txt";
+
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            var mebIEDBServiceUrl = _configuration.GetSection("ArcgisRefererURL:MebIEDBArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            HttpClient httpClient = new HttpClient();
+
+            httpClient.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+            int offsetMultiplier = 0;
+            int featureCount = 1;
+            Sayac sayacObject = new Sayac();
+            int sayac = 0;
+
+            var alanKampusQueryURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/1/query";
+            var maksYapiAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/3/addFeatures";
+
+            try
+            {
+                while (featureCount != 0)
+                {
+                    var resulttOffset = offsetMultiplier * 1000;
+
+                    var formUrlEncodedContent = new FormUrlEncodedContent(new[]
+                       {
+                        new KeyValuePair<string, string>("where", "1=1"),
+                        new KeyValuePair<string, string>("geometryType", "esriGeometryEnvelope"),
+                        new KeyValuePair<string, string>("spatialRel", "esriSpatialRelIntersects"),
+                        new KeyValuePair<string, string>("outFields", "katman,tip,ad,kimlikno" ),
+                        new KeyValuePair<string, string>("returnGeometry", "true"),
+                        new KeyValuePair<string, string>("f", "pjson"),
+                        new KeyValuePair<string, string>("resultOffset", resulttOffset.ToString()),
+                        new KeyValuePair<string, string>("token", arcgisTokenDto.token)
+                    });
+
+                    var response = await httpClient.PostAsync(alanKampusQueryURI, formUrlEncodedContent);
+
+                    var content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+                    var coordinates = "";
+                    var coordinatesForGeom = "";
+                    var features = arcgisResponseData["features"].ToArray();
+
+                    for (var i = 0; i < features.Length; i++)
+                    {
+                        var rings = features[i]["geometry"]["rings"][0].ToArray();
+                        string kimlikNo = features[i]["attributes"]["kimlikno"].ToString();
+                        var type = features[i]["attributes"]["katman"].ToString();
+                        XmlNode lastChildOfCollection;
+
+                        for (var j = 0; j < rings.Length; j++)
+                        {
+                            var ring = rings[j].ToArray();
+
+                            coordinates += ring[0] + " ";
+                            coordinates += ring[1] + " ";
+
+                            coordinatesForGeom += ring[0] + ",";
+                            coordinatesForGeom += ring[1] + " ";
+
+                        }
+                        coordinates = coordinates.Remove(coordinates.Length - 1);
+                        coordinatesForGeom = coordinatesForGeom.Remove(coordinates.Length - 1);
+
+                        var maksInfo = await getMaksNumaratajInfo(coordinates, sayac + 1);
+
+                        if (maksInfo != null)
+                        {
+                            lastChildOfCollection = maksInfo.LastChild; // featureCollection
+
+                            if (lastChildOfCollection.ChildNodes.Count == 0 || lastChildOfCollection.InnerText.Contains("hata"))
+                            {
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("Sayac: {0}", sayac);
+                                    sw.WriteLine("Kimlik No : {0} => Maks:Numaratajda Kesişim Bulunamadi.", kimlikNo);
+                                }
+                                Console.WriteLine("Kimlik No : {0} => Maks:Numaratajda Kesişim Bulunamadi.", kimlikNo);
+                                sayac++;
+                            }
+                            else
+                            {
+                                sayacObject.sayac = sayac;
+                                sayacObject = await getAlanKampusGeometriesContinuationForOnlyPostInfos(type, lastChildOfCollection, sayacObject, kimlikNo);
+                                sayac = sayacObject.sayac;
+                            }
+
+                        }
+                        else
+                        {
+                            using (StreamWriter sw = System.IO.File.AppendText(path))
+                            {
+                                sw.WriteLine("Sayac: {0}", sayac);
+                                sw.WriteLine("Kimlik No : {0} => Maks:Numaratajda Karsiligi Bulunamadi.", kimlikNo);
+                            }
+                            Console.WriteLine("Kimlik No : {0} => Maks:Numaratajda Karsiligi Bulunamadi.", kimlikNo);
+                            sayac++;
+                        }
+
+                        sayacObject.sayac = sayac;
+                        sayac = sayacObject.sayac;
+                        coordinates = "";
+                        coordinatesForGeom = "";
+
+                        // geometri bulamazsa kampüs id bastırıp bu kampus id nin maksta karşılığı bulunamadı
+                        // onun dışındaki hatalarda pilotbinalar için yaptığımız gibi binano, kimlikno, binakodu bu olanın şusu bulunamadı vs.. gibi yazdır.
+                    }
+
+                    featureCount = features.Count();
+
+                    offsetMultiplier++;
+                }
+            }
+            catch (Exception ex)
+            {
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+                    sw.WriteLine("Sayac: {0}", sayac);
+                    sw.WriteLine("HATA: {0}", ex);
+                }
+                Console.WriteLine("Sayac{0}", sayac);
+                Console.WriteLine("HATA: {0}", ex);
+                sayac++;
+            }
+            using (StreamWriter sw = System.IO.File.AppendText(path))
+            {
+                sw.WriteLine("Tarama Bitti.");
+            }
+            Console.WriteLine("Tarama Bitti");
+
+            return Ok();
+        }
+
+        [HttpGet("getAlanKampusGeometries")]
+        public async Task<IActionResult> getAlanKampusGeometries()
+        {
+            string path = @"C:/alankampus.txt";
+
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            var mebIEDBServiceUrl = _configuration.GetSection("ArcgisRefererURL:MebIEDBArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            HttpClient httpClient = new HttpClient();
+
+            httpClient.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", arcgisTokenDto.token);
+
+            int offsetMultiplier = 0;
+            int featureCount = 1;
+            Sayac sayacObject = new Sayac();
+            int sayac = 0;
+
+            var alanKampusQueryURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/0/query";
+            var maksYapiAddFeaturesURI = mebIEDBServiceUrl + "maks_yapi/FeatureServer/1/addFeatures";
+
+            try
+            {
+                while (featureCount != 0)
+                {
+                    var resulttOffset = offsetMultiplier * 1000;
+
+                    var formUrlEncodedContent = new FormUrlEncodedContent(new[]
+                       {
+                        new KeyValuePair<string, string>("where", "1=1"),
+                        new KeyValuePair<string, string>("geometryType", "esriGeometryEnvelope"),
+                        new KeyValuePair<string, string>("spatialRel", "esriSpatialRelIntersects"),
+                        new KeyValuePair<string, string>("outFields", "objectid,refid" ),
+                        new KeyValuePair<string, string>("returnGeometry", "true"),
+                        new KeyValuePair<string, string>("f", "pjson"),
+                        new KeyValuePair<string, string>("resultOffset", resulttOffset.ToString()),
+                        new KeyValuePair<string, string>("token", arcgisTokenDto.token)
+                    });
+
+                    var response = await httpClient.PostAsync(alanKampusQueryURI, formUrlEncodedContent);
+
+                    var content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+                    var coordinates = "";
+                    var coordinatesForGeom = "";
+                    var features = arcgisResponseData["features"].ToArray();
+
+                    for (var i = 0; i < features.Length; i++)
+                    {
+                        var rings = features[i]["geometry"]["rings"][0].ToArray();
+                        var kampusId = features[i]["attributes"]["refid"].ToString();
+                        XmlNode lastChildOfCollection;
+                        XmlNode lastChildOfCollectionOfYapiDiger;
+
+                        for (var j = 0; j < rings.Length; j++)
+                        {
+                            var ring = rings[j].ToArray();
+
+                            coordinates += ring[0] + " ";
+                            coordinates += ring[1] + " ";
+
+                            coordinatesForGeom += ring[0] + ",";
+                            coordinatesForGeom += ring[1] + " ";
+
+                        }
+                        coordinates = coordinates.Remove(coordinates.Length - 1);
+                        coordinatesForGeom = coordinatesForGeom.Remove(coordinates.Length - 1);
+
+                        var maksInfo = await getMaksInfo(coordinates, sayac + 1);
+                        var maksYapiDigerInfo = await getMaksYapiDigerInfo(coordinates, sayac + 1);
+
+                        if (maksInfo != null)
+                        {
+                            lastChildOfCollection = maksInfo.LastChild; // featureCollection
+
+                            if (lastChildOfCollection.ChildNodes.Count == 0)
+                            {
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("Sayac: {0}", sayac);
+                                    sw.WriteLine("Kampus Id : {0} => Maks:Yapida Kesişim Bulunamadi.", kampusId);
+                                }
+                                Console.WriteLine("Kampus Id : {0} => Maks:Yapida Kesişim Bulunamadi.", kampusId);
+                                sayac++;
+                            }
+                            else
+                            {
+                                sayacObject.sayac = sayac;
+                                sayacObject = await getAlanKampusGeometriesContinuationForOnlyPostGeometries("yapi", lastChildOfCollection, sayacObject, kampusId);
+                            }
+
+                        }
+                        else
+                        {
+                            using (StreamWriter sw = System.IO.File.AppendText(path))
+                            {
+                                sw.WriteLine("Sayac: {0}", sayac);
+                                sw.WriteLine("Kampus Id : {0} => Maks:Yapida Karsiligi Bulunamadi.", kampusId);
+                            }
+                            Console.WriteLine("Kampus Id : {0} => Maks:Yapida Karsiligi Bulunamadi.", kampusId);
+                            sayac++;
+                        }
+
+                        if (maksYapiDigerInfo != null)
+                        {
+                            lastChildOfCollectionOfYapiDiger = maksYapiDigerInfo.LastChild; // featureCollection
+
+                            if (lastChildOfCollectionOfYapiDiger.ChildNodes.Count == 0)
+                            {
+                                using (StreamWriter sw = System.IO.File.AppendText(path))
+                                {
+                                    sw.WriteLine("Sayac: {0}", sayac);
+                                    sw.WriteLine("Kampus Id : {0} => Maks:DigerYapida Kesişim Bulunamadi.", kampusId);
+                                }
+                                Console.WriteLine("Kampus Id : {0} => Maks:DigerYapida Kesişim Bulunamadi.", kampusId);
+                                sayac++;
+                            }
+                            else
+                            {
+                                sayacObject.sayac = sayac;
+                                sayacObject = await getAlanKampusGeometriesContinuationForOnlyPostGeometries("digerYapi", lastChildOfCollectionOfYapiDiger, sayacObject, kampusId);
+                            }
+
+                        }
+                        else
+                        {
+                            using (StreamWriter sw = System.IO.File.AppendText(path))
+                            {
+                                sw.WriteLine("Sayac: {0}", sayac);
+                                sw.WriteLine("Kampus Id : {0} => Maks:DigerYapida Karsiligi Bulunamadi.", kampusId);
+
+                            }
+                            Console.WriteLine("Kampus Id : {0} => Maks:DigerYapida Karsiligi Bulunamadi.", kampusId);
+                            sayac++;
+                        }
+
+                        sayacObject.sayac = sayac;
+                        sayac = sayacObject.sayac;
+                        coordinates = "";
+                        coordinatesForGeom = "";
+
+                        // geometri bulamazsa kampüs id bastırıp bu kampus id nin maksta karşılığı bulunamadı
+                        // onun dışındaki hatalarda pilotbinalar için yaptığımız gibi binano, kimlikno, binakodu bu olanın şusu bulunamadı vs.. gibi yazdır.
+                    }
+
+                    featureCount = features.Count();
+
+                    offsetMultiplier++;
+                }
+            }
+            catch (Exception ex)
+            {
+                using (StreamWriter sw = System.IO.File.AppendText(path))
+                {
+                    sw.WriteLine("Sayac: {0}", sayac);
+                    sw.WriteLine("HATA: {0}", ex);
+                }
+                Console.WriteLine("Sayac{0}", sayac);
+                Console.WriteLine("HATA: {0}", ex);
+                sayac++;
+            }
+            using (StreamWriter sw = System.IO.File.AppendText(path))
+            {
+                sw.WriteLine("Tarama Bitti.");
+            }
+            Console.WriteLine("Tarama Bitti");
+
+            return Ok();
+        }
+
+        [Produces("application/xml")]
+        [HttpGet("getMaksInfo")]
+        public async Task<XmlDocument> getMaksInfo(string coordinates, int sayac)
+        {
+            try
+            {
+                var url = "http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Yapi&username=mebcbs&password=9UFdAy7x&FILTER=%3CFilter%20xmlns=%22http://www.opengis.net/ogc%22%20xmlns:gml=%22http://www.opengis.net/gml%22%3E%3CIntersects%3E%3CPropertyName%3Egeometri%3C/PropertyName%3E%3Cgml:Polygon%20xmlns:gml=%22http://www.opengis.net/gml%22%20srsName=%22EPSG:4326%22%3E%3Cgml:exterior%3E%3Cgml:LinearRing%3E%3Cgml:posList%3E" + coordinates + "%3C/gml:posList%3E%3C/gml:LinearRing%3E%3C/gml:exterior%3E%3C/gml:Polygon%3E%3C/Intersects%3E%3C/Filter%3E";
+                // var url2 = "http://mernis.meb.gov.tr/WFSService/wfs.asmx/Get?SERVICE=WFS&VERSION=1.1.0&REQUEST=GetFeature&TYPENAME=maks:Yapi&username=mebcbs&password=9UFdAy7x&FILTER=<Filter xmlns=\"http://www.opengis.net/ogc\" xmlns:gml=\"http://www.opengis.net/gml\"><Intersects><PropertyName>geometri</PropertyName><gml:Polygon xmlns:gml=\"http://www.opengis.net/gml\" srsName=\"EPSG:4326\"><gml:exterior><gml:LinearRing><gml:posList>" + coordinates + "</gml:posList></gml:LinearRing></gml:exterior></gml:Polygon></Intersects></Filter>";
+
+                //int revSlashAsciiCode = 92;
+                //int spaceAsciiCode = 32;
+                //char revSlashCharValue = Convert.ToChar(revSlashAsciiCode);
+                //char spaceCharValue = Convert.ToChar(spaceAsciiCode);
+
+                //var deneme = url.Contains(revSlashCharValue);
+                //char[] ddddd = url.ToCharArray();
+                //string stringOfChars = new string(ddddd);
+                //url = stringOfChars;
+                var httpClient = new HttpClient();
+                System.Threading.Thread.Sleep(1000);
+                var response = await httpClient.GetAsync(url);
+
+                var content = await response.Content.ReadAsStringAsync();
+
+                XmlDocument doc = new XmlDocument();
+
+                doc.LoadXml(content);
+
+                return doc;
+
+            }
+            catch (Exception ex)
+            {
+                //using (StreamWriter sw = System.IO.File.AppendText(path))
+                //{
+                //    sw.WriteLine("{0}. islem ", sayac);
+                //    Console.WriteLine("Error!: {0}", ex);
+                //}
+                Console.WriteLine("Error!: {0}", ex);
+                return null;
+            }
+
+        }
+
