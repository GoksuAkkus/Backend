+                        yapiKullanimAmac = adresBilgi.YapiKullanimAmac.Aciklama,
+                    };
+
+                    yerlesimNviAdres.koyAdresi = nviData;
+                }
+                if (adresBilgileri.YurtDisiAdresi != null)
+                {
+                    var adresBilgi = adresBilgileri.YurtDisiAdresi;
+                    var disTemsiclik = adresBilgi.DisTemsilcilik.Aciklama;
+                    var yabanciAdres = adresBilgi.YabanciAdres;
+                    var yabanciUlke = adresBilgi.YabanciUlke;
+                    var yabanciSehir = adresBilgi.YabanciSehir;
