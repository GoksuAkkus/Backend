+++ b/MebCbsAnka/Middlewares/AuthenticationMiddleware.cs
@@ -0,0 +1,73 @@
+﻿using System;
+using System.Collections.Generic;
+using System.IdentityModel.Tokens.Jwt;
+using System.Linq;
+using System.Security.Claims;
+using System.Text;
+using System.Threading.Tasks;
+using Microsoft.AspNetCore.Http;
+using Newtonsoft.Json.Linq;
+
+namespace MebCbsAnka.Middlewares
+{
+    public class AuthenticationMiddleware
+    {
+        private readonly RequestDelegate _next;
+
+        // Dependency Injection
+        public AuthenticationMiddleware(RequestDelegate next)
+        {
+            _next = next;
+        }
+
+        public async Task Invoke(HttpContext context)
+        {
+            string authHeader = context.Request.Headers["Authorization"];
+
+            if (authHeader != null)
+            {
+                int startPoint = authHeader.IndexOf(".") + 1;
+                int endPoint = authHeader.LastIndexOf(".");
+
+                var tokenString = authHeader.Substring(startPoint, endPoint - startPoint).Split(".");
+                var token = tokenString[0].Replace('-', '+').Replace('_', '/').ToString();
+                while(token.Length % 4 != 0)
+                {
+                    token += '=';
+                }
+                var credentialString = Encoding.UTF8
+                    .GetString(Convert.FromBase64String(token));
+
+                var credentialObject = JObject.Parse(credentialString);
+                // Splitting the data from Jwt
+                var credentials = credentialString.Split(new char[] { ':', ',' });
+
+                // Trim this string.
+                var userId = credentialObject.GetValue("nameid").Value<string>();
+                var userName = credentialObject.GetValue("unique_name").Value<string>();
+                var role = credentialObject.GetValue("role").Value<string>();
+                var parentId= credentialObject.GetValue("parentId").Value<string>();
+                var groupName= credentialObject.GetValue("group").Value<string>();
+                //var userRule = credentials[5].Replace("\"", "");
+                //var userName = credentials[3].Replace("\"", "");
+
+                // Identity Principal
+                var claims = new[]
+                {
+                    new Claim("name", userName),
+                    new Claim("id", userId),
+                    new Claim("role", role),
+                    new Claim(ClaimTypes.Role, role),
+                   // new Claim("parentId",parentId), 
+                    new Claim("group",groupName), 
+                };
+                var identity = new ClaimsIdentity(claims, "basic");
+                context.User = new ClaimsPrincipal(identity);
+
+            }
+            await _next(context);
+        }
+
+
+    }
+}
