+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.Dtos;
+using MebCbsAnka.Entities;
+using Microsoft.AspNetCore.Authorization;
+using Microsoft.AspNetCore.Http;
+using Microsoft.AspNetCore.Mvc;
+
+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/PermLayers")]
+    public class PermLayersController : Controller
+    {
+        private IPermLayerDal _permLayerDal;
+
+        // Dependency Injection
+        public PermLayersController(IPermLayerDal permLayerDal)
+        {
+            _permLayerDal = permLayerDal;
+        }
+
+        [HttpGet("")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public IActionResult Get()
+        {
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var groupName = userClaimList[4].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            var permissionLayers = _permLayerDal.GetList(p => p.UserId == userId).Select(p => new PermLayerDto()
+            {
+                Field = p.Field,
+                Layer = p.Layer,
+                Permission = p.Permission,
+            });
+
+            return Ok(permissionLayers);
+        }
+
+        [HttpGet("{userId}")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public IActionResult Get(int userId)
+        {
+            // TODO: Get User Permission için herhangi bir işlem yok;
+
+            var permissionLayer = _permLayerDal.GetList(p => p.UserId == userId).Select(p => new PermLayerDto()
+            {
+                Layer = p.Layer,
+                Field = p.Field,
+                Permission = p.Permission,
+            });
+            return Ok(permissionLayer);
+        }
+
+        [HttpPost("")]
+        public IActionResult Post([FromBody] PermLayer permLayer)
+        {
+            _permLayerDal.Add(permLayer);
+            return new StatusCodeResult(201);
+        }
+
+        [HttpPut("")]
+        public IActionResult Put([FromBody] PermLayer permLayer)
+        {
+            _permLayerDal.Update(permLayer);
+            return Ok(permLayer);
+        }
+
+        [HttpDelete("{permLayerId}")]
+        public IActionResult Delete(int permLayerId)
+        {
+            _permLayerDal.Delete(new PermLayer() { Id = permLayerId });
+            return Ok();
+        }
+    }
