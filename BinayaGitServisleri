+            //Console.WriteLine("Mernis Bina Kodu: {0}/ Kod: {1}", mernisBinaKodu, kod);
+            Console.WriteLine("Ada: {0}/ Parsel: {1}", ada, parsel);
+            Console.WriteLine("EsBina KimlikNo: {0}/ EsBina Kodu: {1}", esbinaKimlikNo, esBinaKodu);
+            //Console.WriteLine("Bina Durum: {0}", binaDurum);
+            //Console.WriteLine("PostaKodu: {0}/ Csbm Kodu: {1}/ Dis Kapi No: {2}", postaKodu, csbmKodu, disKapiNo);
+            //Console.WriteLine("Site Adi: {0}/ Blok Adi: {1}", siteAdi, blokAdi);
+            //Console.WriteLine("Yol Alti Kat Sayisi: {0}/ Yol Ustu Kat Sayisi: {1}", yolAltiKatSay, yolUstuKatSay);
+            //Console.WriteLine("Pafta: {0}", pafta);
+
+            using (StreamWriter sw = System.IO.File.AppendText(path))
+            {
+                sw.WriteLine("____________________________________________");
+                sw.WriteLine("{0}. Islem Bilgileri: ", sayac);
+
+                if (binaYapiTipi == null)
+                {
+                    sw.WriteLine("Bina Yapi Tipi: null ");
+                }
+                else
+                {
+                    sw.WriteLine("Bina Yapi Tipi: {0} ", binaYapiTipi.Aciklama);
+                }
+                if (numaratajTipi == null)
+                {
+                    sw.WriteLine("Bina Numarataj Tipi: null");
+                }
+                else
+                {
+                    sw.WriteLine("Bina Numarataj Tipi: {0} ", numaratajTipi.Aciklama);
+                }
+                //sw.WriteLine("____________________________________________");
+                //sw.WriteLine("BinaNo: {0}", binaNumarasi);
+                //sw.WriteLine("Mernis Bina Kodu: {0}/ Kod: {1}", mernisBinaKodu, kod);
+                sw.WriteLine("Ada: {0}/ Parsel: {1}", ada, parsel);
+                sw.WriteLine("EsBina KimlikNo: {0}/ EsBina Kodu: {1}", esbinaKimlikNo, esBinaKodu);
+                //sw.WriteLine("Bina Durum: {0}", binaDurum);
+                //sw.WriteLine("PostaKodu: {0}/ Csbm Kodu: {1}/ Dis Kapi No: {2}", postaKodu, csbmKodu, disKapiNo);
+                //sw.WriteLine("Site Adi: {0}/ Blok Adi: {1}", siteAdi, blokAdi);
+                //sw.WriteLine("Yol Alti Kat Sayisi: {0}/ Yol Ustu Kat Sayisi: {1}", yolAltiKatSay, yolUstuKatSay);
+                //sw.WriteLine("Pafta: {0}", pafta);
+            }
+
+            if (response != null)
+            {
+                binaBilgileri.esBinaKimlikNo = response.SorguSonucu[0].EsBinaKimlikNo; //BİNANO OLAN HERŞEY ESBİNANO OLARAK DEĞİŞECEK
+                binaBilgileri.binaNo = response.SorguSonucu[0].BinaNo;
+
+                return binaBilgileri;
+            }
+            else
+            {
+                binaBilgileri.binaNo = 0;
+                binaBilgileri.esBinaKimlikNo = 0;
+                return binaBilgileri;
+            }
+        }
