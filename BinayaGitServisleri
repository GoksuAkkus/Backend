+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.Dtos;
+using MebCbsAnka.Entities;
+using Microsoft.AspNetCore.Authorization;
+using Microsoft.AspNetCore.Http;
+using Microsoft.AspNetCore.Mvc;
+
+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/PermGuis")]
+    public class PermGuisController : Controller
+    {
+        private IPermGUIDal _permGUIDal;
+
+        public PermGuisController(IPermGUIDal permGUIDal)
+        {
+            _permGUIDal = permGUIDal;
+        }
+
+        [HttpGet("")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public IActionResult Get()
+        {
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var groupName = userClaimList[4].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            var permissionGuis = _permGUIDal.GetList(p => p.UserId == userId).Select(p => new PermGUIDto()
+            {
+                Menu = p.Menu,
+                Item = p.Item,
+            });
+            return Ok(permissionGuis);
+        }
+
+        [HttpGet("{userId}")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public IActionResult Get(int userId)
+        {
+            // TODO: Kontrol Mekanizması eklenmeli
+            var permissionGui = _permGUIDal.GetList(p => p.UserId == userId).Select(p => new PermGUIDto()
+            {
+                Item = p.Item,
+                Menu = p.Menu,
+            });
+
+            return Ok(permissionGui);
+        }
+
+        // NOTE* If you want to read the data from JSON/Text you need to add [from body]
+        [HttpPost("")]
+        public IActionResult Post([FromBody] PermGUI permGUI)
+        {
+            _permGUIDal.Add(permGUI);
+            return new StatusCodeResult(201);
+        }
+
+
+        [HttpPut("")]
+        public IActionResult Put([FromBody] PermGUI permGUI)
+        {
+            _permGUIDal.Update(permGUI);
+            return Ok(permGUI);
+        }
+
+
+        [HttpDelete("{permGuiId}")]
+        public IActionResult Delete(int permGuiId)
+        {
+            _permGUIDal.Delete(new PermGUI { Id = permGuiId });
+            return Ok();
+        }
+    }
+}
