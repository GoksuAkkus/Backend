+        [HttpGet]
+        [Authorize(Roles = "Admin,SuperAdmin")]
+        [HttpGet("GetUsers")]
+        public ActionResult GetUsers()
+        {
+            // Read From JWT
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var groupName = userClaimList[4].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            bool isRoot = false;
+            if (userId == 1)
+            {
+                isRoot = true;
+            }
+            // Retrieve Information
+            var users = _authRepository.GetUsers(userId, isRoot);
+            return Ok(users);
+        }
+
+        [HttpGet]
+        [Authorize(Roles = "Admin,SuperAdmin")]
+        [HttpGet("GetGroups")]
+        public ActionResult GetGroups()
+        {
+            // Read From JWT
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var groupName = userClaimList[4].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            bool isRoot = false;
+            if (userId == 1)
+            {
+                isRoot = true;
+            }
+            // Retrieve Information
+            var users = _authRepository.GetGroups(userId, isRoot);
+            if (users == null)
+            {
+                return NotFound();
+            }
+            return Ok(users);
+        }
