+        [HttpPut("UpdateUser")]
+        public IActionResult UpdateUserInformationById([FromBody] UserForUpdatingInformation userForUpdatingInformation)
+        {
+            // The one who is updating will be the new parent of the user
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+
+            var userToUpdate = new User
+            {
+                Id = userForUpdatingInformation.Id,
+                UserName = userForUpdatingInformation.UserName.ToLower(),
+                Email = userForUpdatingInformation.Email.ToLower(),
+                Phone = userForUpdatingInformation.Phone,
+                Title = userForUpdatingInformation.Title.ToLower(),
+                GroupName = userForUpdatingInformation.GroupName,
+                IsActive = userForUpdatingInformation.IsActive,
+                IsAdmin = userForUpdatingInformation.IsAdmin,
+                Dept = userForUpdatingInformation.Dept.ToLower(),
+                UpdatedDate = userForUpdatingInformation.UpdatedDate,
+                FirstName = userForUpdatingInformation.FirstName.ToLower(),
+                LastName = userForUpdatingInformation.LastName.ToLower(),
+                ParentId = userId,
+                tryLoginCount = 0
+            };
+            _authRepository.UpdateInformation(userToUpdate, userForUpdatingInformation.Password);
+            return Ok(userToUpdate.UserName + "Information Updated");
+        }
