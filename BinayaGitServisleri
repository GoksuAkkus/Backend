diff --git a/MebCbsAnka/DataAccess/MebCbsAnkaContextExtensions.cs b/MebCbsAnka/DataAccess/MebCbsAnkaContextExtensions.cs
new file mode 100644
index 0000000000000000000000000000000000000000..ab25c3bb95e85963f1d19f7e0a027d1bb6d6979b
--- /dev/null
+++ b/MebCbsAnka/DataAccess/MebCbsAnkaContextExtensions.cs
@@ -0,0 +1,56 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Concrete;
+using MebCbsAnka.Entities;
+using MebCbsAnka.Helper;
+
+namespace MebCbsAnka.DataAccess
+{
+    public static class MebCbsAnkaContextExtensions
+    {
+        public static void EnsureDatabaseSeeded(this MebCbsAnkaContext context)
+        {
+            // If the user table is empty 
+            // *ALERT: CHANGE IN PRODUCTION
+            if (!context.Users.Any())
+            {
+                byte[] passwordHash, passwordSalt;
+                var util = new Utils();
+                util.CreatePasswordHash("1234", out passwordHash, out passwordSalt);
+                
+                context.AddRange(new User[]
+                {
+                    new User()
+                    {
+                        UserName = "mebsuper",
+                        FirstName = "super",
+                        LastName = "admin",
+                        PasswordHash = passwordHash,
+                        PasswordSalt = passwordSalt,
+                        Email = "user1@abc.com",
+                        Phone = "05558797012",
+                        Dept = "Management",
+                        Title="General Manager",
+                        IsActive = true,
+                        IsAdmin = true,
+                        CreatedDate = DateTime.Today,
+                        GroupName = "superadmin",
+                        ParentId=1
+                    }
+                    //Seed other Users here
+                });
+
+                context.SaveChanges();
+            }
+            // Seed other tables data here
+
+           
+            
+
+        }
+    }
+
+}
+
