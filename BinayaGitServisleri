diff --git a/MebCbsAnka/Controllers/GroupsController.cs b/MebCbsAnka/Controllers/GroupsController.cs
new file mode 100644
index 0000000000000000000000000000000000000000..1af884094f7bd96e579df3e0eb76a1c0b16cda13
--- /dev/null
+++ b/MebCbsAnka/Controllers/GroupsController.cs
@@ -0,0 +1,68 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.Entities;
+using Microsoft.AspNetCore.Http;
+using Microsoft.AspNetCore.Mvc;
+
+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/Groups")]
+    public class GroupsController : Controller
+    {
+        private IGroupDal _groupDal;
+
+        // Dependency Injection
+        public GroupsController(IGroupDal groupDal)
+        {
+            _groupDal = groupDal;
+        }
+
+        [HttpGet("")]
+        public IActionResult Get()
+        {
+            var groups = _groupDal.GetList();
+            return Ok(groups);
+        }
+
+        [HttpGet("{groupId}")]
+        public IActionResult Get(int groupId)
+        {
+            var group = _groupDal.Get(p => p.Id == groupId);
+            if (group == null)
+            {
+                return NotFound($"There is no role with Id= {groupId}");
+            }
+            return Ok(group);
+        }
+
+       
+        // NOTE* If you want to read the data from JSON/Text you need to add [from body]
+        [HttpPost("")]
+        public IActionResult Post([FromBody] Group group)
+        {
+            _groupDal.Add(group);
+            return new StatusCodeResult(201);
+        }
+
+       
+        [HttpPut("")]
+        public IActionResult Put([FromBody] Group group)
+        {
+            _groupDal.Update(group);
+            return Ok(group);
+        }
+
+       
+        [HttpDelete("{groupId}")]
+        public IActionResult Delete(int groupId)
+        {
+            _groupDal.Delete(new Group { Id = groupId });
+            return Ok();
+        }
+    }
+}
+
diff --git a/MebCbsAnka/Controllers/OgrenciController.cs b/MebCbsAnka/Controllers/OgrenciController.cs
new file mode 100644
index 0000000000000000000000000000000000000000..2f6868d8a635240841cfa16fdc4d306f739e8189
--- /dev/null
+++ b/MebCbsAnka/Controllers/OgrenciController.cs
@@ -0,0 +1,358 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Net;
+using System.Net.Http;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.Dtos;
+using MebCbsAnka.Dtos.ArcgisDtos;
+using MebCbsAnka.Dtos.OgrenciDtos;
+using Microsoft.AspNetCore.Authorization;
+using Microsoft.AspNetCore.Http;
+using Microsoft.AspNetCore.Mvc;
+using Microsoft.Extensions.Configuration;
+using Newtonsoft.Json;
+using ServiceOgrenciSayisi;
+
+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/Ogrenci")]
+    public class OgrenciController : Controller
+    {
+        private IAuthRepository _authRepository;
+        private IConfiguration _configuration;
+        private static readonly DateTime epoch = new DateTime(1970, 1, 1, 0, 0, 0, DateTimeKind.Utc);
+        //Dependency Injection
+        public OgrenciController(IAuthRepository authRepository, IConfiguration configuration,
+            ILogRepository logRepository, IHttpContextAccessor accessor)
+        {
+            _authRepository = authRepository;
+            _configuration = configuration;
+        }
+
+        private async Task<ArcgisTokenDto> getToken()
+        {
+            string arcgisTokenURL = _configuration.GetSection("ArcgisRefererURL:ArcgisGenerateTokenUrl").Value;
+            string refererURL = _configuration.GetSection("ArcgisRefererURL:Local").Value;
+            string username = _configuration.GetSection("Credentials:Username").Value;
+            string password = _configuration.GetSection("Credentials:Password").Value;
+
+            HttpClient httpClient = new HttpClient();
+
+            var stringContent = new FormUrlEncodedContent(new[]
+            {
+                new KeyValuePair<string, string>("username", username),
+                new KeyValuePair<string, string>("password", password),
+                new KeyValuePair<string, string>("client", "referer"),
+                new KeyValuePair<string, string>("expiration", "1440"),
+                new KeyValuePair<string, string>("f", "json"),
+                new KeyValuePair<string, string>("referer", refererURL),
+                new KeyValuePair<string, string>("ip", ""),
+                new KeyValuePair<string, string>("encrypted", "false"),
+            });
+            var response = await httpClient.PostAsync(arcgisTokenURL, stringContent);
+            var content = await response.Content.ReadAsStringAsync();
+            var tokenObject = JsonConvert.DeserializeObject<ArcgisTokenDto>(content);
+
+            return tokenObject;
+        }
+
+        [HttpGet("updateKurumInfo")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task UpdateKurumInfo()
+        {
+            HttpClient httpClient = new HttpClient();
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            string ArcgisCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            string kurumQueryUrl = ArcgisCbsUrl + "FeatureServer/0/query";
+            string kurumUpdateFeatureUrl = ArcgisCbsUrl + "FeatureServer/0/updateFeatures";
+
+            string where = "kurum_kodu IS NOT NULL and bagli_kurum_kodu IS NOT NULL";
+
+            decimal totalKurumCount = await getKurumCountFromArcgis(arcgisTokenDto.token, kurumQueryUrl, where);
+            decimal requestCount = Math.Ceiling(totalKurumCount / 1000);
+
+            for (int i = 0; i < requestCount; i++)
+            {
+                // TODO change
+                int resultOffsetCount = i * 1000;
+
+                ArcgisKurumKodObject kurumKodList = await getKurumKodlari(kurumQueryUrl, arcgisTokenDto.token, resultOffsetCount, where);
+
+                List<SavedKurumObject> kurumListesi = new List<SavedKurumObject>();
+
+                foreach(var kod in kurumKodList.features)
+                {
+                    int kurum_kodu = kod.attributes.kurum_kodu;
+                    int bagli_kurum_kodu = kod.attributes.bagli_kurum_kodu;
+                    string kurum_adi = kod.attributes.kurum_adi;
+                    Int32 objectid = kod.attributes.objectid;
+
+                    SavedKurumObject kurumKodObject = new SavedKurumObject();
+
+                    if (kurum_kodu != 0)
+                    {
+                        InstitutionInfoDto institutionInfo = await getKurumInfo(kurum_kodu);
+
+                        SavedKurumKodAtt attributes = new SavedKurumKodAtt();
+
+                        if (institutionInfo != null)
+                        {
+                            attributes.objectid = objectid;
+                            attributes.kurum_adi = institutionInfo.kurum_adi;
+                            attributes.ogrenci_sayisi = institutionInfo.ogrenci_sayisi;
+                            attributes.sube_sayisi = institutionInfo.sube_sayisi;
+                            attributes.egitim_sekli = institutionInfo.egitim_sekli;
+                        }
+                        else
+                        {
+                            attributes.objectid = objectid;
+                            attributes.kurum_adi = kurum_adi;
+                            attributes.ogrenci_sayisi = 0;
+                            attributes.sube_sayisi = 0;
+                            attributes.egitim_sekli = 3;
+                                                        
+                        }
+
+                        kurumKodObject.attributes = attributes;
+
+                        kurumListesi.Add(kurumKodObject);
+
+                    }
+
+                    if (bagli_kurum_kodu != 0)
+                    {
+                        InstitutionInfoDto institutionInfo = await getKurumInfo(kurum_kodu);
+
+                        SavedKurumKodAtt attributes = new SavedKurumKodAtt();
+
+                        if (institutionInfo != null)
+                        {
+                            attributes.kurum_adi = institutionInfo.kurum_adi;
+                            attributes.ogrenci_sayisi = 0;
+                            attributes.sube_sayisi = 0;
+                            attributes.egitim_sekli = institutionInfo.egitim_sekli;
+                            attributes.objectid = objectid;
+                        }
+                        else
+                        {
+                            attributes.kurum_adi = kurum_adi;
+                            attributes.ogrenci_sayisi = 0;
+                            attributes.sube_sayisi = 0;
+                            attributes.egitim_sekli = 3;
+                            attributes.objectid = objectid;
+                        }
+
+                        kurumKodObject.attributes = attributes;
+
+                        kurumListesi.Add(kurumKodObject);
+                    }
+                }
+
+                await SaveUpdatedKurumInfo(kurumListesi, kurumUpdateFeatureUrl, arcgisTokenDto.token);
+            }
+
+        }
+
+        private async Task SaveUpdatedKurumInfo(List<SavedKurumObject> kurumListesi, string kurumAddFeatureUrl, string token)
+        {
+            HttpClient httpClient = new HttpClient();
+
+            var serializedObject = JsonConvert.SerializeObject(kurumListesi);
+
+            var requestContent = new[]
+            {
+                new KeyValuePair<string, string>("f", "pjson"),
+                new KeyValuePair<string, string>("features", serializedObject),
+                new KeyValuePair<string, string>("token", token)
+            };
+
+            var encodedItems = requestContent.Select(j => System.Net.WebUtility.UrlEncode(j.Key) + "=" + WebUtility.UrlEncode(j.Value));
+            var encodedContent = new StringContent(String.Join("&", encodedItems), null, "application/x-www-form-urlencoded");
+
+            var response = await httpClient.PostAsync(kurumAddFeatureUrl, encodedContent);
+            var content = await response.Content.ReadAsStringAsync();
+            var resultObject = JsonConvert.DeserializeObject(content);
+        }
+
+        private async Task<ArcgisKurumKodObject> getKurumKodlari(string url, string token, int resultOffsetCount, string where)
+        {
+            HttpClient httpClient = new HttpClient();
+
+            var formUrlEncodedContent = new FormUrlEncodedContent(new[] {
+                new KeyValuePair<string, string>("where", where),
+                new KeyValuePair<string, string>("outFields", "objectid, kurum_kodu, bagli_kurum_kodu, kurum_adi"),
+                new KeyValuePair<string, string>("returnCountOnly", "false"),
+                new KeyValuePair<string, string>("returnGeometry", "false"),
+                new KeyValuePair<string, string>("resultOffsetCount", resultOffsetCount.ToString()),
+                new KeyValuePair<string, string>("resultRecordCount", "1000"),
+                new KeyValuePair<string, string>("f", "pjson"),
+                new KeyValuePair<string, string>("token", token),
+            });
+
+            var kurumKodlariResponse = await httpClient.PostAsync(url, formUrlEncodedContent);
+            var kurumKodlariContent = await kurumKodlariResponse.Content.ReadAsStringAsync();
+            var kurumKodlariObject = JsonConvert.DeserializeObject<ArcgisKurumKodObject>(kurumKodlariContent);
+
+            return kurumKodlariObject;
+        }
+
+        private async Task<decimal> getKurumCountFromArcgis(string token, string kurumUrl, string where)
+        {
+            HttpClient httpClient = new HttpClient();
+
+            var formUrlEncodedContent = new FormUrlEncodedContent(new[] {
+                new KeyValuePair<string, string>("where", where),
+                new KeyValuePair<string, string>("returnCountOnly", "true"),
+                new KeyValuePair<string, string>("f", "pjson"),
+                new KeyValuePair<string, string>("token", token)
+            });
+
+            var countResponse = await httpClient.PostAsync(kurumUrl, formUrlEncodedContent);
+            var countContent = await countResponse.Content.ReadAsStringAsync();
+            var countObject = JsonConvert.DeserializeObject<ArcgisCountResponse>(countContent);
+
+            decimal totalCount = countObject.count;
+
+            return totalCount;
+        }
+
+        [HttpGet("getKurumInfoByKurumKodu")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<InstitutionInfoDto> GetKurumInfoByKurumKodu(int kurum_kodu)
+        {
+            InstitutionInfoDto institutionInfo = await getKurumInfo(kurum_kodu);
+
+            return institutionInfo;
+        }
+
+        private async Task<InstitutionInfoDto> getKurumInfo(int kurum_kodu)
+        {
+            try
+            {
+                string username = _configuration.GetSection("Credentials:OgrenciServisiUsername").Value;
+                string password = _configuration.GetSection("Credentials:OgrenciServisiPassword").Value;
+
+                int totalCount = 0;
+
+                IService service = new ServiceClient();
+
+                OgrenciSayiSorgulaRequest request = new OgrenciSayiSorgulaRequest();
+
+                request.UygulamaBilgileri = new UygulamaBilgileri()
+                {
+                    KullaniciAdi = username,
+                    KullaniciSifre = password,
+                    UygulamaNo = 5478
+                };
+
+                request.OkulKodu = kurum_kodu;
+
+                OgrenciSayiSorgulaResponse response = await service.OgrenciSayiSorgulaAsync(request);
+                Console.WriteLine(response);
+                if (response.Basarili)
+
+                {
+                    Sayi kurumInfo = response.Sayi;
+                    string egitim_sekli = kurumInfo.OgretimSekli;
+                    int egitim_sekli_int = 0;
+
+                    totalCount = int.Parse(kurumInfo.ErkekOgrenci) + int.Parse(kurumInfo.KizOgrenci);
+
+                    switch(egitim_sekli)
+                    {
+                        case "NORMAL":
+                            egitim_sekli_int = 1;
+                            break;
+                        case "İKİLİ":
+                            egitim_sekli_int = 2;
+                            break;
+                    }
+
+                    InstitutionInfoDto institutionInfo = new InstitutionInfoDto()
+                    {
+                        ogrenci_sayisi = totalCount,
+                        kurum_adi = kurumInfo.KurumAdi,
+                        sube_sayisi = int.Parse(kurumInfo.SubeSayisi),
+                        egitim_sekli = egitim_sekli_int
+                    };
+
+                    return institutionInfo;
+
+                }
+                else
+                {
+                    Console.WriteLine(response.Aciklama);
+
+                    return null;
+                }
+
+            }
+
+            catch (Exception ex)
+            {
+                return null;
+            }
+
+        }
+
+        [HttpGet("getOgrenciSayisiByKurumKodu")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<int> GetStudentCountOfKurumByKurumKodu(int kurum_kodu)
+        {
+            int ogrenci_sayisi = await getOgrenciCount(kurum_kodu);
+
+            return ogrenci_sayisi;
+        }
+
+        private async Task<int> getOgrenciCount(int kurum_kodu)
+        {
+            try
+            {
+                string username = _configuration.GetSection("Credentials:OgrenciServisiUsername").Value;
+                string password = _configuration.GetSection("Credentials:OgrenciServisiPassword").Value;
+
+                int totalCount = 0;
+
+                IService service = new ServiceClient();
+
+                OgrenciSayiSorgulaRequest request = new OgrenciSayiSorgulaRequest();
+
+                request.UygulamaBilgileri = new UygulamaBilgileri() {
+                    KullaniciAdi = username,
+                    KullaniciSifre = password,
+                    UygulamaNo = 5478
+                };
+
+                request.OkulKodu = kurum_kodu;
+
+                OgrenciSayiSorgulaResponse response = await service.OgrenciSayiSorgulaAsync(request);
+                Console.WriteLine(response);
+                if (response.Basarili)
+
+                {
+                    Sayi kizerkek = response.Sayi;
+                    totalCount = int.Parse(kizerkek.ErkekOgrenci) + int.Parse(kizerkek.KizOgrenci);
+                    return totalCount;
+
+                }
+                else
+                {
+                    Console.WriteLine(response.Aciklama);
+
+                    return 0;
+                }
+
+            }
+
+            catch (Exception ex)
+            {
+                return 0;
+            }
+
+        }
+    }
+}
