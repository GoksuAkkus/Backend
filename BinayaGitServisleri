diff --git a/MebCbsAnka/Startup.cs b/MebCbsAnka/Startup.cs
new file mode 100644
index 0000000000000000000000000000000000000000..7c2b25e69b017909b0c002284374791c02f68e5a
--- /dev/null
+++ b/MebCbsAnka/Startup.cs
@@ -0,0 +1,149 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Text;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.DataAccess.Concrete;
+using MebCbsAnka.Middlewares;
+using Microsoft.AspNetCore.Authentication.JwtBearer;
+using Microsoft.AspNetCore.Builder;
+using Microsoft.AspNetCore.Hosting;
+using Microsoft.AspNetCore.Http;
+using Microsoft.AspNetCore.HttpOverrides;
+using Microsoft.EntityFrameworkCore;
+using Microsoft.Extensions.Configuration;
+using Microsoft.Extensions.DependencyInjection;
+using Microsoft.Extensions.Logging;
+using Microsoft.Extensions.Options;
+using Microsoft.IdentityModel.Tokens;
+
+namespace MebCbsAnka
+{
+    public class Startup
+    {
+        public Startup(IConfiguration configuration)
+        {
+            Configuration = configuration;
+        }
+
+        public IConfiguration Configuration { get; }
+
+        // This method gets called by the runtime. Use this method to add services to the container.
+        public void ConfigureServices(IServiceCollection services)
+        {
+            //Key
+            var key = Encoding.ASCII.GetBytes(Configuration.GetSection("AppSettings:Token").Value);
+
+            services.AddDbContext<MebCbsAnkaContext>(opt =>
+                opt.UseNpgsql(Configuration.GetConnectionString("MebCbsConection")));
+
+            // Postgres initialization
+            services.AddEntityFrameworkNpgsql().AddDbContext<MebCbsAnkaContext>(opt =>
+                opt.UseNpgsql(Configuration.GetConnectionString("MebCbsConection")));
+
+            services.AddEntityFrameworkNpgsql().AddDbContext<LoggerDbContext>(opt =>
+                opt.UseNpgsql(Configuration.GetConnectionString("MebCbsLogger")));
+
+            /*     // For MSSQLSERVER
+                 services.AddDbContext<DataContext>(x =>
+                     x.UseSqlServer(Configuration.GetConnectionString("DefaultConnection")));   */
+
+            // Added Cors
+            services.AddCors(options =>
+            {
+                options.AddPolicy("AllowAll",
+                    builder =>
+                    {
+                        builder
+                            .AllowAnyOrigin()
+                            .AllowAnyMethod()
+                            .AllowAnyHeader()
+                            .AllowCredentials();
+                    });
+            });
+
+            services.AddMvc().AddXmlSerializerFormatters();
+            // Forwarded Header
+            services.Configure<ForwardedHeadersOptions>(options =>
+                {
+                    options.ForwardedHeaders = 
+                        ForwardedHeaders.XForwardedFor | ForwardedHeaders.XForwardedProto;
+                });
+            
+            // Accesssor Service
+            services.AddSingleton<IHttpContextAccessor, HttpContextAccessor>();
+
+            // Mappings Scopes
+            // In case a controller request for IAppRepository Return AppRepository
+            services.AddScoped<IAuthRepository, AuthRepository>();
+            services.AddScoped<IGroupDal, EfGroupDal>();
+            services.AddScoped<IPermLayerDal, EfPermLayerDal>();
+            services.AddScoped<IPermGUIDal, EfPermGUIDal>();
+            services.AddScoped<IPermZoneDal, EfPermZoneDal>();
+            services.AddScoped<ILogRepository, LogRepository>();
+
+            // Jwt Authentication
+            services.AddAuthentication(JwtBearerDefaults.AuthenticationScheme).AddJwtBearer(options =>
+            {
+                options.TokenValidationParameters = new TokenValidationParameters
+                {
+                    ValidateIssuerSigningKey = true,
+                    IssuerSigningKey = new SymmetricSecurityKey(key),
+                    ValidateIssuer = false,
+                    ValidateActor = false,
+                };
+            });
+
+            // # SwashBuckle.Asp.netCore
+            services.AddSwaggerGen(c =>
+            {
+                c.SwaggerDoc("v1", new Swashbuckle.AspNetCore.Swagger.Info
+                {
+                    Title = "ASP.net Core API",
+                    Description = "Swagger Core API"
+                });
+            });
+        }
+
+        // This method gets called by the runtime. Use this method to configure the HTTP request pipeline.
+        public void Configure(IApplicationBuilder app, IHostingEnvironment env)
+        {
+            app.UseForwardedHeaders();
+            if (env.IsDevelopment())
+            {
+                app.UseDeveloperExceptionPage();
+            }
+
+            // Authorization Middleware
+            app.UseMiddleware<AuthenticationMiddleware>();
+            //   app.UseMiddleware<AuthMiddleware>();
+
+            //// Get instance of the context class
+            using (var scope = app.ApplicationServices.GetRequiredService<IServiceScopeFactory>().CreateScope())
+            {
+                var context = scope.ServiceProvider.GetService<MebCbsAnkaContext>();
+                // var loggerContext = scope.ServiceProvider.GetService<LoggerDbContext>();
+                // Ensure database is upgraded with lasted migration
+                context.Database.Migrate();
+                context.EnsureDatabaseSeeded();
+                //    loggerContext.Database.Migrate();
+            }
+            // Cors Middleware
+            app.UseCors(x => x.AllowAnyHeader().AllowAnyMethod().AllowAnyOrigin().AllowCredentials());
+
+            app.UseMvc();
+
+            app.UseAuthentication();
+
+
+            // for swagger documentation
+            app.UseSwagger();
+            app.UseSwaggerUI(c =>
+            {
+                c.SwaggerEndpoint("/swagger/v1/swagger.json", "ASP.net Core API");
+            });
+        }
+    }
+}
