+        [HttpGet("getMaksGeometryFromNumarataj")]
+        public async Task<MaksGeriDonusDegerleri> getMaksGeometryFromNumarataj(int? kimlikNo)
+        {
+            MaksGeriDonusDegerleri maksResult = new MaksGeriDonusDegerleri();
+
+            string yapiKimlikNoString = await getMaksNumaratajInfoWithKimlikNo(kimlikNo);
+
+            if (yapiKimlikNoString == "" || yapiKimlikNoString == null)
+            {
+                maksResult.error.code = 0;
+                maksResult.error.description = "Kimlik No: " + kimlikNo + " Olan Verinin  Yapı Kimlik Nosu Yok!. ";
+
+            }
+            else
+            {
+                int? yapiKimlikNo = int.Parse(yapiKimlikNoString);
+
+                XmlDocument maksYapiInfo;
+
+                maksYapiInfo = await getMaksYapiInfoWithBinaNo(yapiKimlikNo);
+
+                if (maksYapiInfo != null)
+                {
+                    XmlNode lastChildOfCollection = maksYapiInfo.LastChild; // featureCollection
+
+                    if (lastChildOfCollection.ChildNodes.Count == 0)
+                    {
+                        maksResult.error.code = 1;
+                        maksResult.error.description = "Maks Tarafinda Yapı Kimlik No: " + yapiKimlikNo + " / Kimlik No: " + kimlikNo + " Olan Verinin Karsiligi Bulunamadi. ";
+                    }
+                    foreach (XmlNode feature in lastChildOfCollection.ChildNodes) //featureMembers
+                    {
+                        XmlNode featureAttributes = feature.FirstChild;
+
+                        var maksGeom = featureAttributes["maks:geometri"] != null ? featureAttributes["maks:geometri"].InnerText : "";
+
+                        if (maksGeom == "" || maksGeom == null)
+                        {
+                            maksResult.error.code = 2;
+                            maksResult.error.description = "Maks Tarafinda Yapı Kimlik No: " + yapiKimlikNo + " / Kimlik No: " + kimlikNo + " Olan Verinin Geometrisi Yok. ";
+                        }
+                        else
+                        {
+                            var geomForNoComma = createRingsForNoComma(maksGeom);
+                            maksResult.geometry.rings = geomForNoComma;
+                        }
+                    }
+                }
+                else
+                {
+                    maksResult.error.code = 3;
+                    maksResult.error.description = "Yapı Kimlik No: " + yapiKimlikNo + " / Kimlik No: " + kimlikNo + " Makstan Sonuç Dönmedi. ";
+                }
+            }
+
+            return maksResult;
+        }
+
+        //BUNA GEREK KALMADI
+        [HttpGet("MaksBinaKoduSorgula")]
+        public async Task<MaksGeriDonusDegerleri> MaksBinaKoduSorgula(int csbmKodu)
+        {
+            MaksGeriDonusDegerleri maksResult = new MaksGeriDonusDegerleri();
+
+            var giris = new ServiceMernis.KullaniciGiris();
+            giris.UserName = "mebcbs";
+            giris.Password = "9UFdAy7x";
+
+            var logClient = new ServiceMernis.LogClient();
+            logClient.EkranKodu = "1";
+            logClient.Aciklama = "MebCbsAnka";
+            logClient.ModulKodu = "1";
+
+            logClient.YetkiliIPAdresi = "10.30.1.244";
+            logClient.YetkiliKullaniciAdi = "1234567910";
+            logClient.YetkiliKullaniciNo = 1234567910;
+            logClient.YetkiliTCKimlikNo = 44257273824;
+
+            var kriter = new ServiceMernis.AksOkuTumCsbmyeBagliBinaSorguKriter();
+            kriter.CsbmKodu = csbmKodu;
+
+            var client = new ServiceMernis.AksOkuTumCsbmyeBagliBinaServisClient();
+            ServiceMernis.BinaSonucu response = await client.CsbmyeBagliBinaListeleAsync(kriter, giris, logClient);
+
+            if (response != null)
+            {
+                ServiceMernis.Bina[] sorguSonucu = response.SorguSonucu;
+
+                if (sorguSonucu != null)
+                {
+                    maksResult.binalar = sorguSonucu;
+
+                }
+                else
+                {
+                    maksResult.error.code = 1;
+                    maksResult.error.description = "HATA CODE: #" + maksResult.error.code + " Sorgu Sonucu Null.";
+                }
+
+                return maksResult;
+
+            }
+            else
+            {
+                maksResult.error.code = 0;
+                maksResult.error.description = "HATA CODE: #" + maksResult.error.code + " CsbmyeBagliBinaListeleAsync Sonuç Vermedi.";
+                return maksResult;
+
+            }
+        }
