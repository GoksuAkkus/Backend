+        [HttpGet("createRings")]
+        public List<List<double[]>> createRings(string geom)
+        {
+            List<List<double[]>> ringList = new List<List<double[]>>();
+            geom = geom.Trim();
+            string[] rings = geom.Split(" ");
+            List<double[]> data = new List<double[]>();
+            foreach (string ring in rings)
+            {
+                ring.Trim();
+                string[] coord = ring.Split(',');
+
+                data.Add(new double[] { Convert.ToDouble(coord[0], CultureInfo.InvariantCulture), Convert.ToDouble(coord[1], CultureInfo.InvariantCulture) });
+            }
+            ringList.Add(data);
+            return ringList;
+        }
+        [HttpGet("createRingsForNoComma")]
+        public List<List<double[]>> createRingsForNoComma(string geom)
+        {
+            List<List<double[]>> ringList = new List<List<double[]>>();
+            geom = geom.Trim();
+            string[] rings = geom.Split(" ");
+            int incorrectRingCount = 0;
+            List<double[]> data = new List<double[]>();
+            for (var i = 0; i < rings.Length; i++)
+            {
+                int dotCount = 0;
+                dotCount = rings[i].ToCharArray().Count(dot => dot == '.');
+                if (dotCount > 1) //1 ringdeki . sayısı 1den fazla ise geometri hatalı demektir.
+                {
+                    incorrectRingCount++;
+                }
+            }
+            if (incorrectRingCount == 0) // yanlış ring yoksa buraya girebilir.
+            {
+                for (var i = 0; i < rings.Length; i += 2)
+                {
+                    data.Add(new double[] { Convert.ToDouble(rings[i], CultureInfo.InvariantCulture), Convert.ToDouble(rings[i + 1], CultureInfo.InvariantCulture) });
+                }
+
+                ringList.Add(data);
+            }
+
+            return ringList;
+        }
+
+        [HttpGet("write")]
+        public bool writeDeneme()
+        {
+            string path = @"C:/maksyapilogs2.txt";
+            // FileStream fs = new FileStream(path, FileMode.Append, FileAccess.ReadWrite);
+            using (StreamWriter sw = System.IO.File.AppendText(path))
+            {
+                sw.WriteLine("deneme");
+            }
+
+            using (StreamWriter sw = System.IO.File.AppendText(path))
+            {
+                sw.WriteLine("deneme2");
+            }
+            return true;
+        }
