diff --git a/MebCbsAnka/Middlewares/AuthMiddleware.cs b/MebCbsAnka/Middlewares/AuthMiddleware.cs
new file mode 100644
index 0000000000000000000000000000000000000000..ef5f074efff30ba7259fc21eb00d2872fa751ca3
--- /dev/null
+++ b/MebCbsAnka/Middlewares/AuthMiddleware.cs
@@ -0,0 +1,25 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+using Microsoft.AspNetCore.Http;
+
+namespace MebCbsAnka.Middlewares
+{
+    public class AuthMiddleware
+    {
+        private readonly RequestDelegate _next;
+
+        public AuthMiddleware(RequestDelegate next)
+        {
+            _next = next;
+        }
+
+        public async Task Invoke(HttpContext context)
+        {
+            var authHeader = context.Request.Headers["Authorization"];
+
+            await _next(context);
+        }
+    }
+}
