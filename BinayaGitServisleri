+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/Cbs")]
+    public class CbsController : Controller
+    {
+        private IAuthRepository _authRepository;
+        private IConfiguration _configuration;
+        private IPermLayerDal _permLayerDal;
+        private IPermGUIDal _permGUIDal;
+        private IPermZoneDal _permZoneDal;
+        private ILogRepository _logRepository;
+        private IHttpContextAccessor _accessor;
+        private static readonly DateTime epoch = new DateTime(1970, 1, 1, 0, 0, 0, DateTimeKind.Utc);
+        //Dependency Injection
+        public CbsController(IAuthRepository authRepository, IConfiguration configuration, IPermZoneDal permZoneDal,
+            IPermLayerDal permLayerDal, IPermGUIDal permGUIDal, ILogRepository logRepository, IHttpContextAccessor accessor)
+        {
+            _authRepository = authRepository;
+            _configuration = configuration;
+            _permLayerDal = permLayerDal;
+            _permGUIDal = permGUIDal;
+            _permZoneDal = permZoneDal;
+            _logRepository = logRepository;
+            _accessor = accessor;
+        }
+        [HttpGet("me")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> me()
+        {
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            var subUserList = _authRepository.GetGroupById(userId, 1).SubUsers.Select(u => u.UserName).ToList();
+
+            var permLayer = _permLayerDal.GetList(x => x.UserId == userId).Select(x => new PermLayerDto()
+            {
+                Field = x.Field,
+                Layer = x.Layer,
+                Permission = x.Permission,
+            }).ToList();
+            var permGUI = _permGUIDal.GetList(x => x.UserId == userId).Select(x => new PermGUIDto()
+            {
+                Item = x.Item,
+                Menu = x.Menu,
+            }).ToList();
+
+            var permZone = _permZoneDal.GetList(x => x.UserId == userId).Select(x => new PermZoneDto()
+            {
+                Zone = x.Zone
+            }).ToList();
+
+            var permDto = new UserPermissionDto()
+            {
+                permGUI = permGUI,
+                PermLayer = permLayer,
+                PermZone = permZone,
+            };
+
+            if (subUserList.Count() > 0)
+            {
+                var role = userClaimList[2].ToString().Split(": ")[1];
+                // var groupAdmin = _authRepository.GetUserGroupInfo(userId, 1);
+                if (role == "Admin")
+                {
+                    permDto.permUsers = subUserList;
+                }
+            }
+            return Ok(permDto);
+        }
+
+        [HttpPost("login")]
+        public async Task<IActionResult> Login([FromBody] UserForLoginDto userForLoginDto)
+        {
+            var foundUser = await _authRepository.GetUserWithUsername(userForLoginDto.UserName);
+
+            var userIp = _accessor.HttpContext.Connection.RemoteIpAddress.ToString();
+
+            Log logToCreate;
+
+            if (foundUser.tryLoginCount == 3)
+            {
+
+                logToCreate = new Log
+                {
+                    CreatedDate = DateTime.Now,
+                    Description = userForLoginDto.UserName + " " + userIp + "CBS - Basarsiz Giriş",
+                    Action = "CBS Basarsiz Giriş",
+                    UserName = userForLoginDto.UserName
+                };
+                _logRepository.Add(logToCreate);
+
+                UserTryLoginCountDto userTryLoginCount = new UserTryLoginCountDto()
+                {
+                    tryLoginCount = foundUser.tryLoginCount
+                };
+                return Ok(userTryLoginCount);
+            }
+
+            var user = await _authRepository.Login(userForLoginDto.UserName, userForLoginDto.Password);
+
+            if (user == null)
+            {
+                _authRepository.UpdateUserLoginTryCount(userForLoginDto.UserName);
+
+                logToCreate = new Log
+                {
+                    CreatedDate = DateTime.Now,
+                    Description = userForLoginDto.UserName + " " + userIp + "CBS - Basarsiz Giriş",
+                    Action = "CBS Basarsiz Giriş",
+                    UserName = userForLoginDto.UserName
+                };
+                _logRepository.Add(logToCreate);
+
+                return Unauthorized();
+            }
+            // User available return JWT Token 
+            var tokenHandler = new JwtSecurityTokenHandler();
+            // Generate token with private key in appsetting.json
+            var key = Encoding.ASCII.GetBytes(_configuration.GetSection("AppSettings:Token").Value);
+            // User Authorization Mechanism For Controlling
+            string role;
+            if (user.IsAdmin == true && user.ParentId == 0)
+                role = "SuperAdmin";
+            else if (user.IsAdmin == true && user.ParentId != 0)
+                role = "Admin";
+            else
+                role = "User";
+
+            var tokenDescriptor = new SecurityTokenDescriptor()
+            {
+                Subject = new ClaimsIdentity(new Claim[]
+                {
+                    new Claim(ClaimTypes.NameIdentifier, user.Id.ToString()),
+                    new Claim("group",user.GroupName),
+                    new Claim(ClaimTypes.Name, user.UserName),
+                    new Claim(ClaimTypes.Role, role),
+                    new Claim("parentId",user.ParentId.ToString()),
+                    new Claim("isModerator", user.IsModerator.ToString()),
+                    new Claim("moderatorRequired", user.ModeratorRequired.ToString())
+                }),
+                // Set token validity
+                Expires = DateTime.Now.AddDays(1),
+                SigningCredentials = new SigningCredentials(new SymmetricSecurityKey(key), SecurityAlgorithms.HmacSha512Signature)
+            };
+            // Creating Token according to tokenDescriptor
+            var token = tokenHandler.CreateToken(tokenDescriptor);
+            var tokenString = tokenHandler.WriteToken(token);
+
+            var tokenObject = await getToken();
+
+            LoginDto loginDto = new LoginDto()
+            {
+                arcgisData = tokenObject,
+                token = tokenString,
+            };
+            logToCreate = new Log
+            {
+                CreatedDate = DateTime.Now,
+                Description = user.UserName + " " + userIp + "CBS - Giriş Yapıldı",
+                Action = "CBS Giris",
+                UserId = user.Id,
+                UserName = user.UserName
+            };
+            _logRepository.Add(logToCreate);
+
+            // Signing In user and give token
+            return Ok(loginDto);
+        }
+
+        [HttpPost("exportExcel")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> ExportExcel([FromBody] ExportDataDto exportDataDto)
+        {
+            using (var package = new ExcelPackage())
+            {
+                var offsetValue = 0;
+                var columnHeader = exportDataDto.headers;
+                var workSheetName = exportDataDto.workSheetName;
+                var URI = exportDataDto.uri;
+                string arcgisUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+
+                var worksheet = package.Workbook.Worksheets.Add(workSheetName);
+
+                using (var cells = worksheet.Cells[1, 1, 1, columnHeader.Count])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                for (int i = 0; i < columnHeader.Count(); i++)
+                {
+                    worksheet.Cells[1, i + 1].Value = columnHeader[i];
+                }
+
+                var tokenObject = await getToken();
+
+                HttpClient httpClient = new HttpClient();
+                Regex regex = new Regex(@"\/[0-9]+\/", RegexOptions.Compiled);
+
+                Match matchURI = regex.Match(URI);
+                var index = matchURI.Captures[0].Index + matchURI.Captures[0].Length - 1;
+                var serviceAddress = URI.Substring(0, index);
+                var requestURI = serviceAddress + "?f=json&token=" + tokenObject.token;
+
+                var response = await httpClient.GetAsync(requestURI);
+                var content = await response.Content.ReadAsStringAsync();
+                var arcgisServiceData = JObject.Parse(content);
+                var fieldData = arcgisServiceData["fields"];
+
+                if (exportDataDto.isIslem)
+                {
+                    string mebArcgisUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+                    string islemTableUrl = mebArcgisUrl + "FeatureServer/14?f=pjson&token=" + tokenObject.token;
+                    string hareketTableUrl = mebArcgisUrl + "FeatureServer/13?f=pjson&token=" + tokenObject.token;
+
+                    var islemResponse = await httpClient.GetAsync(islemTableUrl);
+                    var islemContent = await islemResponse.Content.ReadAsStringAsync();
+                    var islemServiceData = JObject.Parse(islemContent);
+                    var islemFields = islemServiceData["fields"];
+
+                    var hareketResponse = await httpClient.GetAsync(hareketTableUrl);
+                    var hareketContent = await hareketResponse.Content.ReadAsStringAsync();
+                    var hareketServiceData = JObject.Parse(hareketContent);
+                    var hareketFields = hareketServiceData["fields"];
+
+                    foreach (var islemField in islemFields)
+                    {
+                        foreach (var field in fieldData)
+                        {
+                            if (("i_" + islemField.Value<string>("name")) == field.Value<string>("name"))
+                            {
+                                field["domain"] = islemField["domain"];
+                            }
+
+                            if ((islemField.Value<string>("name") == "durumu") && field.Value<string>("name") == "h_durumu")
+                            {
+                                foreach (var hareketField in hareketFields)
+                                {
+                                    if (hareketField.Value<string>("name") == "durumu")
+                                    {
+                                        field["domain"] = hareketField["domain"];
+                                    }
+                                }
+                            }
+                        }
+                    }
+                }
+
+                int featureCount = 1;
+                URI = URI + "&token=" + tokenObject.token;
+
+                while (featureCount != 0)
+                {
+                    response = await httpClient.GetAsync(URI);
+                    content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+
+                    var features = arcgisResponseData["features"].ToArray();
+                    featureCount = features.Count();
+                    for (int i = 0; i < features.Count(); i++)
+                    {
+                        var feature = arcgisResponseData["features"][i];
+                        var featureAttributes = feature["attributes"].ToArray();
+
+                        for (int j = 0; j < featureAttributes.Count(); j++)
+                        {
+                            var cellRow = offsetValue + i + 2;
+                            var cellColumn = j + 1;
+                            var attribute = featureAttributes[j];
+                            JProperty jProperty = attribute.ToObject<JProperty>();
+                            for (int k = 0; j < fieldData.Count(); k++)
+                            {
+                                var field = fieldData[k];
+                                if (field.Value<string>("name") == jProperty.Name)
+                                {
+                                    var domainData = field["domain"];
+                                    if (domainData.Count() != 0)
+                                    {
+                                        var codedValues = domainData["codedValues"];
+                                        foreach (var codedValue in codedValues)
+                                        {
+                                            try
+                                            {
+                                                if (codedValue["code"].Value<int>() == attribute.First.Value<int>())
+                                                {
+                                                    var name = codedValue["name"].Value<string>();
+                                                    worksheet.Cells[cellRow, cellColumn].Value = name;
+                                                    break;
+                                                }
+
+                                            }
+                                            catch (Exception ex)
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                                break;
+                                            }
+
+                                        }
+                                    }
+                                    else
+                                    {
+                                        var type = field["type"].Value<string>();
+                                        if (type == "esriFieldTypeDate")
+                                        {
+                                            var dateString = attribute.First.ToString();
+                                            if (dateString != "")
+                                            {
+                                                var date = epoch.AddMilliseconds(Convert.ToDouble(dateString));
+                                                worksheet.Cells[cellRow, cellColumn].Value = date.ToString("dd-MM-yyyy");
+                                            }
+                                            else
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                            }
+                                        }
+                                        else
+                                        {
+                                            worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                        }
+                                    }
+                                    break;
+                                }
+                            }
+                        }
+                    }
+                    Regex reg = new Regex(@"resultOffset=[0-9]{1,10}&", RegexOptions.Compiled);
+
+                    Match match = reg.Match(URI);
+                    var matchLength = match.Captures[0].Length;
+                    var matchIndex = match.Captures[0].Index;
+
+                    var startString = URI.Substring(0, matchIndex);
+                    var endString = URI.Substring(matchIndex + matchLength - 1);
+                    var offSetCount = match.Value.TrimEnd('&');
+
+                    var splittedText = offSetCount.Split('=');
+                    offsetValue = int.Parse(splittedText[1]) + 1000;
+
+                    URI = startString + "resultOffset=" + (offsetValue) + endString;
+                }
+
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+        }
+
+        [HttpPost("exportExcelDynamic")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> ExportExcelDynamic([FromBody] ExportDynamicDataDto exportDataDto)
+        {
+            using (var package = new ExcelPackage())
+            {
+                var offsetValue = 0;
+                var fields = exportDataDto.fields;
+                var workSheetName = exportDataDto.workSheetName;
+                var URI = exportDataDto.uri;
+                string arcgisUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+
+                var worksheet = package.Workbook.Worksheets.Add(workSheetName);
+
+                using (var cells = worksheet.Cells[1, 1, 1, fields.Count])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                for (int i = 0; i < fields.Count(); i++)
+                {
+                    worksheet.Cells[1, i + 1].Value = fields[i].alias;
+                }
+
+                var tokenObject = await getToken();
+
+                HttpClient httpClient = new HttpClient();
+
+                int featureCount = 1;
+                URI = URI + "&token=" + tokenObject.token;
+
+                while (featureCount != 0)
+                {
+                    var response = await httpClient.GetAsync(URI);
+                    var content = await response.Content.ReadAsStringAsync();
+                    var arcgisResponseData = JObject.Parse(content);
+
+                    var features = arcgisResponseData["features"].ToArray();
+                    featureCount = features.Count();
+                    for (int i = 0; i < features.Count(); i++)
+                    {
+                        var feature = arcgisResponseData["features"][i];
+                        var featureAttributes = feature["attributes"].ToArray();
+
+                        for (int j = 0; j < featureAttributes.Count(); j++)
+                        {
+                            var cellRow = offsetValue + i + 2;
+                            var cellColumn = j + 1;
+                            var attribute = featureAttributes[j];
+                            JProperty jProperty = attribute.ToObject<JProperty>();
+                            for (int k = 0; j < fields.Count(); k++)
+                            {
+                                var field = fields[k];
+                                if (field.name == jProperty.Name)
+                                {
+                                    var domainData = field.domain;
+                                    if (domainData != null)
+                                    {
+                                        var codedValues = domainData.codedValues;
+                                        foreach (var codedValue in codedValues)
+                                        {
+                                            try
+                                            {
+                                                if (codedValue.code == attribute.First.Value<int>())
+                                                {
+                                                    var name = codedValue.name;
+                                                    worksheet.Cells[cellRow, cellColumn].Value = name;
+                                                    break;
+                                                }
+
+                                            }
+                                            catch (Exception ex)
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                                break;
+                                            }
+
+                                        }
+                                    }
+                                    else
+                                    {
+                                        var type = field.type;
+                                        if (type == "esriFieldTypeDate")
+                                        {
+                                            var dateString = attribute.First.ToString();
+                                            if (dateString != "")
+                                            {
+                                                var date = epoch.AddMilliseconds(Convert.ToDouble(dateString));
+                                                worksheet.Cells[cellRow, cellColumn].Value = date.ToString("dd-MM-yyyy");
+                                            }
+                                            else
+                                            {
+                                                worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                            }
+                                        }
+                                        else
+                                        {
+                                            worksheet.Cells[cellRow, cellColumn].Value = attribute.First.ToString();
+                                        }
+                                    }
+                                    break;
+                                }
+                            }
+                        }
+                    }
+                    Regex reg = new Regex(@"resultOffset=[0-9]{1,10}&", RegexOptions.Compiled);
+
+                    Match match = reg.Match(URI);
+                    var matchLength = match.Captures[0].Length;
+                    var matchIndex = match.Captures[0].Index;
+
+                    var startString = URI.Substring(0, matchIndex);
+                    var endString = URI.Substring(matchIndex + matchLength - 1);
+                    var offSetCount = match.Value.TrimEnd('&');
+
+                    var splittedText = offSetCount.Split('=');
+                    offsetValue = int.Parse(splittedText[1]) + 1000;
+
+                    URI = startString + "resultOffset=" + (offsetValue) + endString;
+                }
+
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+        }
+
+        private async Task<ArcgisTokenDto> getToken()
+        {
+            string arcgisTokenURL = _configuration.GetSection("ArcgisRefererURL:ArcgisGenerateTokenUrl").Value;
+            string refererURL = _configuration.GetSection("ArcgisRefererURL:Local").Value;
+            string username = _configuration.GetSection("Credentials:Username").Value;
+            string password = _configuration.GetSection("Credentials:Password").Value;
+
+            HttpClient httpClient = new HttpClient();
+
+            var stringContent = new FormUrlEncodedContent(new[]
+            {
+                new KeyValuePair<string, string>("username", username),
+                new KeyValuePair<string, string>("password", password),
+                new KeyValuePair<string, string>("client", "referer"),
+                new KeyValuePair<string, string>("expiration", "1440"),
+                new KeyValuePair<string, string>("f", "json"),
+                new KeyValuePair<string, string>("referer", refererURL),
+                new KeyValuePair<string, string>("ip", ""),
+                new KeyValuePair<string, string>("encrypted", "false"),
+            });
+            var response = await httpClient.PostAsync(arcgisTokenURL, stringContent);
+            var content = await response.Content.ReadAsStringAsync();
+            var tokenObject = JsonConvert.DeserializeObject<ArcgisTokenDto>(content);
+
+            return tokenObject;
+        }
+
+        [HttpGet("getMapIstToken")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<ArcgisTokenDto> createMapIstToken()
+        {
+            string mapIstGenerateTokenUrl = _configuration.GetSection("OtherServiceRefererURL:MapIstGenerateTokenUrl").Value;
+            string username = _configuration.GetSection("Credentials:MapIstUsername").Value;
+            string password = _configuration.GetSection("Credentials:MapIstPassword").Value;
+
+            HttpClient httpClient = new HttpClient();
+
+            var stringContent = new FormUrlEncodedContent(new[]
+            {
+                new KeyValuePair<string, string>("username", username),
+                new KeyValuePair<string, string>("password", password),
+                new KeyValuePair<string, string>("client", "requestip"),
+                new KeyValuePair<string, string>("expiration", "1440"),
+                new KeyValuePair<string, string>("f", "json"),
+                new KeyValuePair<string, string>("Content-Type", "application/x-www-form-urlencoded"),
+
+            });
+            var response = await httpClient.PostAsync(mapIstGenerateTokenUrl, stringContent);
+            var content = await response.Content.ReadAsStringAsync();
+            var tokenObject = JsonConvert.DeserializeObject<ArcgisTokenDto>(content);
+
+            return tokenObject;
+        }
+
+        private static DateTime FromUnixTime(long unixTime)
+        {
+            return epoch.AddMilliseconds(unixTime);
+        }
+
+        private async Task<decimal> GetTotalRecordCountFromArcgis(string url, string query)
+        {
+            HttpClient httpClient = new HttpClient();
+
+            ArcgisTokenDto arcgisToken = await getToken();
+
+            var formUrlEncodedContent = new FormUrlEncodedContent(new[]
+            {
+                new KeyValuePair<string, string>("where", query),
+                new KeyValuePair<string, string>("returnCountOnly", "true"),
+                new KeyValuePair<string, string>("f", "pjson"),
+                new KeyValuePair<string, string>("token", arcgisToken.token)
+
+            });
+
+            var countResponse = await httpClient.PostAsync(url, formUrlEncodedContent);
+            var countContent = await countResponse.Content.ReadAsStringAsync();
+            var countObject = JsonConvert.DeserializeObject<ArcgisCountResponse>(countContent);
+
+            decimal totalCount = Math.Ceiling(((decimal)countObject.count / 1000));
+
+            return totalCount;
+        }
+
+        [HttpPost("createSayistayIcmalCetveli")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<IActionResult> CreateSayistayIcmalCetveli()
+        {
+            var mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            ArcgisTokenDto arcgisTokenDto = await getToken();
+
+            using (var package = new ExcelPackage())
+            {
+                string[] headers = new string[13] {"TAŞINMAZ CİNSİ", "GEÇEN YILDAN DEVREDEN ADED", "GEÇEN YILDAN DEVREDEN YÜZÖLÇÜM", "GEÇEN YILDAN DEVREDEN DEĞERİ", "YIL İÇİNDE EDİNİLEN ADED", "YIL İÇİNDE EDİNİLEN YÜZÖLÇÜM", "YIL İÇİNDE EDİNİLEN DEĞERİ",
+                    "YIL İÇİNDE ELDEN ÇIKARILAN ADED", "YIL İÇİNDE ELDEN ÇIKARILAN YÜZÖLÇÜM", "YIL İÇİNDE ELDEN ÇIKARILAN DEĞERİ", "GELECEK YILA DEVREDEN ADED", "GELECEK YILA DEVREDEN YÜZÖLÇÜM", "GELECEK YILA DEVREDEN DEĞERİ"};
+                var worksheetName = "TAPUDA KAYITLI OLAN TAŞINMAZLAR İÇİN İCMAL CETVELİ";
+
+                var worksheet = package.Workbook.Worksheets.Add(worksheetName);
+
+                using (var cells = worksheet.Cells[1, 1, 1, headers.Count()])
+                {
+                    cells.Style.Font.Bold = true;
+                }
+
+                worksheet.Cells[1, 1].Value = "TAPUDA KAYITLI OLAN TAŞINMAZLAR İÇİN İCMAL CETVELİ";
+                worksheet.Cells[2, 1].Value = "KAMU İDARESİ: MİLLİ EĞİTİM BAKANLIĞI";
+                worksheet.Cells[2, 2].Value = "TARİH:" + DateTime.Now.ToString("dd-MM-yyyy");
+                worksheet.Cells[3, 1].Value = "BİRİMİ: İNŞAAT EMLAK DAİRE BAŞKANLIĞI";
+
+                for (int i = 0; i < headers.Count(); i++)
+                {
+                    worksheet.Cells[4, i + 1].Value = headers[i];
+                }
+
+                SayistayIcmalObject egitimAlaniIcmal = await SetSayistayIcmalObject(1, "mevcut_kullanim=1", arcgisTokenDto.token);
+                SayistayIcmalObject egitimDisiAlanIcmal = await SetSayistayIcmalObject(3, "mevcut_kullanim <> 1 and mevcut_kullanim <> 2", arcgisTokenDto.token);
+
+                worksheet.Cells[5, 1].Value = egitimAlaniIcmal.tasinmaz_cinsi.ToString();
+                worksheet.Cells[5, 2].Value = egitimAlaniIcmal.gelen_aded.ToString();
+                worksheet.Cells[5, 3].Value = egitimAlaniIcmal.gelen_yuzolcum.ToString();
+                worksheet.Cells[5, 4].Value = egitimAlaniIcmal.gelen_deger.ToString();
+                worksheet.Cells[5, 5].Value = egitimAlaniIcmal.edinilen_aded.ToString();
+                worksheet.Cells[5, 6].Value = egitimAlaniIcmal.edinilen_yuzolcum.ToString();
+                worksheet.Cells[5, 7].Value = egitimAlaniIcmal.edinilen_deger.ToString();
+                worksheet.Cells[5, 8].Value = egitimAlaniIcmal.cikarilan_aded.ToString();
+                worksheet.Cells[5, 9].Value = egitimAlaniIcmal.cikarilan_yuzolcum.ToString();
+                worksheet.Cells[5, 10].Value = egitimAlaniIcmal.cikarilan_deger.ToString();
+                worksheet.Cells[5, 11].Value = egitimAlaniIcmal.devreden_aded.ToString();
+                worksheet.Cells[5, 12].Value = egitimAlaniIcmal.devreden_yuzolcum.ToString();
+                worksheet.Cells[5, 13].Value = egitimAlaniIcmal.devreden_deger.ToString();
+
+                worksheet.Cells[6, 1].Value = egitimDisiAlanIcmal.tasinmaz_cinsi.ToString();
+                worksheet.Cells[6, 2].Value = egitimDisiAlanIcmal.gelen_aded.ToString();
+                worksheet.Cells[6, 3].Value = egitimDisiAlanIcmal.gelen_yuzolcum.ToString();
+                worksheet.Cells[6, 4].Value = egitimDisiAlanIcmal.gelen_deger.ToString();
+                worksheet.Cells[6, 5].Value = egitimDisiAlanIcmal.edinilen_aded.ToString();
+                worksheet.Cells[6, 6].Value = egitimDisiAlanIcmal.edinilen_yuzolcum.ToString();
+                worksheet.Cells[6, 7].Value = egitimDisiAlanIcmal.edinilen_deger.ToString();
+                worksheet.Cells[6, 8].Value = egitimDisiAlanIcmal.cikarilan_aded.ToString();
+                worksheet.Cells[6, 9].Value = egitimDisiAlanIcmal.cikarilan_yuzolcum.ToString();
+                worksheet.Cells[6, 10].Value = egitimDisiAlanIcmal.cikarilan_deger.ToString();
+                worksheet.Cells[6, 11].Value = egitimDisiAlanIcmal.devreden_aded.ToString();
+                worksheet.Cells[6, 12].Value = egitimDisiAlanIcmal.devreden_yuzolcum.ToString();
+                worksheet.Cells[6, 13].Value = egitimDisiAlanIcmal.devreden_deger.ToString();
+
+                byte[] result = package.GetAsByteArray();
+                return File(result, "application/ms-excel", $"work.xlsx");
+            }
+        }
+
+        private async Task<SayistayIcmalObject> SetSayistayIcmalObject(int type, string query, string token)
+        {
+            string mebCbsUrl = _configuration.GetSection("ArcgisRefererURL:MebCbsArcgisUrl").Value;
+            string kadastroParselQueryURI = mebCbsUrl + "FeatureServer/6/query";
+
+            HttpClient httpClient = new HttpClient();
+
+            decimal count = await GetTotalRecordCountFromArcgis(kadastroParselQueryURI, query);
+
+            SayistayIcmalObject icmal = new SayistayIcmalObject()
+            {
+                tasinmaz_cinsi = type == 1 ? "ÜZERİNDE BİNA VE TESİS OLAN TAŞINMAZLAR" : "ARSALAR",
+                gelen_aded = 0,
+                gelen_deger = 0,
+                gelen_yuzolcum = 0,
+                edinilen_aded = 0,
+                edinilen_deger = 0,
+                edinilen_yuzolcum = 0,
+                cikarilan_aded = 0,
+                cikarilan_deger = 0,
+                cikarilan_yuzolcum = 0,
+                devreden_aded = 0,
+                devreden_deger = 0,
+                devreden_yuzolcum = 0
+            };
+
+            int current_year = DateTime.Now.Year;
+
+            for (int i = 0; i < count; i++)
+            {
+                int resultOffset = i * 1000;
+
+                var formUrlEncodedContent = new FormUrlEncodedContent(new[]
+                {
+                    new KeyValuePair<string, string>("where", query),
+                    new KeyValuePair<string, string>("geometryType", "esriGeometryEnvelope"),
+                    new KeyValuePair<string, string>("spatialRel", "esriSpatialRelIntersects"),
+                    new KeyValuePair<string, string>("outFields", "meb_edinim_tarih, tapu_alan, vergi_degeri, rayic_bedeli, kayit_tarih" ),
+                    new KeyValuePair<string, string>("returnGeometry", "false"),
+                    new KeyValuePair<string, string>("f", "pjson"),
+                    new KeyValuePair<string, string>("resultOffset", resultOffset.ToString()),
+                    new KeyValuePair<string, string>("token", token)
+                });
+
+                var response = await httpClient.PostAsync(kadastroParselQueryURI, formUrlEncodedContent);
+                var content = await response.Content.ReadAsStringAsync();
+                var featuresList = JsonConvert.DeserializeObject<ArcgisSayistayIcmalFeature>(content);
+
+                foreach (var feature in featuresList.features)
+                {
+                    double tapu_alan = feature.tapu_alan != null ? Double.Parse(feature.tapu_alan) : 0;
+                    double rayic_bedeli = feature.rayic_bedeli;
+                    double vergi_degeri = feature.vergi_degeri;
+                    DateTime meb_edinim_tarih = feature.meb_edinim_tarih;
+                    DateTime kayit_tarih = feature.kayit_tarih;
+
+                    int edinim_yili = meb_edinim_tarih != null ? meb_edinim_tarih.Year : 0;
+                    int elden_cikarma_yili = kayit_tarih != null ? kayit_tarih.Year : 0;
+
+                    if (edinim_yili < current_year && elden_cikarma_yili == null)
+                    {
+                        icmal.gelen_aded++;
+                        icmal.gelen_yuzolcum += tapu_alan;
+                        if (rayic_bedeli != null)
+                        {
+                            icmal.gelen_deger += rayic_bedeli;
+                        }
+                        else
+                        {
+                            icmal.gelen_deger += vergi_degeri;
+                        }
+                    }
+                    else if (edinim_yili == current_year && elden_cikarma_yili == null)
+                    {
+                        icmal.edinilen_aded++;
+                        icmal.edinilen_yuzolcum += tapu_alan;
+                        if (rayic_bedeli != null)
+                        {
+                            icmal.edinilen_deger += rayic_bedeli;
+                        }
+                        else
+                        {
+                            icmal.edinilen_deger += vergi_degeri;
+                        }
+                    }
+                    else if (elden_cikarma_yili == current_year)
+                    {
+                        icmal.cikarilan_aded++;
+                        icmal.cikarilan_yuzolcum += tapu_alan;
+                        if (rayic_bedeli != null)
+                        {
+                            icmal.cikarilan_deger += rayic_bedeli;
+                        }
+                        else
+                        {
+                            icmal.cikarilan_deger += vergi_degeri;
+                        }
+                    }
+                    if (kayit_tarih == null)
+                    {
+                        icmal.devreden_aded++;
+                        icmal.devreden_yuzolcum += tapu_alan;
+                        if (rayic_bedeli != null)
+                        {
+                            icmal.devreden_deger += rayic_bedeli;
+                        }
+                        else
+                        {
+                            icmal.devreden_yuzolcum += vergi_degeri;
+                        }
+                    }
+                }
+
+            }
+
+            return icmal;
+        }
+
+        [HttpGet("GetMernisBinaKoduFromKimlikNo")]
+        public async Task<string> GetMernisBinaKoduFromKimlikNo(long kimlikNo)
+        {
+            var giris = new ServiceMernis.KullaniciGiris();
+            giris.UserName = "mebcbs";
+            giris.Password = "9UFdAy7x";
+
+            var logClient = new ServiceMernis.LogClient();
+            logClient.EkranKodu = "1";
+            logClient.Aciklama = "MebCbsAnka";
+            logClient.ModulKodu = "1";
+
+            logClient.YetkiliIPAdresi = "10.30.1.244";
+            logClient.YetkiliKullaniciAdi = "1234567910";
+            logClient.YetkiliKullaniciNo = 1234567910;
+            logClient.YetkiliTCKimlikNo = 44257273824;
+
+            var binaNoKriter = new ServiceMernis.AksOkuBagimsizBolumBinaNoSorguKriter();
+
+            binaNoKriter.BinaNo = int.Parse(kimlikNo.ToString());
+
+            var binaNoServisClient = new ServiceMernis.AksOkuBagimsizBolumBinaNoServisClient();
+
+            var response = await binaNoServisClient.BinaNodanBagimsizBolumListeleAsync(binaNoKriter, giris, logClient);
+            if (response != null)
+            {
+                var binaKodu = response.SorguSonucu[0].BinaKodu.ToString();
+                return binaKodu;
+            }
+            else
+            {
+                return "";
+            }
+
+        }
