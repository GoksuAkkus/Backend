diff --git a/MebCbsAnka/Controllers/PermZonesController.cs b/MebCbsAnka/Controllers/PermZonesController.cs
new file mode 100644
index 0000000000000000000000000000000000000000..9aebfa72ac32773c6dd76600bede5a915ee1efe0
--- /dev/null
+++ b/MebCbsAnka/Controllers/PermZonesController.cs
@@ -0,0 +1,76 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.Dtos;
+using MebCbsAnka.Entities;
+using Microsoft.AspNetCore.Http;
+using Microsoft.AspNetCore.Mvc;
+
+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/PermZones")]
+    public class PermZonesController : Controller
+    {
+        private IPermZoneDal _permZoneDal;
+
+        // Dependency Injection
+        public PermZonesController(IPermZoneDal permZoneDal)
+        {
+            _permZoneDal = permZoneDal;
+        }
+
+        [HttpGet("")]
+        public IActionResult Get()
+        {
+            var permissionZones = _permZoneDal.GetList();
+            return Ok(permissionZones);
+        }
+
+        [HttpGet("user/{userId}")]
+        public IActionResult GetByUser(int userId)
+        {
+            var permissionZone = _permZoneDal.GetList(p => p.UserId == userId).Select(p => new PermZoneDto()
+            {
+                Zone = p.Zone
+            });
+
+            return Ok(permissionZone);
+        }
+
+
+        [HttpGet("{permZoneId}")]
+        public IActionResult Get(int permZoneId)
+        {
+            var permissionZone = _permZoneDal.Get(p => p.Id == permZoneId);
+            if (permissionZone == null)
+            {
+                return NotFound($"There is no permission zone with Id= {permZoneId}");
+            }
+            return Ok(permissionZone);
+        }
+
+        [HttpPost("")]
+        public IActionResult Post([FromBody] PermZone permZone)
+        {
+            _permZoneDal.Add(permZone);
+            return new StatusCodeResult(201);
+        }
+
+        [HttpPut("")]
+        public IActionResult Put([FromBody] PermZone permZone)
+        {
+            _permZoneDal.Update(permZone);
+            return Ok(permZone);
+        }
+
+        [HttpDelete("{permZoneId}")]
+        public IActionResult Delete(int permZoneId)
+        {
+            _permZoneDal.Delete(new PermZone { Id = permZoneId });
+            return Ok();
+        }
+    }
+}
