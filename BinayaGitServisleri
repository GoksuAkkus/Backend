diff --git a/MebCbsAnka/Controllers/TKGMController.cs b/MebCbsAnka/Controllers/TKGMController.cs
new file mode 100644
index 0000000000000000000000000000000000000000..d5add9406eab53786132a41dde7734e136a20fe6
--- /dev/null
+++ b/MebCbsAnka/Controllers/TKGMController.cs
@@ -0,0 +1,3213 @@
+﻿using MebCbsAnka.DataAccess.Abstruct;
+using Microsoft.AspNetCore.Mvc;
+using Microsoft.Extensions.Configuration;
+using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Net.Http;
+using System.Threading.Tasks;
+using System.Xml;
+using TKGM;
+using Newtonsoft.Json;
+using MebCbsAnka.Dtos;
+using MebCbsAnka.Dtos.TKGMDtos;
+using Microsoft.AspNetCore.Authorization;
+using System.Globalization;
+using MebCbsAnka.Dtos.TapuResponseDtos;
+using MebCbsAnka.Dtos.NVIDtos;
+using OfficeOpenXml;
+using System.IO;
+
+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/TKGM")]
+    public class TKGMController : Controller
+    {
+        private IAuthRepository _authRepository;
+        private IConfiguration _configuration;
+        private TKGMSoapHeader _header;
+        private ServiceSoapClient _client;
+        private ServiceMernis.KimlikNoSorgulaAdresServisClient _mernisKimlikNoSorgularAdresClient;
+        private ServiceMernis.YerlesimYerindeOturanlarBilgiServisClient _yerlesimYerindeOturanlarBilgiServisClient;
+
+        public TKGMController(IAuthRepository authRepository, IConfiguration configuration)
+        {
+            _authRepository = authRepository;
+            _configuration = configuration;
+            _header = GetHeader();
+            _client = new ServiceSoapClient(ServiceSoapClient.EndpointConfiguration.ServiceSoap);
+            _mernisKimlikNoSorgularAdresClient = new ServiceMernis.KimlikNoSorgulaAdresServisClient();
+            _yerlesimYerindeOturanlarBilgiServisClient = new ServiceMernis.YerlesimYerindeOturanlarBilgiServisClient();
+        }
+
+        private TKGMSoapHeader GetHeader()
+        {
+            TKGMSoapHeader header = new TKGMSoapHeader();
+            header.Token = _configuration.GetSection("TKGMConnection:TKGMToken").Value;
+            header.UserHostAddress = _configuration.GetSection("TKGMConnection:TKGMHostAddress").Value;
+            return header;
+        }
+
+        private HttpClient GetMEGSISHeader(HttpClient client)
+        {
+            var megsisToken = _configuration.GetSection("MEGSISConnection:MEGSISToken").Value;
+            client.DefaultRequestHeaders.Add("Authorization", megsisToken);
+            return client;
+        }
+
+        [HttpGet("getHisseByMalikID")]
+        public async Task<List<TasinmazObject>> GetHisseByMalikID(decimal[] mahalleId, decimal malikId, int malikTip)
+        {
+            List<TasinmazObject> TasinmazList = new List<TasinmazObject>();
+
+            if (malikTip == 1)
+            {
+                MalikTip typeOfMalik = MalikTip.GercekKisi;
+
+                Tasinmaz[] tasinmazList = await GetirTasinmazMalikBilgisinden(mahalleId, malikId, typeOfMalik, TapuBolumDurum.Aktif);
+
+                if (tasinmazList.Length > 0)
+                {
+                    foreach (var tasinmaz in tasinmazList)
+                    {
+                        TasinmazObject t = new TasinmazObject()
+                        {
+                            tasinmazID = tasinmaz.ID,
+                            tasinmazTip = tasinmaz.Tip,
+                            il = tasinmaz.Il,
+                            ilce = tasinmaz.Ilce,
+                            kurum = tasinmaz.Kurum,
+                            mahalle = tasinmaz.Mahalle,
+                            mahalleID = tasinmaz.MahalleID,
+                            kurumID = tasinmaz.KurumID,
+                            mevkii = tasinmaz.Mevkii,
+                            nitelik = tasinmaz.Nitelik,
+                            ciltNo = tasinmaz.CiltNo,
+                            sayfaNo = tasinmaz.SayfaNo,
+                            yuzolcum = tasinmaz.Yuzolcum,
+                            pafta = tasinmaz.Pafta,
+                            tapuBolumDurum = tasinmaz.TapuBolumDurum,
+                            ada = tasinmaz.Ada,
+                            parsel = tasinmaz.Parsel,
+                        };
+
+                        if (tasinmaz.TesisIslem != null)
+                        {
+                            t.islemTanimAd = tasinmaz.TesisIslem.IslemTanimAd;
+                            t.baslamaSekilAd = tasinmaz.TesisIslem.BaslamaSekilAd;
+                            if (tasinmaz.TesisIslem.Yevmiye != null)
+                            {
+                                t.islemTarih = tasinmaz.TesisIslem.Yevmiye.Tarih;
+                            }
+                        }
+
+                        TasinmazList.Add(t);
+                    }
+                }
+            }
+
+            return TasinmazList;
+
+        }
+
+
+        [HttpGet("getGercekKisi")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<GercekKisiVeTasinmazData> GetGercekKisiAndTasinmazData(PersonRequestDto personRequest)
+        {
+            try
+            {
+                List<GercekKisiData> GercekKisiData = new List<GercekKisiData>();
+                List<TasinmazObject> TasinmazList = new List<TasinmazObject>();
+
+                if (personRequest.ad == null)
+                {
+                    personRequest.ad = "";
+                }
+                if (personRequest.soyad == null)
+                {
+                    personRequest.soyad = "";
+                }
+                if (personRequest.babaAd == null)
+                {
+                    personRequest.babaAd = "";
+                }
+                if (personRequest.anaAd == null)
+                {
+                    personRequest.anaAd = "";
+                }
+
+                GercekKisi[] GercekKisiResponse = await GetirGercekKisi(personRequest.ad, personRequest.soyad, personRequest.tckn, personRequest.babaAd, personRequest.anaAd);
+
+                if (GercekKisiResponse.Length != 0)
+                {
+                    foreach (var kisi in GercekKisiResponse)
+                    {
+                        GercekKisiData gercekKisi = new GercekKisiData()
+                        {
+                            id = kisi.ID,
+                            ad = kisi.Ad,
+                            soyAd = kisi.Soyad,
+                            tcKimlikNo = kisi.TCKimlikNo,
+                            anaAd = kisi.AnaAd,
+                            babaAd = kisi.BabaAd,
+                            dogumTarih = kisi.DogumTarih,
+                            olumTarih = kisi.OlumTarih,
+                            cilt = kisi.Cilt,
+                            sira = kisi.Sira,
+                            nufusCuzdaniSeriNo = kisi.NufusCuzdaniSeriNo,
+                            dogumYer = kisi.DogumYer,
+                            cinsiyet = kisi.Cinsiyet,
+                            durum = kisi.Durum
+                        };
+
+                        GercekKisiData.Add(gercekKisi);
+
+                        Tasinmaz[] tasinmazList;
+
+                        if (personRequest.mahalleId.Length > 0)
+                        {
+                            tasinmazList = await GetirTasinmazMalikBilgisinden(personRequest.mahalleId, kisi.ID, MalikTip.GercekKisi, TapuBolumDurum.Aktif);
+                        }
+                        else
+                        {
+                            tasinmazList = await GetirTasinmazMalikBilgisinden(personRequest.mahalleId, kisi.ID, MalikTip.GercekKisi, TapuBolumDurum.Aktif);
+                        }
+
+                        if (tasinmazList.Length > 0)
+                        {
+                            foreach (var tasinmaz in tasinmazList)
+                            {
+
+                                TasinmazObject t = new TasinmazObject()
+                                {
+                                    tasinmazID = tasinmaz.ID,
+                                    tasinmazTip = tasinmaz.Tip,
+                                    il = tasinmaz.Il,
+                                    ilce = tasinmaz.Ilce,
+                                    kurum = tasinmaz.Kurum,
+                                    mahalle = tasinmaz.Mahalle,
+                                    mahalleID = tasinmaz.MahalleID,
+                                    kurumID = tasinmaz.KurumID,
+                                    mevkii = tasinmaz.Mevkii,
+                                    nitelik = tasinmaz.Nitelik,
+                                    ciltNo = tasinmaz.CiltNo,
+                                    sayfaNo = tasinmaz.SayfaNo,
+                                    yuzolcum = tasinmaz.Yuzolcum,
+                                    pafta = tasinmaz.Pafta,
+                                    tapuBolumDurum = tasinmaz.TapuBolumDurum,
+                                    ada = tasinmaz.Ada,
+                                    parsel = tasinmaz.Parsel,
+                                    malikID = kisi.ID
+                                };
+
+                                if (tasinmaz.TesisIslem != null)
+                                {
+                                    t.islemTanimAd = tasinmaz.TesisIslem.IslemTanimAd;
+                                    t.baslamaSekilAd = tasinmaz.TesisIslem.BaslamaSekilAd;
+                                    if (tasinmaz.TesisIslem.Yevmiye != null)
+                                    {
+                                        t.islemTarih = tasinmaz.TesisIslem.Yevmiye.Tarih;
+                                    }
+                                }
+
+                                TasinmazList.Add(t);
+                            }
+
+                        }
+                    }
+
+                }
+
+                GercekKisiVeTasinmazData gercekKisiVeTasinmazData = new GercekKisiVeTasinmazData()
+                {
+                    gercekKisi = GercekKisiData,
+                    gercekKisiFields = CreateGercekKisiFields(),
+                    tasinmazList = TasinmazList,
+                    tasinmazFields = CreateTasinmazFields(),
+                };
+
+                return gercekKisiVeTasinmazData;
+            }
+            catch (Exception e)
+            {
+                GercekKisiVeTasinmazData gercekKisiVeTasinmazData = new GercekKisiVeTasinmazData()
+                {
+                    gercekKisi = null,
+                    gercekKisiFields = CreateGercekKisiFields(),
+                    tasinmazList = null,
+                    tasinmazFields = CreateTasinmazFields(),
+                };
+                return gercekKisiVeTasinmazData;
+            }
+
+        }
+
+        [HttpGet("getTuzelKisi")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<TuzelKisiVeTasinmazData> GetTuzelKisi(string ad, string vergi_no, decimal[] mahalleIds)
+        {
+            List<TuzelKisiData> tuzelKisiData = new List<TuzelKisiData>();
+            List<TasinmazObject> TasinmazList = new List<TasinmazObject>();
+            try
+            {
+                //TuzelKisi tuzelKisi = await GetirTuzelKisiIDDen(id);
+                TuzelKisi[] tuzelKisiList = new TuzelKisi[] { };
+                tuzelKisiList = await GetirTuzelKisi(ad, vergi_no);
+
+                //TuzelKisi tuzelKisi = tuzelKisiList[0];
+
+                if (tuzelKisiList.Length > 0)
+                {
+                    foreach (var tuzelKisi in tuzelKisiList)
+                    {
+                        TuzelKisiData TuzelKisi = new TuzelKisiData()
+                        {
+                            id = tuzelKisi.ID,
+                            ad = tuzelKisi.Ad,
+                            vergiNo = tuzelKisi.VergiNo,
+                            sicilNo = tuzelKisi.SicilNo,
+                            yabanciSermayeli = tuzelKisi.YabanciSermayeli,
+                            yabanciTemsilcili = tuzelKisi.YabanciTemsilcili,
+                            durum = tuzelKisi.Durum,
+                            tuzelKisiTipi = tuzelKisi.TuzelKisiTipi
+                        };
+
+                        tuzelKisiData.Add(TuzelKisi);
+                    }
+
+                    foreach (var tuzelKisi in tuzelKisiList)
+                    {
+                        Tasinmaz[] tasinmazList;
+
+                        if (mahalleIds.Length > 0)
+                        {
+                            tasinmazList = await GetirTasinmazMalikBilgisinden(mahalleIds, tuzelKisi.ID, MalikTip.TuzelKisi, TapuBolumDurum.Aktif);
+                        }
+                        else
+                        {
+                            tasinmazList = await GetirTasinmazMalikBilgisinden(null, tuzelKisi.ID, MalikTip.TuzelKisi, TapuBolumDurum.Aktif);
+                        }
+
+                        if (tasinmazList.Length > 0)
+                        {
+                            foreach (var tasinmaz in tasinmazList)
+                            {
+                                TasinmazObject t = new TasinmazObject()
+                                {
+                                    tasinmazID = tasinmaz.ID,
+                                    tasinmazTip = tasinmaz.Tip,
+                                    il = tasinmaz.Il,
+                                    ilce = tasinmaz.Ilce,
+                                    kurum = tasinmaz.Kurum,
+                                    mahalle = tasinmaz.Mahalle,
+                                    mahalleID = tasinmaz.MahalleID,
+                                    kurumID = tasinmaz.KurumID,
+                                    mevkii = tasinmaz.Mevkii,
+                                    nitelik = tasinmaz.Nitelik,
+                                    ciltNo = tasinmaz.CiltNo,
+                                    sayfaNo = tasinmaz.SayfaNo,
+                                    yuzolcum = tasinmaz.Yuzolcum,
+                                    pafta = tasinmaz.Pafta,
+                                    tapuBolumDurum = tasinmaz.TapuBolumDurum,
+                                    ada = tasinmaz.Ada,
+                                    parsel = tasinmaz.Parsel,
+                                    malikID = tuzelKisi.ID
+                                };
+
+                                if (tasinmaz.TesisIslem != null)
+                                {
+                                    t.islemTanimAd = tasinmaz.TesisIslem.IslemTanimAd;
+                                    t.baslamaSekilAd = tasinmaz.TesisIslem.BaslamaSekilAd;
+                                    if (tasinmaz.TesisIslem.Yevmiye != null)
+                                    {
+                                        t.islemTarih = tasinmaz.TesisIslem.Yevmiye.Tarih;
+                                    }
+                                }
+
+                                TasinmazList.Add(t);
+                            }
+                        }
+                    }
+
+                }
+
+                TuzelKisiVeTasinmazData tuzelKisiVeTasinmazData = new TuzelKisiVeTasinmazData()
+                {
+                    tuzelKisi = tuzelKisiData,
+                    tuzelKisiFields = CreateTuzelKisiFields(),
+                    tasinmazFields = CreateTasinmazFields(),
+                    tasinmazList = TasinmazList
+                };
+
+                return tuzelKisiVeTasinmazData;
+            }
+            catch (Exception e)
+            {
+                TuzelKisiVeTasinmazData tuzelKisiVeTasinmazData = new TuzelKisiVeTasinmazData()
+                {
+                    tuzelKisi = tuzelKisiData,
+                    tuzelKisiFields = CreateTuzelKisiFields(),
+                    tasinmazFields = CreateTasinmazFields(),
+                    tasinmazList = TasinmazList
+                };
+
+                return tuzelKisiVeTasinmazData;
+            }
+
+        }
