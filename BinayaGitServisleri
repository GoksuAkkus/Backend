+++ b/MebCbsAnka/Entities/User.cs
@@ -0,0 +1,49 @@
+﻿using System;
+using System.Collections.Generic;
+using System.ComponentModel.DataAnnotations;
+using System.Linq;
+using System.Threading.Tasks;
+using Newtonsoft.Json;
+
+namespace MebCbsAnka.Entities
+{
+    public class User:IEntity
+    {
+
+        public int    Id { get; set; }
+        [MaxLength(15)]
+        public string UserName { get; set; }
+        [MaxLength(30)]
+        public string FirstName { get; set; }
+        [MaxLength(30)]
+        public string LastName { get; set; }
+        public byte[] PasswordHash { get; set; }
+        public byte[] PasswordSalt { get; set; }
+        [MaxLength(50)]
+        public string Email { get; set; }
+        [MaxLength(32)]
+        public string Phone { get; set; }
+        [MaxLength(128)]
+        public string Dept { get; set; }
+        [MaxLength(128)]
+        public string Title { get; set; }
+        public bool   IsActive { get; set; }
+        public bool   IsAdmin { get; set; }
+        public bool   IsModerator { get; set; }
+        public bool   ModeratorRequired { get; set; }
+        public DateTime CreatedDate { get; set; }
+        public DateTime UpdatedDate { get; set; }
+        [MaxLength(30)]
+        public string   GroupName  { get; set; }
+        public int ParentId { get; set; }
+        public int tryLoginCount { get; set; }
+
+        // Navigation Properties
+        public virtual User Parent { get; set; }
+        public virtual ICollection<User> Children { get; set; }
+        public ICollection<PermLayer> PermLayer { get; set; }
+        public ICollection<PermGUI> PermGUI { get; set; }
+        public ICollection<PermZone> PermZone { get; set; }
+      
+    }
+}
diff --git a/MebCbsAnka/Helper/JwtExtension.cs b/MebCbsAnka/Helper/JwtExtension.cs
new file mode 100644
index 0000000000000000000000000000000000000000..8c01135cda428a6616cfd384af476b3c10e32333
--- /dev/null
+++ b/MebCbsAnka/Helper/JwtExtension.cs
@@ -0,0 +1,20 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Threading.Tasks;
+using Microsoft.AspNetCore.Http;
+
+namespace MebCbsAnka.Helper
+{
+    public static class JwtExtension
+    {
+        //handle errors
+        public static void AddApplicationError(this HttpResponse response, string message)
+        {
+            // Jwt operation add to the header
+            response.Headers.Add("Application-Error", message);
+            response.Headers.Add("Access-Control-Allow-Origin", "*");
+            response.Headers.Add("Access-Control-Expose-Header", "Application-Error");
+        }
+    }
+}
diff --git a/MebCbsAnka/Helper/Utils.cs b/MebCbsAnka/Helper/Utils.cs
new file mode 100644
index 0000000000000000000000000000000000000000..467ca72c3dafab8340145d7d653882e4b2e09b31
--- /dev/null
+++ b/MebCbsAnka/Helper/Utils.cs
@@ -0,0 +1,15 @@
+﻿namespace MebCbsAnka.Helper
+{
+    public class Utils
+    {
+        public void CreatePasswordHash(string password, out byte[] passwordHash, out byte[] passwordSalt)
+        {
+            using (var hmac = new System.Security.Cryptography.HMACSHA512())
+            {
+                //Key used here
+                passwordSalt = hmac.Key;
+                passwordHash = hmac.ComputeHash(System.Text.Encoding.UTF8.GetBytes(password));
+            }
+        }
+    }
+}
