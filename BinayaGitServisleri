diff --git a/MebCbsAnka/DataAccess/Concrete/LogRepository.cs b/MebCbsAnka/DataAccess/Concrete/LogRepository.cs
new file mode 100644
index 0000000000000000000000000000000000000000..c1b63f46ac6a4cb4b6f5d03cf43a060aac616f40
--- /dev/null
+++ b/MebCbsAnka/DataAccess/Concrete/LogRepository.cs
@@ -0,0 +1,121 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Runtime.InteropServices.WindowsRuntime;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.Dtos;
+using MebCbsAnka.Entities;
+using Microsoft.AspNetCore.Http;
+using Microsoft.EntityFrameworkCore;
+
+namespace MebCbsAnka.DataAccess.Concrete
+{
+    public class LogRepository : ILogRepository
+    {
+        private LoggerDbContext _context;
+        private MebCbsAnkaContext _mebCbsContext;
+
+        public LogRepository(LoggerDbContext context, MebCbsAnkaContext mebCbsContext)
+        {
+            _context = context;
+            _mebCbsContext = mebCbsContext;
+        }
+
+        public void Add<T>(T entity) where T : class
+        {
+            _context.Add(entity);
+            _context.SaveChanges();
+        }
+
+        public List<Log> GetLogByUserId(int userId)
+        {
+            var user = _mebCbsContext.Users.FirstOrDefault(u => u.Id == userId);
+            if (user == null)
+                return null;
+            var logs = _context.Logs
+                .Where(log => log.UserId == userId).ToList();
+            return logs;
+        }
+
+        public List<LogListDto> GetLogs(List<int> ids)
+        {
+            List<LogListDto> logListDto = new List<LogListDto>();
+            var logList = _context.Logs.Where(x => ids.Contains(x.UserId)).ToList();
+            foreach (var data in logList)
+            {
+                LogListDto log = new LogListDto()
+                {
+                    Id = data.Id,
+                    UserId = data.UserId,
+                    Description = data.Description,
+                    CreatedDate = data.CreatedDate,
+                    Action = data.Action,
+                    UserName = data.UserName
+                };
+                logListDto.Add(log);
+            }
+
+            return new List<LogListDto>(logListDto.OrderByDescending(x => x.Id));
+        }
+
+        public List<UserListDto> GetUsers(int parentId, bool isRoot)
+        {
+            List<User> userList = new List<User>();
+            List<UserListDto> userDtoList = new List<UserListDto>();
+            if (isRoot)
+            {
+                userList = _mebCbsContext.Users.Where(u => u.UserName != "mebsuper").ToList();
+            }
+            else
+            {
+                userList = _mebCbsContext.Users.Include(u => u.Children).ToList().Where(x => x.Id == parentId).ToList();
+                userList = GetFlatMap(userList);
+            }
+
+            userDtoList = userList.Select(u => new UserListDto()
+            {
+                id = u.Id,
+                Dept = u.Dept,
+                Email = u.Email,
+                FirstName = u.FirstName,
+                GroupName = u.GroupName,
+                LastName = u.LastName,
+                Phone = u.Phone,
+                Title = u.Title,
+                UserName = u.UserName,
+            }).ToList();
+
+            return userDtoList;
+        }
+
+        private List<User> GetFlatMap(List<User> userlist)
+        {
+            List<User> userList = new List<User>();
+            foreach (User user in userlist)
+            {
+                if (user.Children == null)
+                {
+                    var hiearchyUser = _mebCbsContext.Users.Where(u => u.Id == user.Id).Include(u => u.Children)
+                        .FirstOrDefault();
+                    var children = hiearchyUser.Children.ToList<User>();
+                    var groupUser = children.Where(c => c.IsAdmin == false).ToList<User>();
+                    var parentUser = children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(groupUser);
+                    userList.AddRange(parentUser);
+                    userList.AddRange(GetFlatMap(parentUser));
+                }
+                else
+                {
+                    var children = user.Children.ToList<User>();
+                    var groupUser = children.Where(c => c.IsAdmin == false).ToList<User>();
+                    var parentUser = children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(groupUser);
+                    userList.AddRange(parentUser);
+                    userList.AddRange(GetFlatMap(parentUser));
+                }                
+            }
+            return userList;
+        }
+    }
+}
