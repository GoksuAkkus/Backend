+        [HttpGet("getIL")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<Il[]> GetirIl()
+        {
+            Il[] ilList = await GetirIlTum();
+
+            return ilList;
+        }
+
+        [HttpGet("getIlce")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<Ilce[]> getIlce(decimal ilID)
+        {
+            Ilce[] ilceList = await GetirIlceIlIDDen(ilID);
+            return ilceList;
+        }
+
+        [HttpGet("getMahalle")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<List<TapuResponse>> getMahalle(decimal ilceID)
+        {
+            Mahalle[] mahalleList = await GetirMahalleIlceIDDen(ilceID, Durum.Aktif);
+            List<TapuResponse> tapuMahalleList = new List<TapuResponse>();
+
+            foreach (var mahalle in mahalleList)
+            {
+                TapuResponse tapuMahalle = new TapuResponse()
+                {
+                    id = mahalle.ID,
+                    ad = mahalle.Ad
+                };
+                tapuMahalleList.Add(tapuMahalle);
+            }
+            return tapuMahalleList;
+        }
+
+        [HttpGet("getAdaNumarasi")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<string[]> getAdaNumarasi(decimal mahalleID)
+        {
+            string[] adaNumaralari = await GetirAdaNumaralari(mahalleID);
+            return adaNumaralari;
+        }
+
+        [HttpGet("getParselNumarasi")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<string[]> getParseller(decimal mahalleID, string adaNo)
+        {
+            if (adaNo == null)
+            {
+                adaNo = "0";
+            }
+            string[] parselNumaralari = await GetirParselNumaralari(mahalleID, adaNo);
+            return parselNumaralari;
+        }
+
+        [HttpPost("getPassiveParcel")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<List<TKGMFeatureSet>> getPassiveParcel([FromBody] MEGSISSearchParcelRequestDto MEGSISSearchParcelRequest)
+        {
+            List<TKGMFeatureSet> tkgmFeatureSetList = await SetTKGMActiveOrPassiveFeatureSet(MEGSISSearchParcelRequest);
+
+            return tkgmFeatureSetList;
+        }
