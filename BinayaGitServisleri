diff --git a/MebCbsAnka/DataAccess/Concrete/AuthRepository.cs b/MebCbsAnka/DataAccess/Concrete/AuthRepository.cs
new file mode 100644
index 0000000000000000000000000000000000000000..68411c09409dda41a9d7c3c001f89532c56d9846
--- /dev/null
+++ b/MebCbsAnka/DataAccess/Concrete/AuthRepository.cs
@@ -0,0 +1,906 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Globalization;
+using System.Linq;
+using System.Security.Cryptography;
+using System.Threading.Tasks;
+using MebCbsAnka.DataAccess.Abstruct;
+using MebCbsAnka.Dtos;
+using MebCbsAnka.Entities;
+using Microsoft.AspNetCore.Mvc;
+using Microsoft.EntityFrameworkCore;
+namespace MebCbsAnka.DataAccess.Concrete
+{
+    public class AuthRepository : IAuthRepository
+    {
+        private MebCbsAnkaContext _context;
+        // Dependency Injection
+        public AuthRepository(MebCbsAnkaContext context)
+        {
+            _context = context;
+        }
+
+        //IAuth Implementation
+        public bool SaveAll()
+        {
+            // If there are changes to save
+            return _context.SaveChanges() > 0;
+        }
+
+        public async Task<User> Register(User user, string password)
+        {
+            byte[] passwordHash, passwordSalt;
+            CreatePasswordHash(password, out passwordHash, out passwordSalt);
+
+            user.PasswordHash = passwordHash;
+            user.PasswordSalt = passwordSalt;
+            _context.Users.Add(user);
+            await _context.SaveChangesAsync();
+
+            return user;
+        }
+
+        //// TODO: Need Cleaning THIS Part !!!
+        //public async Task<User> RegisterAll(User user, PermGUI permGUI, PermLayer permLayer, PermZone permZone, string password)
+        //{
+        //    byte[] passwordHash, passwordSalt;
+        //    CreatePasswordHash(password, out passwordHash, out passwordSalt);
+
+        //    user.PasswordHash = passwordHash;
+        //    user.PasswordSalt = passwordSalt;
+
+        //    await _context.Users.AddAsync(user);
+        //    await _context.SaveChangesAsync();
+        //    return user;
+        //}
+
+        private void CreatePasswordHash(string password, out byte[] passwordHash, out byte[] passwordSalt)
+        {
+            using (var hmac = new System.Security.Cryptography.HMACSHA512())
+            {
+                //Key used here
+                passwordSalt = hmac.Key;
+                passwordHash = hmac.ComputeHash(System.Text.Encoding.UTF8.GetBytes(password));
+            }
+        }
+
+        public User GetUserWithId(int id)
+        {
+            var user = _context.Users.FirstOrDefault(u => u.Id == id);
+            return user;
+        }
+
+        public async Task<User> GetUserWithUsername(string userName)
+        {
+            var user = _context.Users.FirstOrDefault(u => u.UserName == userName);
+            return user;
+        }
+
+        [HttpPost("login")]
+        public async Task<User> Login(string userName, string password)
+        {
+            var user = await _context.Users.FirstOrDefaultAsync(u => u.UserName == userName);
+
+            // User is not registered
+            if (user == null)
+            {
+                return null;
+            }
+
+            // Check user password
+            if (!VerifyPasswordHash(password, user.PasswordHash, user.PasswordSalt))
+            {
+                return null;
+            }
+
+            return user;
+        }
+
+        private bool VerifyPasswordHash(string password, byte[] userPasswordHash, byte[] userPasswordSalt)
+        {
+            using (var hmac = new System.Security.Cryptography.HMACSHA512(userPasswordSalt))
+            {
+                var computedHash = hmac.ComputeHash(System.Text.Encoding.UTF8.GetBytes(password));
+
+                //Compare the both hashes
+                for (int i = 0; i < computedHash.Length; i++)
+                {
+                    if (computedHash[i] != userPasswordHash[i])
+                    {
+                        return false;
+                    }
+                }
+
+                return true;
+            }
+        }
+
+        public async Task<bool> UserExits(string userName)
+        {
+            if (await _context.Users.AnyAsync(u => u.UserName == userName.ToLower()))
+            {
+                return true;
+            }
+            return false;
+        }
+
+        public bool RelatedUserExists(int id)
+        {
+            if (_context.Users.Any(u => u.ParentId == id))
+            {
+                return true;
+            }
+            return false;
+        }
+
+        private List<UserListDto> GetGroupUsers(int parentId)
+        {
+            var userList = _context.Users.Where(u => u.IsAdmin == false && u.ParentId == parentId).Select(u => new UserListDto()
+            {
+                Dept = u.Dept,
+                Email = u.Email,
+                FirstName = u.FirstName,
+                GroupName = u.GroupName,
+                id = u.Id,
+                LastName = u.LastName,
+                Phone = u.Phone,
+                Title = u.Title,
+                UserName = u.UserName,
+            }).ToList();
+
+            return userList;
+        }
+
+        public List<UserListDto> GetUsers(int parentId, bool isRoot)
+        {
+            List<User> userList = new List<User>();
+            List<UserListDto> userDtoList = new List<UserListDto>();
+            if (isRoot)
+            {
+                userList = _context.Users.Where(u => u.IsAdmin == false).ToList();
+            }
+            else
+            {
+                userList = _context.Users.Include(u => u.Children).ToList().Where(x => x.Id == parentId).ToList();
+                userList = getFlatMap(userList);
+            }
+            userDtoList = userList.Select(u => new UserListDto()
+            {
+                id = u.Id,
+                Dept = u.Dept,
+                Email = u.Email,
+                FirstName = u.FirstName,
+                GroupName = u.GroupName,
+                LastName = u.LastName,
+                Phone = u.Phone,
+                Title = u.Title,
+                UserName = u.UserName,
+            }).ToList();
+
+            return userDtoList;
+        }
+
+        private List<User> GetSubUsersGroups(int parentId)
+        {
+            List<User> userList = new List<User>();
+            List<UserListDto> userDtoList = new List<UserListDto>();
+
+            userList = _context.Users.Include(u => u.Children).ToList().Where(x => x.Id == parentId).ToList();
+            userList = getUserGroupsFlatMap(userList);
+
+            return userList;
+        }
+
+        private List<User> getUserGroupsFlatMap(List<User> userlist)
+        {
+            List<User> userList = new List<User>();
+            foreach (User user in userlist)
+            {
+                if (user.Children == null)
+                {
+                    var hiearchyUser = _context.Users.Where(u => u.Id == user.Id).Include(u => u.Children).FirstOrDefault();
+                    var children = hiearchyUser.Children.ToList<User>();
+                    var groupUser = children.Where(c => c.IsAdmin == false).ToList<User>();
+                    var parentUser = children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(parentUser);
+                    userList.AddRange(groupUser);
+                    userList.AddRange(getFlatMap(parentUser));
+                }
+                else
+                {
+                    var children = user.Children.ToList<User>();
+                    var groupUser = children.Where(c => c.IsAdmin == false).ToList<User>();
+                    var parentUser = children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(parentUser);
+                    userList.AddRange(groupUser);
+                    userList.AddRange(getFlatMap(parentUser));
+                }
+            }
+            return userList;
+        }
+
+        private List<LastAddedUsers> GetLastAddedUsers(int parentId, bool isRoot)
+        {
+            List<User> userList = new List<User>();
+            List<LastAddedUsers> userDtoList = new List<LastAddedUsers>();
+            if (isRoot)
+            {
+                userList = _context.Users.Where(u => u.IsAdmin == false).ToList();
+            }
+            else
+            {
+                userList = _context.Users.Include(u => u.Children).ToList().Where(x => x.Id == parentId).ToList();
+                userList = getFlatMap(userList);
+            }
+            userDtoList = userList.Select(u => new LastAddedUsers()
+            {
+                Id = u.Id,
+                CreatedDate = u.CreatedDate,
+                UserName = u.UserName,
+            }).ToList();
+
+            return userDtoList;
+        }
+
+        private List<GroupListDto> GetSubGroups(int parentId, bool isRoot)
+        {
+            List<User> userList = new List<User>();
+            List<GroupListDto> groupDtoList = new List<GroupListDto>();
+            if (isRoot)
+            {
+                userList = _context.Users.Include(u => u.Children).Where(u => u.IsAdmin == true && u.ParentId == parentId && u.Id != 1).ToList();
+            }
+            else
+            {
+                userList = _context.Users.Include(u => u.Children).ToList().Where(x => x.IsAdmin == true && x.ParentId == parentId).ToList();
+            }
+
+            foreach (User user in userList)
+            {
+                var countInfo = getChildrenCount(user, 0, 0).Split('-');
+                int userCount = int.Parse(countInfo[0]);
+                int groupCount = int.Parse(countInfo[1]);
+                GroupListDto group = new GroupListDto()
+                {
+                    CreatedDate = user.CreatedDate,
+                    GroupName = user.GroupName,
+                    Id = user.Id,
+                    SubGroupCount = groupCount,
+                    SubGroupUsers = userCount,
+                };
+                groupDtoList.Add(group);
+            }
+
+            return groupDtoList;
+        }
+
+        public List<SubGroupInfoDto> GetSubGroupsIds(int userId, bool isRoot)
+        {
+            List<User> userList = new List<User>();
+            if (isRoot)
+            {
+                userList = _context.Users.Include(u => u.Children).Where(u => u.IsAdmin == true).ToList();
+            }
+            else
+            {
+                userList = _context.Users.Include(u => u.Children).ToList().Where(x => x.IsAdmin == true && (x.Id == userId)).ToList();
+                userList.AddRange(GetSubGroupsFlat(userList));
+            }
+            var groupIds = userList.Select(x => new SubGroupInfoDto()
+            {
+                GroupName = x.GroupName,
+                Id = x.Id
+            }).ToList();
+            return groupIds;
+        }
+
+        public List<SubGroupInfoDto> GetSubGroupsIds(int userId, bool isRoot, int groupId)
+        {
+            List<User> excludedUsers = _context.Users.Include(u => u.Children).Where(u => u.Id == groupId).ToList();
+            excludedUsers.AddRange(GetSubGroupsFlat(excludedUsers));
+            List<User> userList = new List<User>();
+            if (isRoot)
+            {
+                userList = _context.Users.Include(u => u.Children).Where(u => u.IsAdmin == true).ToList();
+            }
+            else
+            {
+                userList = _context.Users.Include(u => u.Children).ToList().Where(x => x.IsAdmin == true && (x.Id == userId)).ToList();
+                userList.AddRange(GetSubGroupsFlat(userList));
+            }
+            userList = userList.Except(excludedUsers).ToList();
+            var groupIds = userList.Select(x => new SubGroupInfoDto()
+            {
+                GroupName = x.GroupName,
+                Id = x.Id
+            }).ToList();
+            return groupIds;
+        }
+
+        private List<User> GetSubGroupsFlat(List<User> list, int groupId)
+        {
+            List<User> userList = new List<User>();
+            foreach (User user in list)
+            {
+                if (user.Children == null)
+                {
+                    var hiearchyUser = _context.Users.Where(u => u.Id == user.Id).Include(u => u.Children).FirstOrDefault();
+                    var parentUser = hiearchyUser.Children.Where(c => c.IsAdmin == true && c.Id != groupId).ToList<User>();
+                    userList.AddRange(parentUser);
+                    userList.AddRange(getFlatMap(parentUser));
+                }
+                else
+                {
+                    var parentUser = user.Children.Where(c => c.IsAdmin == true && c.Id != groupId).ToList<User>();
+                    userList.AddRange(parentUser);
+                    userList.AddRange(getFlatMap(parentUser));
+                }
+
+            }
+            return userList;
+        }
+
+        private List<User> GetSubGroupsFlat(List<User> list)
+        {
+            List<User> userList = new List<User>();
+            foreach (User user in list)
+            {
+                if (user.Children == null)
+                {
+                    var hiearchyUser = _context.Users.Where(u => u.Id == user.Id).Include(u => u.Children).FirstOrDefault();
+                    var parentUser = hiearchyUser.Children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(parentUser);
+                    userList.AddRange(GetSubGroupsFlat(parentUser));
+                }
+                else
+                {
+                    var parentUser = user.Children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(parentUser);
+                    userList.AddRange(GetSubGroupsFlat(parentUser));
+                }
+
+            }
+            return userList;
+        }
+
+        public List<GroupListDto> GetGroups(int parentId, bool isRoot)
+        {
+            List<User> userList = new List<User>();
+            List<GroupListDto> groupDtoList = new List<GroupListDto>();
+            if (isRoot)
+            {
+                userList = _context.Users.Include(u => u.Children).Where(u => u.IsAdmin == true && u.ParentId == parentId).ToList();
+            }
+            else
+            {
+                userList = _context.Users.Include(u => u.Children).Where(u => u.IsAdmin == true && u.ParentId == parentId || u.Id == parentId).ToList();
+            }
+
+            foreach (User user in userList)
+            {
+                var countInfo = getChildrenCount(user, 0, 0).Split('-');
+                int userCount = int.Parse(countInfo[0]);
+                int groupCount = int.Parse(countInfo[1]);
+                GroupListDto group = new GroupListDto()
+                {
+                    CreatedDate = user.CreatedDate,
+                    GroupName = user.GroupName,
+                    Id = user.Id,
+                    SubGroupCount = groupCount,
+                    SubGroupUsers = userCount,
+                };
+                groupDtoList.Add(group);
+            }
+
+            return groupDtoList;
+        }
+
+        public GroupInfoDto GetGroupById(int userId, int parentId)
+        {
+            var user = _context.Users.Include(u => u.Parent).Where(u => u.Id == userId).FirstOrDefault();
+            if (user == null)
+            {
+                return null;
+            }
+            if (userId != parentId)
+            {
+                if (!checkIfChild(user, parentId))
+                {
+                    return null;
+                }
+            }
+
+
+            GroupInfoDto groupInfo = new GroupInfoDto();
+            bool isRoot = false;
+            if (userId == 1)
+            {
+                isRoot = true;
+            }
+
+            groupInfo.Dept = user.Dept;
+            groupInfo.Email = user.Email;
+            groupInfo.FirstName = user.FirstName;
+            groupInfo.GroupName = user.GroupName;
+            groupInfo.LastName = user.LastName;
+            groupInfo.Phone = user.Phone;
+            groupInfo.Title = user.Title;
+            groupInfo.UserName = user.UserName;
+            groupInfo.IsModerator = user.IsModerator;
+
+            groupInfo.SubGroups = GetSubGroups(userId, isRoot);
+            groupInfo.SubUsers = GetGroupUsers(userId);
+
+            return groupInfo;
+        }
+
+        private string getChildrenCount(User user, int userCount, int groupCount)
+        {
+            if (user.Children == null)
+            {
+                user = _context.Users.Where(u => u.Id == user.Id).Include(u => u.Children).FirstOrDefault();
+            }
+
+            List<User> childGroups = user.Children.Where(ch => ch.IsAdmin == true && ch.Id != 1).ToList();
+
+
+            userCount += user.Children.Where(ch => ch.IsAdmin == false).Count();
+            groupCount += childGroups.Count();
+
+            foreach (User childUser in childGroups)
+            {
+                var countString = getChildrenCount(childUser, userCount, groupCount).Split('-');
+                userCount = int.Parse(countString[0]);
+                groupCount = int.Parse(countString[1]);
+            }
+
+            return userCount.ToString() + '-' + groupCount.ToString();
+        }
+        private List<User> getFlatMap(List<User> userlist)
+        {
+            List<User> userList = new List<User>();
+            foreach (User user in userlist)
+            {
+                if (user.Children == null)
+                {
+                    var hiearchyUser = _context.Users.Where(u => u.Id == user.Id).Include(u => u.Children).FirstOrDefault();
+                    var children = hiearchyUser.Children.ToList<User>();
+                    var groupUser = children.Where(c => c.IsAdmin == false).ToList<User>();
+                    var parentUser = children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(groupUser);
+                    userList.AddRange(getFlatMap(parentUser));
+                }
+                else
+                {
+                    var children = user.Children.ToList<User>();
+                    var groupUser = children.Where(c => c.IsAdmin == false).ToList<User>();
+                    var parentUser = children.Where(c => c.IsAdmin == true).ToList<User>();
+                    userList.AddRange(groupUser);
+                    userList.AddRange(getFlatMap(parentUser));
+                }
+            }
+            return userList;
+        }
+
+        public UserInfoDto GetUserById(int id, int parentId)
+        {
+            var user = _context.Users.Include(x => x.Parent).Include(x => x.PermGUI).Include(x => x.PermLayer).Include(x => x.PermZone)
+                .FirstOrDefault(c => c.Id == id && c.IsAdmin == false);
+
+            UserInfoDto userInfo = null;
+            if (user != null)
+            {
+                if (checkIfChild(user, parentId))
+                {
+                    userInfo = new UserInfoDto()
+                    {
+                        Phone = user.Phone,
+                        Title = user.Title,
+                        UserName = user.UserName,
+                        FirstName = user.FirstName,
+                        Dept = user.Dept,
+                        Email = user.Email,
+                        GroupName = user.GroupName,
+                        LastName = user.LastName,
+                        ParentId = user.ParentId,
+                        IsModerator = user.IsModerator,
+                        ModeratorRequired = user.ModeratorRequired,
+                        permGUI = user.PermGUI.Select(g => new PermGUIDto()
+                        {
+                            Item = g.Item,
+                            Menu = g.Menu,
+                        }).ToList(),
+                        PermLayer = user.PermLayer.Select(l => new PermLayerDto()
+                        {
+                            Layer = l.Layer,
+                            Field = l.Field,
+                            Permission = l.Permission,
+                        }).ToList(),
+                        PermZone = user.PermZone.Select(z => new PermZoneDto()
+                        {
+                            Zone = z.Zone,
+                        }).ToList(),
+                    };
+                }
+                else
+                {
+                    return null;
+                }
+            }
+            return userInfo;
+        }
+
+        public UserInfoDto GetUserGroupInfo(int id, int parentId)
+        {
+            if (id != 1)
+            {
+                var user = _context.Users.Include(x => x.Parent).Include(x => x.PermGUI).Include(x => x.PermLayer).Include(x => x.PermZone)
+                .FirstOrDefault(c => c.Id == id && c.IsAdmin == true);
+
+                UserInfoDto userInfo = null;
+                if (user != null)
+                {
+                    if (checkIfChild(user, parentId))
+                    {
+                        userInfo = new UserInfoDto()
+                        {
+                            Phone = user.Phone,
+                            Title = user.Title,
+                            UserName = user.UserName,
+                            FirstName = user.FirstName,
+                            Dept = user.Dept,
+                            Email = user.Email,
+                            GroupName = user.GroupName,
+                            IsModerator = user.IsModerator,
+                            LastName = user.LastName,
+                            ParentId = user.ParentId,
+                            permGUI = user.PermGUI.Select(g => new PermGUIDto()
+                            {
+                                Item = g.Item,
+                                Menu = g.Menu,
+                            }).ToList(),
+                            PermLayer = user.PermLayer.Select(l => new PermLayerDto()
+                            {
+                                Layer = l.Layer,
+                                Field = l.Field,
+                                Permission = l.Permission,
+                            }).ToList(),
+                            PermZone = user.PermZone.Select(z => new PermZoneDto()
+                            {
+                                Zone = z.Zone,
+                            }).ToList(),
+                        };
+                    }
+                    else
+                    {
+                        return null;
+                    }
+                }
+                return userInfo;
+            }
+            else
+            {
+                return null;
+            }
+        }
+
+        private bool CheckChildrens(User user, int userid)
+        {
+            User parent = user.Parent;
+            if (user.Parent.Parent == null)
+            {
+                parent = _context.Users.Where(u => u.Id == parent.Id).Include(u => u.Parent).FirstOrDefault();
+            }
+            // Super Admin Can also not delete user that are parent to other users!!!
+            //if (userid == 1)
+            //{
+            //    return true;
+            //}
+            if (parent.Id == userid)
+            {
+                if (parent.Parent != null && parent.Parent.Id != 1)
+                    return (checkIfChild(parent, userid));
+                else
+                    return false;
+            }
+            else if (parent.Parent != null && parent.Parent.Id != 1)
+            {
+                return (checkIfChild(parent, userid));
+            }
+            else
+            {
+                return false;
+            }
+
+        }
+
+        private bool checkIfChild(User user, int userid)
+        {
+            User parent = user.Parent;
+            if (user.Parent.Parent == null)
+            {
+                parent = _context.Users.Where(u => u.Id == parent.Id).Include(u => u.Parent).FirstOrDefault();
+            }
+            if (userid == 1)
+            {
+                return true;
+            }
+            if (parent.Id == userid)
+            {
+                return true;
+            }
+            else if (parent.Parent != null && parent.Parent.Id != 1)
+            {
+                return (checkIfChild(parent, userid));
+            }
+            else
+            {
+                return false;
+            }
+
+        }
+
+        public User Update(int id, string oldPassword, string newPassword)
+        {
+
+            //var user = _context.Users.Where(c => c.Id == id).FirstOrDefault();
+            // var userInDb = await _context.Users.FirstOrDefaultAsync(c => c.Id == id);
+            var userInDb = _context.Users.FirstOrDefault(c => c.Id == id);
+            if (userInDb == null)
+                return null;
+
+            //check the user old password
+            if (!VerifyPasswordHash(oldPassword, userInDb.PasswordHash, userInDb.PasswordSalt))
+                return null;
+
+            // Update Process
+            byte[] passwordHash, passwordSalt;
+            CreatePasswordHash(newPassword, out passwordHash, out passwordSalt);
+
+            userInDb.PasswordHash = passwordHash;
+            userInDb.PasswordSalt = passwordSalt;
+
+            _context.Users.Update(userInDb);
+            _context.SaveChanges();
+            return userInDb;
+        }
+
+
+        public User UpdateUserLoginTryCount(string userName)
+        {
+            var userInDb = _context.Users.FirstOrDefault(c => c.UserName == userName);
+            // If user with enter id is not in db
+
+            userInDb.tryLoginCount += 1;
+            _context.Users.Update(userInDb);
+            _context.SaveChanges();
+
+            return userInDb;
+        }
+        public User UpdateInformation(User user, string password)
+        {
+            var userInDb = _context.Users.FirstOrDefault(c => c.Id == user.Id);
+            // If user with enter id is not in db
+            if (userInDb == null)
+            {
+                return null;
+            }
+            // Update Process
+            // Update the password if the user has entered an new password
+            byte[] passwordHash, passwordSalt;
+            if (password != null)
+            {
+                CreatePasswordHash(password, out passwordHash, out passwordSalt);
+                userInDb.PasswordHash = passwordHash;
+                userInDb.PasswordSalt = passwordSalt;
+            }
+
+            userInDb.UserName = userInDb.UserName;
+            userInDb.FirstName = user.FirstName;
+            userInDb.LastName = user.LastName;
+            userInDb.Phone = user.Phone;
+            userInDb.Title = user.Title;
+            userInDb.UpdatedDate = user.UpdatedDate;
+            userInDb.ParentId = user.ParentId;
+            userInDb.IsActive = user.IsActive;
+            userInDb.IsAdmin = user.IsAdmin;
+            userInDb.IsModerator = user.IsModerator;
+            userInDb.ModeratorRequired = user.ModeratorRequired;
+            userInDb.GroupName = user.GroupName;
+            userInDb.Dept = user.Dept;
+            userInDb.Email = user.Email;
+            userInDb.tryLoginCount = user.tryLoginCount;
+
+            _context.Users.Update(userInDb);
+            _context.SaveChanges();
+            return userInDb;
+        }
+
+        // For System Administrator to reset the Password of a current user
+        public async Task<User> ResetPassword(int id, string newPassword)
+        {
+            var userInDb = _context.Users.FirstOrDefault(c => c.Id == id);
+            if (userInDb == null)
+            {
+                return null;
+            }
+
+            byte[] passwordHash, passwordSalt;
+            // Reset the password by Admin
+            CreatePasswordHash(newPassword, out passwordHash, out passwordSalt);
+            userInDb.PasswordHash = passwordHash;
+            userInDb.PasswordSalt = passwordSalt;
+
+            _context.Users.Update(userInDb);
+            await _context.SaveChangesAsync();
+            return userInDb;
+        }
+
+        public async Task<User> ResetPasswordByUserName(string userName, string newPassword)
+        {
+            var userInDb = _context.Users.FirstOrDefault(c => c.UserName == userName);
+            if (userInDb == null)
+            {
+                return null;
+            }
+
+            byte[] passwordHash, passwordSalt;
+            // Reset the password by Admin
+            CreatePasswordHash(newPassword, out passwordHash, out passwordSalt);
+            userInDb.PasswordHash = passwordHash;
+            userInDb.PasswordSalt = passwordSalt;
+
+            _context.Users.Update(userInDb);
+            await _context.SaveChangesAsync();
+            return userInDb;
+        }
+
+        public bool CheckUserDependencies(int id, int parentId)
+        {
+            var userInDb = _context.Users.Include(x => x.Parent).FirstOrDefault(c => c.Id == id);
+            if (userInDb == null)
+                throw new InvalidOperationException();
+
+            return RelatedUserExists(id);
+        }
+
+
+
+        public void DeleteUserById(int id, int parentId)
+        {
+            var userInDb = _context.Users.Include(x => x.Parent).FirstOrDefault(c => c.Id == id);
+            if (userInDb == null)
+            {
+                throw new InvalidOperationException();
+            }
+            if (!checkIfChild(userInDb, parentId))
+            {
+                throw new InvalidOperationException();
+            }
+            List<PermLayer> permLayers = new List<PermLayer>();
+            List<PermGUI> permGUIs = new List<PermGUI>();
+            List<PermZone> permZones = new List<PermZone>();
+            if (userInDb.IsAdmin)
+            {
+                var users = GetSubUsersGroups(id);
+
+                foreach (var user in users)
+                {
+                    permLayers.AddRange(_context.PermLayers.Where(p => p.UserId == user.Id).ToList());
+                    permGUIs.AddRange(_context.PermGUIs.Where(p => p.UserId == user.Id).ToList());
+                    permZones.AddRange(_context.PermZones.Where(p => p.UserId == user.Id).ToList());
+                }
+                permLayers.AddRange(_context.PermLayers.Where(p => p.UserId == userInDb.Id).ToList());
+                permGUIs.AddRange(_context.PermGUIs.Where(p => p.UserId == userInDb.Id).ToList());
+                permZones.AddRange(_context.PermZones.Where(p => p.UserId == userInDb.Id).ToList());
+                users.Add(userInDb);
+                _context.PermLayers.RemoveRange(permLayers);
+                _context.PermGUIs.RemoveRange(permGUIs);
+                _context.PermZones.RemoveRange(permZones);
+                _context.Users.RemoveRange(users);
+            }
+            else
+            {
+                permLayers.AddRange(_context.PermLayers.Where(p => p.UserId == userInDb.Id).ToList());
+                permGUIs.AddRange(_context.PermGUIs.Where(p => p.UserId == userInDb.Id).ToList());
+                permZones.AddRange(_context.PermZones.Where(p => p.UserId == userInDb.Id).ToList());
+                _context.PermLayers.RemoveRange(permLayers);
+                _context.PermGUIs.RemoveRange(permGUIs);
+                _context.PermZones.RemoveRange(permZones);
+                _context.Users.Remove(userInDb);
+            }
+
+            _context.SaveChanges();
+        }
+
+        public PanelInfoDto GetPanelInfo(int userId, bool isRoot)
+        {
+            List<UserRegisterRecord> records = new List<UserRegisterRecord>();
+            var users = GetLastAddedUsers(userId, isRoot);
+            var groups = GetSubGroups(userId, isRoot);
+            int groupCount = 0;
+            List<LastAddedUsers> lastAddedUsers = users.OrderBy(u => u.CreatedDate).Take(10).ToList();
+
+            DateTime now = DateTime.Now;
+            DateTime filterStartDate = now.AddDays(-now.Day);
+            DateTime filterEndDate = now;
+            int monthCounter = 0;
+            while (monthCounter != 12)
+            {
+                string month = filterEndDate.ToString("MMMM", CultureInfo.GetCultureInfoByIetfLanguageTag("tr-TR"));
+                string year = filterEndDate.ToString("yyyy");
+                int count = users.Where(u => u.CreatedDate > filterStartDate && u.CreatedDate < filterEndDate).Count();
+                UserRegisterRecord record = new UserRegisterRecord()
+                {
+                    Date = month + ' ' + year,
+                    Count = count,
+                };
+
+                filterEndDate = filterStartDate;
+                filterStartDate = filterEndDate.AddMonths(-1);
+
+                records.Add(record);
+
+                monthCounter++;
+            }
+
+            foreach (var group in groups)
+            {
+                groupCount += group.SubGroupCount;
+                groupCount += 1;
+            }
+
+            PanelInfoDto panelInfo = new PanelInfoDto()
+            {
+                LastAddedUsers = lastAddedUsers,
+                UserRegisterRecord = records,
+                TotalUsers = users.Count(),
+                MonthlyAddedGroups = getSubGroupMonthlyCount(userId, isRoot),
+                TotalGroups = groupCount + 1
+            };
+            return panelInfo;
+        }
+
+        private int getSubGroupMonthlyCount(int parentId, bool isRoot)
+        {
+            int count = 0;
+            DateTime now = DateTime.Now;
+            List<User> userList = new List<User>();
+            if (isRoot)
+            {
+                userList = _context.Users.Include(u => u.Children).Where(u => u.IsAdmin == true && u.ParentId == parentId && u.Id != 1).ToList();
+            }
+            else
+            {
+                userList = _context.Users.Include(u => u.Children).ToList().Where(x => x.IsAdmin == true && x.ParentId == parentId).ToList();
+            }
+            foreach (User user in userList)
+            {
+                if (user.CreatedDate < now && user.CreatedDate > now.AddMonths(-1))
+                {
+                    count += 1;
+                }
+                count += getMonthlyCount(user, now.AddMonths(-1), now);
+            }
+
+            return count;
+        }
+
+        private int getMonthlyCount(User user, DateTime filterStart, DateTime filterEnd)
+        {
+            int count = 0;
+            if (user.Children == null)
+            {
+                user = _context.Users.Where(u => u.Id == user.Id).Include(u => u.Children).FirstOrDefault();
+            }
+            List<User> childGroups = user.Children.Where(ch => ch.IsAdmin == true && ch.Id != 1).ToList();
+            count += childGroups.Where(ch => ch.CreatedDate > filterStart && ch.CreatedDate < filterEnd).Count();
+            foreach (User child in childGroups)
+            {
+                count += getMonthlyCount(child, filterStart, filterEnd);
+            }
+
+            return count;
+        }
+    }
+}
