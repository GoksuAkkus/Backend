+        [HttpGet("CsbmKoduFromMahalleKodu")]
+        public async Task<string> CsbmKoduFromIlceKodu(long? adresNo)
+        {
+            var giris = new ServiceMernis.KullaniciGiris();
+            giris.UserName = "mebcbs";
+            giris.Password = "9UFdAy7x";
+
+            var logClient = new ServiceMernis.LogClient();
+            logClient.EkranKodu = "1";
+            logClient.Aciklama = "MebCbsAnka";
+            logClient.ModulKodu = "1";
+
+            logClient.YetkiliIPAdresi = "10.30.1.244";
+            logClient.YetkiliKullaniciAdi = "1234567910";
+            logClient.YetkiliKullaniciNo = 1234567910;
+            logClient.YetkiliTCKimlikNo = 44257273824;
+
+            var sorguKriter = new ServiceMernis.AksIlceyeBagliTumMahalleVeSokakSorguKriter();
+            var sorguKriter2 = new ServiceMernis.AksOkuTumMahalleyeBagliCsbmSorguKriter();
+            var sorguKriter3 = new ServiceMernis.AksOkuTumCsbmyeBagliBinaSorguKriter();
+            var sorguKriter4 = new ServiceMernis.AksOkuTumBinayaBagliBgmszSorguKriter();
+            sorguKriter2.MahalleKodu = 52525;
+            sorguKriter.IlceKodu = 1723;
+            sorguKriter3.CsbmKodu = 250389;
+            sorguKriter4.BinaKodu = 32311103;
+
+            var client = new ServiceMernis.AksIlceyeBagliTumMahalleVeSokakServisClient();
+            var client2 = new ServiceMernis.AksOkuTumMahalleyeBagliCsbmServisClient();
+            var client3 = new ServiceMernis.AksOkuTumCsbmyeBagliBinaServisClient();
+            var client4 = new ServiceMernis.AksOkuTumBinayaBagliBgmszClient();
+
+            ServiceMernis.IlceyeBagliTumMahalleVeSokakBilgisiSonucu response = await client.IlceyeBagliTumMahalleVeSokakListeleAsync(sorguKriter, giris, logClient);
+            ServiceMernis.CsbmSonucu response2 = await client2.MahalleyeBagliTumCsbmListeleAsync(sorguKriter2, giris, logClient);
+            ServiceMernis.BinaSonucu response3 = await client3.CsbmyeBagliBinaListeleAsync(sorguKriter3, giris, logClient); // (BİNA KODU SORGULA İÇİN)
+            ServiceMernis.BagimsizBolumSonucu response4 = await client4.BinayaBagliTumBagimsizBolumListeleAsync(sorguKriter4, giris, logClient); // (ADRES KODU SORGULA İÇİN)
+
+
+
+            if (response != null)
+            {
+                var binaKodu = response.SorguSonucu[0];
+                return "";
+            }
+            else
+            {
+                return "";
+            }
+        }
