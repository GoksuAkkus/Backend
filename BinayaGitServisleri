+        [HttpGet("getMalikSerhBeyan")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<SerhBeyanMalikData> GetMalikSerhBeyan(decimal tasinmazID, double tapuAlan, decimal mahalleID, string adaNo, string parselNo)
+        {
+            int bagimsizCount = await GetirUstTasinmazSayisi(tasinmazID, TasinmazTip.BagimsizBolum);
+            List<MalikData> totalMalikData = new List<MalikData>();
+            List<SerhBeyanData> totalSerhBeyanData = new List<SerhBeyanData>();
+            if (bagimsizCount > 0)
+            {
+                decimal[] mahalleIds = { mahalleID };
+                Tasinmaz[] parcels = await GetirBagimsizBolum(mahalleIds, adaNo, parselNo, TapuBolumDurum.Hepsi, null, null, null, null);
+                foreach (var parcel in parcels)
+                {
+                    totalMalikData.AddRange(await GetMalik(parcel.ID, (double)parcel.Yuzolcum));
+                    totalSerhBeyanData.AddRange(await GetSerhBeyan(parcel.ID));
+                }
+            }
+
+            totalMalikData.AddRange(await GetMalik(tasinmazID, tapuAlan));
+            totalSerhBeyanData.AddRange(await GetSerhBeyan(tasinmazID));
+            List<Field> malikFields = CreateMalikFields();
+            List<Field> serhBeyanFields = CreateSerhBeyanFields();
+            SerhBeyanMalikData serhBeyanMalikData = new SerhBeyanMalikData()
+            {
+                malik = totalMalikData,
+                malikFields = malikFields,
+                serhBeyan = totalSerhBeyanData,
+                serhBeyanFields = serhBeyanFields
+            };
+
+            return serhBeyanMalikData;
+        }
+
+        [HttpGet("getParcelHistory")]
+        [Authorize(Roles = "SuperAdmin, Admin, User")]
+        public async Task<TKGMFeature> getParcelHistory(decimal tasinmazID, int isHistory)
+        {
+            bool historyValue = false;
+            if (isHistory == 1)
+            {
+                historyValue = true;
+            }
+
+            Tasinmaz[] parcels = await GetirTasinmazHaraket(tasinmazID, historyValue);
+            List<ParcelObject> parcelObjects = new List<ParcelObject>();
+            for (int i = 0; i < parcels.Length; i++)
+            {
+                Tasinmaz tasinmaz = parcels[i];
+
+                DateTime tarih = DateTime.MinValue;
+
+                ParcelObject TKGMPassiveParcelObject = new ParcelObject()
+                {
+                    tasinmazID = tasinmaz.ID,
+                    il = tasinmaz.Il,
+                    ilce = tasinmaz.Ilce,
+                    mahalle = tasinmaz.Mahalle,
+                    mahalleID = tasinmaz.MahalleID,
+                    nitelik = tasinmaz.Nitelik,
+                    ada = tasinmaz.Ada,
+                    parsel = tasinmaz.Parsel,
+                    tasinmazTip = tasinmaz.Tip,
+                    tapuAlan = tasinmaz.Yuzolcum,
+                    tapuBolumDurum = tasinmaz.TapuBolumDurum,
+                    tarih = tarih,
+                    edinimSekli = tasinmaz.TesisIslem.IslemTanimAd,
+                };
+
+                if (tasinmaz.TesisIslem.Yevmiye != null)
+                {
+                    tarih = tasinmaz.TesisIslem.Yevmiye.Tarih;
+                }
+
+                parcelObjects.Add(TKGMPassiveParcelObject);
+            }
+
+            TKGMFeature response = new TKGMFeature()
+            {
+                parcelData = parcelObjects,
+                fields = GetTKGMParcelObjectFields()
+            };
+
+            return response;
+        }
+
