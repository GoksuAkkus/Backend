+        [HttpGet("GetLogs")]
+        public ActionResult GetLogsForUsers()
+        {
+            // Read From JWT
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var groupName = userClaimList[4].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            var isRoot = userId == 1;
+            // Find Users children
+            var users = _logRepository.GetUsers(userId, isRoot);
+
+            var userList = users.Select(t => t.id).ToList();
+            userList.Add(userId);
+            List<int> userListOrder = userList.OrderBy(order=>order).ToList();
+            //var loges = _logRepository.GetLogs(userListOrder).OrderByDescending(c=>c.Id).ToList();
+            var loges = _logRepository.GetLogs(userListOrder).ToList();
+            return Ok(loges);
+        }
+
+        
+    }
+}
\ No newline at end of file
