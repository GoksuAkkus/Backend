+                        .WithMany("PermLayer")
+                        .HasForeignKey("UserId")
+                        .OnDelete(DeleteBehavior.Cascade);
+                });
+
+            modelBuilder.Entity("MebCbsAnka.Entities.PermZone", b =>
+                {
+                    b.HasOne("MebCbsAnka.Entities.User", "User")
+                        .WithMany("PermZone")
+                        .HasForeignKey("UserId")
+                        .OnDelete(DeleteBehavior.Cascade);
+                });
+
+            modelBuilder.Entity("MebCbsAnka.Entities.User", b =>
+                {
+                    b.HasOne("MebCbsAnka.Entities.User", "Parent")
+                        .WithMany("Children")
+                        .HasForeignKey("ParentId")
+                        .OnDelete(DeleteBehavior.Cascade);
+                });
+#pragma warning restore 612, 618
+        }
+    }
+}
diff --git a/MebCbsAnka/Program.cs b/MebCbsAnka/Program.cs
