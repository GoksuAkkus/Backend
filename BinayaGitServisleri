+namespace MebCbsAnka.Controllers
+{
+    [Produces("application/json")]
+    [Route("api/Auth")]
+    public class AuthController : Controller
+    {
+        private IAuthRepository _authRepository;
+        private IConfiguration _configuration;
+        private IPermGUIDal _permGUIDal;
+        private IPermLayerDal _permLayerDal;
+        private IPermZoneDal _permZoneDal;
+        private ILogRepository _logRepository;
+        private IHttpContextAccessor _accessor;
+        //Dependency Injection
+        public AuthController(IAuthRepository authRepository, IConfiguration configuration, IPermGUIDal permGUIDal,
+            IPermLayerDal permLayerDal, IPermZoneDal permZoneDal, ILogRepository logRepository, IHttpContextAccessor accessor)
+        {
+            _authRepository = authRepository;
+            _configuration = configuration;
+            _permGUIDal = permGUIDal;
+            _permLayerDal = permLayerDal;
+            _permZoneDal = permZoneDal;
+            _logRepository = logRepository;
+            _accessor = accessor;
+        }
+
+        [HttpPost("registerUser")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public async Task<IActionResult> Register([FromBody] UserForRegistrerDto userForRegistrerDto)
+        {
+            // Check if Username
+            if (await _authRepository.UserExits(userForRegistrerDto.UserName))
+            {
+                ModelState.AddModelError("UserName", "Username already exists");
+            }
+
+            // check validity
+            if (!ModelState.IsValid)
+            {
+                return BadRequest();
+            }
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+
+            var userToCreate = new User
+            {
+                UserName = userForRegistrerDto.UserName.ToLower(),
+                Email = userForRegistrerDto.Email.ToLower(),
+                Phone = userForRegistrerDto.Phone,
+                Title = userForRegistrerDto.Title.ToLower(),
+                GroupName = userForRegistrerDto.GroupName,
+                IsActive = userForRegistrerDto.IsActive,
+                IsAdmin = userForRegistrerDto.IsAdmin,
+                Dept = userForRegistrerDto.Dept.ToLower(),
+                CreatedDate = userForRegistrerDto.CreatedDate,
+                UpdatedDate = userForRegistrerDto.UpdatedDate,
+                FirstName = userForRegistrerDto.FirstName.ToLower(),
+                LastName = userForRegistrerDto.LastName.ToLower(),
+                ParentId = userId,
+                tryLoginCount = userForRegistrerDto.tryLoginCount
+            };
+
+            // Hashed password added
+            var createdUser = await _authRepository.Register(userToCreate, userForRegistrerDto.Password);
+            // 201 Created 
+            return StatusCode(201);
+        }
+
+        [HttpPost("register")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public async Task<IActionResult> RegisterAll([FromBody] UserRegistrationDto userRegistrationDto)
+        {
+            // Check if Username
+            if (await _authRepository.UserExits(userRegistrationDto.UserName))
+            {
+                ModelState.AddModelError("UserName", "Username already exists");
+            }
+            // check validity
+            if (!ModelState.IsValid)
+            {
+                return BadRequest();
+            }
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var userName = userClaimList[0].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+
+            var userToCreate = new User
+            {
+                UserName = userRegistrationDto.UserName,
+                Email = userRegistrationDto.Email,
+                Phone = userRegistrationDto.Phone,
+                Title = userRegistrationDto.Title,
+                GroupName = userRegistrationDto.GroupName,
+                IsActive = userRegistrationDto.IsActive,
+                IsAdmin = userRegistrationDto.IsAdmin,
+                IsModerator = userRegistrationDto.IsModerator,
+                ModeratorRequired = userRegistrationDto.ModeratorRequired,
+                Dept = userRegistrationDto.Dept,
+                CreatedDate = userRegistrationDto.CreatedDate,
+                UpdatedDate = userRegistrationDto.UpdatedDate,
+                FirstName = userRegistrationDto.FirstName,
+                LastName = userRegistrationDto.LastName,
+                ParentId = userRegistrationDto.ParentId,
+                tryLoginCount = userRegistrationDto.tryLoginCount
+            };
+            // Hashed password added
+            var createdUser = await _authRepository.Register(userToCreate, userRegistrationDto.Password);
+            //if (! await _authRepository.UserExits(userRegistrationDto.UserName))
+            //{
+            //    return Ok("User was not added successfully");
+            //}
+
+            foreach (var dtoGui in userRegistrationDto.PermGUI)
+            {
+                var permGUIToCreate = new PermGUI
+                {
+                    UserId = createdUser.Id,
+                    CreatedDate = userRegistrationDto.CreatedDate,
+                    Item = dtoGui.Item,
+                    Menu = dtoGui.Menu
+                };
+                _permGUIDal.Add(permGUIToCreate);
+            }
+
+            foreach (var dtoLayer in userRegistrationDto.PermLayer)
+            {
+                var permLayerToCreate = new PermLayer
+                {
+                    UserId = createdUser.Id,
+                    CreatedDate = userRegistrationDto.CreatedDate,
+                    Field = dtoLayer.Field,
+                    Layer = dtoLayer.Layer,
+                    Permission = dtoLayer.Permission,
+                };
+                _permLayerDal.Add(permLayerToCreate);
+            }
+
+            foreach (var dtoZone in userRegistrationDto.PermZone)
+            {
+                var permZoneToCreate = new PermZone
+                {
+                    UserId = createdUser.Id,
+                    CreatedDate = userRegistrationDto.CreatedDate,
+                    Zone = dtoZone.Zone
+
+                };
+                _permZoneDal.Add(permZoneToCreate);
+            }
+            
+            var logToCreate = new Log
+            {
+                CreatedDate = DateTime.Now,
+                Description = createdUser.UserName + " Kullanıcı Eklendi",
+                Action = "Ekleme",
+                UserId = userId,
+                UserName = userName
+            };
+            _logRepository.Add(logToCreate);
+            return StatusCode(201, createdUser.Id);
+        }
+
+        [HttpPost("login")]
+        public async Task<IActionResult> Login([FromBody] UserForLoginDto userForLoginDto)
+        {
+            var user = await _authRepository.Login(userForLoginDto.UserName, userForLoginDto.Password);
+
+            Log logToCreate;
+            string arcgisTokenURL = _configuration.GetSection("ArcgisRefererURL:ArcgisGenerateTokenUrl").Value;
+            var userIp = _accessor.HttpContext.Connection.RemoteIpAddress.ToString();       
+            // Check if user not available
+            if (user == null)
+            {
+                logToCreate = new Log
+                 {
+                    CreatedDate = DateTime.Now,
+                    Description = userForLoginDto.UserName + " " + userIp + " Basarsiz Giriş",
+                    Action = "Basarsiz Giriş",
+                    UserName = userForLoginDto.UserName
+                 };
+                _logRepository.Add(logToCreate);
+                return Unauthorized();
+            }
+
+            // User available return JWT Token 
+            var tokenHandler = new JwtSecurityTokenHandler();
+
+            // Generate token with private key in appsetting.json
+            var key = Encoding.ASCII.GetBytes(_configuration.GetSection("AppSettings:Token").Value);
+            // User Authorization Mechanism For Controlling
+            string role;
+            if (user.IsAdmin == true && user.ParentId == 0)
+                role = "SuperAdmin";
+            else if (user.IsAdmin == true && user.ParentId != 0)
+                role = "Admin";
+            else
+                role = "User";
+
+            if (role == "User")
+                return Unauthorized();
+             
+            var tokenDescriptor = new SecurityTokenDescriptor()
+            {
+                Subject = new ClaimsIdentity(new Claim[]
+                 {
+                    new Claim(ClaimTypes.NameIdentifier, user.Id.ToString()),
+                    new Claim("group",user.GroupName),
+                    new Claim(ClaimTypes.Name, user.UserName),
+                    new Claim(ClaimTypes.Role, role),
+                    new Claim("parentId",user.ParentId.ToString())
+                 }),
+                // Set token validity
+                Expires = DateTime.Now.AddDays(1),
+                SigningCredentials = new SigningCredentials(new SymmetricSecurityKey(key), SecurityAlgorithms.HmacSha512Signature)
+            };
+            // Creating Token according to tokenDescriptor
+            var token = tokenHandler.CreateToken(tokenDescriptor);
+            var tokenString = tokenHandler.WriteToken(token);
+
+            HttpClient httpClient = new HttpClient();
+            var refererURL = _configuration.GetSection("ArcgisRefererURL:AdminLocal").Value;
+            string username = _configuration.GetSection("Credentials:Username").Value;
+            string password = _configuration.GetSection("Credentials:Password").Value;
+
+            var stringContent = new FormUrlEncodedContent(new[]
+            {
+                new KeyValuePair<string, string>("username", username),
+                new KeyValuePair<string, string>("password", password),
+                new KeyValuePair<string, string>("client", "referer"),
+                new KeyValuePair<string, string>("expiration", "1440"),
+                new KeyValuePair<string, string>("f", "json"),
+                new KeyValuePair<string, string>("referer", refererURL),
+                new KeyValuePair<string, string>("ip", ""),
+                new KeyValuePair<string, string>("encrypted", "false"),
+            });
+
+            var response = await httpClient.PostAsync(arcgisTokenURL, stringContent);
+            var content = await response.Content.ReadAsStringAsync();
+
+            var tokenObject = JsonConvert.DeserializeObject<ArcgisTokenDto>(content);
+
+            LoginDto loginDto = new LoginDto()
+            {
+                arcgisData = tokenObject,
+                token = tokenString,
+            };
+            logToCreate = new Log
+            {
+                CreatedDate = DateTime.Now,
+                Description = user.UserName +" "+userIp+ " Giriş Yapıldı ",
+                Action = "Giriş",
+                UserId = user.Id,
+                UserName=user.UserName         
+            };
+           _logRepository.Add(logToCreate);
+               
+            // Signing In user and give token
+            return Ok(loginDto);
+        }
+        // api/auth/GetUser/1
+        [HttpGet("GetUser/{id}")]
+        public ActionResult GetUserById(int id)
+        {
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var groupName = userClaimList[4].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            var user = _authRepository.GetUserById(id, userId);
+            if (user == null)
+            {
+                return NotFound();
+            }
+            return Ok(user);
+        }
+
+        [HttpDelete("delete/{id}")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public ActionResult DeleteById(int id)
+        {
+            // Read From JWT
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var userName = userClaimList[0].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+
+            // User cannot delete his account
+            if (userId == id)
+                return Ok("You cannot delete your own account!");
+
+            // SuperAdmin Cannot delete
+            if (id==1)
+                return Ok("You cannot delete the system Super Admin!");
+
+            var userDependencies =  _authRepository.CheckUserDependencies(id, userId);
+            if (userDependencies)
+                return Unauthorized();
+            var selectedUser = _authRepository.GetUserWithId(id);
+            _authRepository.DeleteUserById(id, userId);
+            _authRepository.SaveAll();
+            var logToCreate = new Log
+            {
+                CreatedDate = DateTime.Now,
+                Description = selectedUser.UserName + " Kullanıcı Silindi",
+                Action = "Silme",
+                UserId = userId,
+                UserName = userName
+            };
+            _logRepository.Add(logToCreate);
+            return Ok();
+
+        }
+
+        [HttpPut("Update/{id}")]
+        [Authorize(Roles = "SuperAdmin, Admin")]
+        public IActionResult UpdateAllInformationById(int id,
+            [FromBody] UserUpdateAllInformationDto userUpdateAllInformationDto)
+        {
+            // The one who is updating will be the new parent of the user
+            var userClaimList = HttpContext.User.Claims.ToList();
+            var userName = userClaimList[0].ToString().Split(": ")[1];
+            var userId = int.Parse(userClaimList[1].ToString().Split(": ")[1]);
+            var userToUpdate = new User
+            {
+                Id = id,
+                // UserName = userUpdateAllInformationDto.UserName,
+                Email = userUpdateAllInformationDto.Email,
+                Phone = userUpdateAllInformationDto.Phone,
+                Title = userUpdateAllInformationDto.Title,
+                GroupName = userUpdateAllInformationDto.GroupName,
+                IsActive = userUpdateAllInformationDto.IsActive,
+                IsAdmin = userUpdateAllInformationDto.IsAdmin,
+                IsModerator = userUpdateAllInformationDto.IsModerator,
+                ModeratorRequired = userUpdateAllInformationDto.ModeratorRequired,
+                Dept = userUpdateAllInformationDto.Dept,
+                UpdatedDate = userUpdateAllInformationDto.UpdatedDate,
+                FirstName = userUpdateAllInformationDto.FirstName,
+                LastName = userUpdateAllInformationDto.LastName,
+                ParentId = userUpdateAllInformationDto.ParentId,
+                tryLoginCount = userUpdateAllInformationDto.tryLoginCount
+            };
+            //UserInformation Updated
+            _authRepository.UpdateInformation(userToUpdate, userUpdateAllInformationDto.Password);
+
+            //User Permission Updated
+
+            _permGUIDal.DeleteMany(_permGUIDal.GetList(p => p.UserId == id));
+            foreach (var dtoGui in userUpdateAllInformationDto.PermGUI)
+            {
+                var permGUIToUpdate = new PermGUI
+                {
+                    Id = dtoGui.Id,
+                    UserId = id,
+                    UpdatedDate = userUpdateAllInformationDto.UpdatedDate,
+                    Item = dtoGui.Item,
+                    Menu = dtoGui.Menu
+                };
+                _permGUIDal.Add(permGUIToUpdate);
+            }
+            _permLayerDal.DeleteMany(_permLayerDal.GetList(p => p.UserId == id));
+            foreach (var dtoLayer in userUpdateAllInformationDto.PermLayer)
+            {
+                var permLayerToUpdate = new PermLayer
+                {
+                    Id = dtoLayer.Id,
+                    UserId = id,
+                    UpdatedDate = userUpdateAllInformationDto.UpdatedDate,
+                    Field = dtoLayer.Field,
+                    Layer = dtoLayer.Layer,
+                    Permission = dtoLayer.Permission,
+                };
+                _permLayerDal.Add(permLayerToUpdate);
+            }
+            _permZoneDal.DeleteMany(_permZoneDal.GetList(p => p.UserId == id));
+            foreach (var dtoZone in userUpdateAllInformationDto.PermZone)
+            {
+                var permZoneToCreate = new PermZone
+                {
+                    Id = dtoZone.Id,
+                    UserId = id,
+                    UpdatedDate = userUpdateAllInformationDto.UpdatedDate,
+                    Zone = dtoZone.Zone
+
+                };
+                _permZoneDal.Add(permZoneToCreate);
+            }
+            var logToCreate = new Log
+            {
+                CreatedDate = DateTime.Now,
+                Description = userToUpdate.UserName + " Kullanıcı Güncellendi",
+                Action = " Günceleme",
+                UserId = userId,
+                UserName = userName
+            };
+            _logRepository.Add(logToCreate);
+            return Ok(id);
+
+        }
+
+        [HttpPut("UpdatePassword/{id}")]
+        public IActionResult UpdatePasswordById(int id, [FromBody] UserForUpdatingPasswordDto userForUpdatingPasswordDto)
+        {
+            var updatedPassword = _authRepository.Update(id, userForUpdatingPasswordDto.OldPassword, userForUpdatingPasswordDto.NewPassword);
+            if (updatedPassword == null)
+                return StatusCode(204,"Password is not Updated");
+            return Ok("Password Updated Successfully");
+        }
