+                }
+
+                nviObject.yerlesimYeriAdresi = yerlesimNviAdres;
+                //if (cevap.HataBilgisi != null)
+                //{
+                //    throw null;
+                //}
+                return nviObject;
+
+            }
+            catch (Exception ex)
+            {
+                return null;
