diff --git a/MebCbsAnka/DataAccess/Abstruct/IEntityRepository.cs b/MebCbsAnka/DataAccess/Abstruct/IEntityRepository.cs
new file mode 100644
index 0000000000000000000000000000000000000000..ef03eea276cf1fbfad97c1260283aa2a840ae1b9
--- /dev/null
+++ b/MebCbsAnka/DataAccess/Abstruct/IEntityRepository.cs
@@ -0,0 +1,21 @@
+﻿using System;
+using System.Collections.Generic;
+using System.Linq;
+using System.Linq.Expressions;
+using System.Threading.Tasks;
+using MebCbsAnka.Entities;
+
+namespace MebCbsAnka.DataAccess.Abstruct
+{
+   public interface IEntityRepository<T>
+       where T:class , IEntity, new ()
+    {
+        T Get(Expression<Func<T, bool>> filter = null);
+        List<T> GetList(Expression<Func<T, bool>> filter = null);
+        void Add(T entity);
+        void Update(T entity);
+        void DeleteMany(List<T> entities);
+        void Delete(T entity);
+
+    }
+}
