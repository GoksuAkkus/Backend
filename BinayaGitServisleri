+
+        [HttpGet("getAdresBilgi")]
+        public async Task<NVIDto> GetirAdresBilgiKimlikNoDan(string kimlikNo)
+        {
+            try
+            {
+                long _kimlikNo = long.Parse(kimlikNo);
+                var kriter = new ServiceMernis.KimlikNoSorgulaAdresSorguKriter();
+                kriter.KimlikNo = _kimlikNo;
+                var giris = new ServiceMernis.KullaniciGiris();
+                giris.UserName = "mebcbs";
+                giris.Password = "9UFdAy7x";
+
+                var logClient = new ServiceMernis.LogClient();
+                logClient.EkranKodu = "1";
+                logClient.Aciklama = "MebCbsAnka";
+                logClient.ModulKodu = "1";
+
+                logClient.YetkiliIPAdresi = "10.30.1.244";
+                logClient.YetkiliKullaniciAdi = "1234567910";
+                logClient.YetkiliKullaniciNo = 1234567910;
+                logClient.YetkiliTCKimlikNo = 44257273824;
+                var mernisResponse = await _mernisKimlikNoSorgularAdresClient.KimlikNodanAdresBilgisiSorgulaAsync(kriter, giris, logClient);
+
+                NVIDto nviObject = new NVIDto();
+
+                var digerAdresBilgileri = mernisResponse.DigerAdresBilgileri;
+                var adresBilgileri = mernisResponse.YerlesimYeriAdresi;
+
+                foreach (var digerAdres in digerAdresBilgileri)
+                {
+                    if (digerAdres.AcikAdres == null)
+                    {
+                        break;
+                    }
+
+                    AdresBilgi nviAdres = new AdresBilgi()
+                    {
+                        acikAdres = digerAdres.AcikAdres,
+                        adresNo = digerAdres.AdresNo,
+                        adresTip = digerAdres.AdresTip.Aciklama,
+                    };
+
+                    if (digerAdres.IlIlceMerkezAdresi != null)
+                    {
+                        var adresBilgi = digerAdres.IlIlceMerkezAdresi;
+
+                        NVIData nviData = new NVIData()
+                        {
+                            bagimsizBolumDurum = adresBilgi.BagimsizBolumDurum.Aciklama,
+                            bagimsizBolumTipi = adresBilgi.BagimsizBolumTipi.Aciklama,
+                            binaAda = adresBilgi.BinaAda,
+                            binaBlokAdi = adresBilgi.BinaBlokAdi,
+                            binaDurum = adresBilgi.BinaDurum.Aciklama,
+                            binaKodu = adresBilgi.BinaKodu,
+                            binaNo = adresBilgi.BinaNo,
+                            binaNumaratajTipi = adresBilgi.BinaNumaratajTipi.Aciklama,
+                            binaParsel = adresBilgi.BinaParsel,
+                            binaSiteAdi = adresBilgi.BinaSiteAdi,
+                            binaYapiTipi = adresBilgi.BinaYapiTipi.Aciklama,
+                            csbm = adresBilgi.Csbm,
+                            disKapiNo = adresBilgi.DisKapiNo,
+                            icKapiNo = adresBilgi.IcKapiNo,
+                            il = adresBilgi.Il,
+                            ilce = adresBilgi.Ilce,
+                            katNo = adresBilgi.KatNo,
+                            mahalle = adresBilgi.Mahalle,
+                            tapuBagimsizBolumNo = adresBilgi.TapuBagimsizBolumNo,
+                            yapiKullanimAmac = adresBilgi.YapiKullanimAmac.Aciklama,
+                        };
+
+                        nviAdres.ilIlceMerkeziAdresi = nviData;
+                    }
+                    if (digerAdres.KoyAdresi != null)
+                    {
+                        var adresBilgi = digerAdres.KoyAdresi;
+
+                        NVIData nviData = new NVIData()
+                        {
+                            bagimsizBolumDurum = adresBilgi.BagimsizBolumDurum.Aciklama,
+                            bagimsizBolumTipi = adresBilgi.BagimsizBolumTipi.Aciklama,
+                            binaAda = adresBilgi.BinaAda,
+                            binaBlokAdi = adresBilgi.BinaBlokAdi,
+                            binaDurum = adresBilgi.BinaDurum.Aciklama,
+                            binaKodu = adresBilgi.BinaKodu,
+                            binaNo = adresBilgi.BinaNo,
+                            binaNumaratajTipi = adresBilgi.BinaNumaratajTipi.Aciklama,
+                            binaParsel = adresBilgi.BinaParsel,
+                            binaSiteAdi = adresBilgi.BinaSiteAdi,
+                            binaYapiTipi = adresBilgi.BinaYapiTipi.Aciklama,
+                            csbm = adresBilgi.Csbm,
+                            disKapiNo = adresBilgi.DisKapiNo,
+                            icKapiNo = adresBilgi.IcKapiNo,
+                            il = adresBilgi.Il,
+                            ilce = adresBilgi.Ilce,
+                            katNo = adresBilgi.KatNo,
+                            mahalle = adresBilgi.Mahalle,
+                            tapuBagimsizBolumNo = adresBilgi.TapuBagimsizBolumNo,
+                            yapiKullanimAmac = adresBilgi.YapiKullanimAmac.Aciklama,
+                        };
+
+                        nviAdres.koyAdresi = nviData;
+                    }
+                    if (digerAdres.YurtDisiAdresi != null)
+                    {
+                        var adresBilgi = digerAdres.YurtDisiAdresi;
+                        var disTemsiclik = adresBilgi.DisTemsilcilik.Aciklama;
+                        var yabanciAdres = adresBilgi.YabanciAdres;
+                        var yabanciUlke = adresBilgi.YabanciUlke;
+                        var yabanciSehir = adresBilgi.YabanciSehir;
+                        YurtDisiNVIData nviData = new YurtDisiNVIData()
+                        {
+                            disTemsilcilik = adresBilgi.DisTemsilcilik.Aciklama,
+                            yabanciAdres = adresBilgi.YabanciAdres,
+                            yabanciSehir = adresBilgi.YabanciSehir,
+                            yabanciUlke = adresBilgi.YabanciUlke.Aciklama
+                        };
+
+                        nviAdres.yurtdisiAdresi = nviData;
+                    }
+
+                    nviObject.digerAdresBilgileri.Add(nviAdres);
+                }
+
+                AdresBilgi yerlesimNviAdres = new AdresBilgi()
+                {
+                    acikAdres = adresBilgileri.AcikAdres,
+                    adresNo = adresBilgileri.AdresNo,
+                    adresTip = adresBilgileri.AdresTip.Aciklama,
+                };
+
+                if (adresBilgileri.IlIlceMerkezAdresi != null)
+                {
+                    var adresBilgi = adresBilgileri.IlIlceMerkezAdresi;
+
+                    NVIData nviData = new NVIData()
+                    {
+                        bagimsizBolumDurum = adresBilgi.BagimsizBolumDurum.Aciklama,
+                        bagimsizBolumTipi = adresBilgi.BagimsizBolumTipi.Aciklama,
+                        binaAda = adresBilgi.BinaAda,
+                        binaBlokAdi = adresBilgi.BinaBlokAdi,
+                        binaDurum = adresBilgi.BinaDurum.Aciklama,
+                        binaKodu = adresBilgi.BinaKodu,
+                        binaNo = adresBilgi.BinaNo,
+                        binaNumaratajTipi = adresBilgi.BinaNumaratajTipi.Aciklama,
+                        binaParsel = adresBilgi.BinaParsel,
+                        binaSiteAdi = adresBilgi.BinaSiteAdi,
+                        binaYapiTipi = adresBilgi.BinaYapiTipi.Aciklama,
+                        csbm = adresBilgi.Csbm,
+                        disKapiNo = adresBilgi.DisKapiNo,
+                        icKapiNo = adresBilgi.IcKapiNo,
+                        il = adresBilgi.Il,
+                        ilce = adresBilgi.Ilce,
+                        katNo = adresBilgi.KatNo,
+                        mahalle = adresBilgi.Mahalle,
+                        tapuBagimsizBolumNo = adresBilgi.TapuBagimsizBolumNo,
+                        yapiKullanimAmac = adresBilgi.YapiKullanimAmac.Aciklama,
+                    };
+
+                    yerlesimNviAdres.ilIlceMerkeziAdresi = nviData;
+                }
+                if (adresBilgileri.KoyAdresi != null)
+                {
+                    var adresBilgi = adresBilgileri.KoyAdresi;
+
+                    NVIData nviData = new NVIData()
+                    {
+                        bagimsizBolumDurum = adresBilgi.BagimsizBolumDurum.Aciklama,
+                        bagimsizBolumTipi = adresBilgi.BagimsizBolumTipi.Aciklama,
+                        binaAda = adresBilgi.BinaAda,
+                        binaBlokAdi = adresBilgi.BinaBlokAdi,
+                        binaDurum = adresBilgi.BinaDurum.Aciklama,
+                        binaKodu = adresBilgi.BinaKodu,
+                        binaNo = adresBilgi.BinaNo,
+                        binaNumaratajTipi = adresBilgi.BinaNumaratajTipi.Aciklama,
+                        binaParsel = adresBilgi.BinaParsel,
+                        binaSiteAdi = adresBilgi.BinaSiteAdi,
+                        binaYapiTipi = adresBilgi.BinaYapiTipi.Aciklama,
+                        csbm = adresBilgi.Csbm,
+                        disKapiNo = adresBilgi.DisKapiNo,
+                        icKapiNo = adresBilgi.IcKapiNo,
+                        il = adresBilgi.Il,
